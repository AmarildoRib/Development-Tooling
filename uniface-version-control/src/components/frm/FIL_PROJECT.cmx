<DAT name="SFUNC" xml:space='preserve'>;--------------------------------------------------------------------
operation init
;--------------------------------------------------------------------
variables
	String  vMessages
	Numeric vStatus
endvariables

	$currentVariation$	= $variation
	$variation			= "VC_LIBRARY"

	;----------------------------------------------------
	; Set up our services.
	;----------------------------------------------------
	$VCINTERFACE$ = "FIL"
	call lpSetupServices($VCINTERFACE$);
	if($status &lt; 0)
		return &lt;FAILED&gt;
	endif

	;-----------------------------------------------------
	; Set version control implementation to filesync
	;-----------------------------------------------------
	$status = $vcMain$-&gt;setVcHandler("FIL", $vcWrp$, vMessages)
	if ($status &lt; 0)
		message/error vMessages
	endif

	;----------------------------------------------------
	; Get the working directory
	;----------------------------------------------------
	$settingsSvc$-&gt;getWorkingCopyPath($baseDir$, vMessages)
	#include VC_LIBRARY:IP_POSTCALL_PRES

end ;init


;--------------------------------------------------------------------
operation cleanup
;--------------------------------------------------------------------

	call lpDestroyServices();

	$variation = $currentVariation$

end ;cleanup


