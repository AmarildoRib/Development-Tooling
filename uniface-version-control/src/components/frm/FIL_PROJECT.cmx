;--------------------------------------------------------------------
partner operation delete
; TODO: move version control invocation logic to VC_MAIN
;--------------------------------------------------------------------
variables
   String  vCommand
   String  vPath
   String  vMessages
   Numeric vStatus
   String  vSelectionPath
   boolean bIsUnifaceExport, bLib_U
   string  sUnifaceRelease, sObjects, sObject
   string  sObjectType, sObjectName, sObjectLang, sObjectLib
   numeric nCounter, nNotDeleted
   handle  hDeleteHandle
endvariables
   
   call lpGetNodePath(FILE_TREE.DUMMY, vPath)
   ; Save the currently selected node
   vSelectionPath = lpBuildSelectionPath(FILE_TREE.DUMMY)
   ; Call Tortoise delete screen
	call lpDoMessages("Calling TortoiseSVN remove...")

   if ($valuepart($lfileproperties(vPath, "FILETYPE")) == "DIR")
      askmess/warning "Folders cannot be deleted, only single objects", "OK"
      return
   endif
   activate "VREPO_EXPPARSER".parseUnifaceExport( vPath, bIsUnifaceExport, sUnifaceRelease, sObjects, vMessages )
   if ( $status &lt; 0 )
      vStatus = $status
      call lpAddMessage("Failed to instantiate VREPO_EXPPARSER", vMessages)
      $status = vStatus 
   endif
   #include VC_LIBRARY:IP_POSTCALL_PRES
   
   nCounter = $itemcount(sObjects)
   askmess/question "Are you sure you want to delete %%vPath%%% ? %%^%%nCounter%%% object(s) will be removed from Uniface Repository", "Yes, No"   
   if ($status == 1)  ; Yes
      newinstance "VREPO_DELETE", hDeleteHandle
      if ($status &lt; 0)
         vStatus = $status
         call lpAddMessage("Failed to instantiate VREPO_DELETE", vMessages)
         $status = vStatus
      endif
      #include VC_LIBRARY:IP_POSTCALL_PRES
      bLib_U      = 0
      nCounter    = 1
      nNotDeleted = 0
      getitem sObject, sObjects, nCounter
      while ( $status &gt; 0 &amp; ! bLib_U )
         getitem/id sObjectType, sObject, "OBJECT_TYPE"
         getitem/id sObjectName, sObject, "OBJECT_NAME"
         getitem/id sObjectLang, sObject, "OBJECT_LANG"
         getitem/id sObjectLib,  sObject, "OBJECT_LIB"
         if ( sObjectName == "U" | sObjectLib == "U" ) 
            bLib_U = 1
            askmess/warning "Objects from library U cannot be deleted ", "OK" 
            call lpAddMessage("Objects from library U not deleted ", vMessages)
            nNotDeleted = nNotDeleted + 1
         else
            if (sObjectType == 'library')
             ; do not delete library, other object types may exist
               call lpaddmessage("library %%sObjectname%%% not deleted ",vMessages) 
               nNotDeleted = nNotDeleted + 1 
            else 
               $status = hDeleteHandle-&gt;DeleteSourceObject(sObjecttype, sObjectname, sObjectlib, sObjectlang, vMessages)
               if ($status &lt; 0) 
                  return (-1)
               endif  
            endif  
         endif  
         nCounter = nCounter + 1 
         getitem sObject, sObjects, nCounter 
      endwhile 
      nCounter = nCounter - 1 - nNotDeleted
      if (nCounter &gt; 0)
       ; Commit the delete
         call lpAddMessage("deleted: %%nCounter%%% objects", vMessages)
         hDeleteHandle-&gt;commitDelete(vMessages)
         if ( $status == 0 )
            call lpAddMessage("committed: %%nCounter%%% objects", vMessages)
         endif
         if ( !bLib_U )
            $status = $vcWrp$-&gt;callremove(vPath, vMessages)
            if ( $status == 0 )
               call lpAddMessage("File %%vPath%%% deleted from SVN", vMessages)
            endif
            activate $vcMain$-&gt;DeleteOccVCStatus(vPath, vMessages)
            if ( $status == 0 )
               call lpAddMessage("Entry %%vPath%%% deleted from table VC_STATUS", vMessages)
            endif
         endif
      else
       ; Rollback the delete
         call lpAddMessage("Delete failed: %%nCounter%%% objects $procerror=%%$procerror%%%", vMessages )
         hDeleteHandle-&gt;rollbackDelete(vMessages)
      endif
	   
      ; Rebuild the source tree view, as it might have changed
      call lpDoMessages("Delete complete.")

      $status = $vcMain$-&gt;refreshVCStatus($baseDir$, vMessages)
      #include VC_LIBRARY:IP_POSTCALL_PRES
      
    ; Need to rebuild the actual tree structure here, as an item has been removed.
      call lpRefreshTree(vSelectionPath)

      deleteinstance hDeleteHandle ; "VREPO_DELETE"

   endif
   
end ; op. delete


