;--------------------------------------------------------------------
partner operation optionStatus
;--------------------------------------------------------------------
params
	String  pOption   : IN
	Boolean pDisabled : OUT
endparams

variables
	String	vType
	String	vFieldSyntaxList
	String	vFieldSyntaxItem
endvariables

	selectcase pOption
		;case "DIFF"
		;	vFieldSyntaxList = $fieldsyntax(DIFF_BUTTON)
		;case "LOG"
		;	vFieldSyntaxList = $fieldsyntax(LOG_BUTTON)
		;case "REVERT"
		;	vFieldSyntaxList = $fieldsyntax(REVERT_BUTTON)
		case "DELETE"
			vFieldSyntaxList = $fieldsyntax(DELETE_BUTTON)
		;case "UPDATE"
		;	vFieldSyntaxList = $fieldsyntax(UPDATE_BUTTON)
		;case "COMMIT"
		;	vFieldSyntaxList = $fieldsyntax(COMMIT_BUTTON)
		;case "ADD"
		;	vFieldSyntaxList = $fieldsyntax(ADD_BUTTON)
		case "IMPORT"
			if ($NODETYPE$ = "FILE")
				vFieldSyntaxList = ""
			else
				vFieldSyntaxList = "DIM"
			endif
		;case "EDIT_CONFLICTS", "RESOLVE"
		;	vType = FILE_TREE_getObjectType(FILE_TREE.DUMMY)
		;	if(vType == "FILECONFLICT")
		;		pDisabled = 0
		;		return &lt;OK&gt;
		;	endif
		;	pDisabled = 1
		;	return &lt;OK&gt;
		case "LOCK"
			;debug
			;vType = FILE_TREE_getObjectType(FILE_TREE.DUMMY)
			;if(vType[1:3] == "FLD")
			;	pDisabled = 1
			;	return &lt;OK&gt;
			;endif
			pDisabled = 1
			return &lt;OK&gt;
		case "RELEASELOCK"
			;vType = FILE_TREE_getObjectType(FILE_TREE.DUMMY)
			;if(vType == "FILECONFLICT")
			;	pDisabled = 0
			;	return &lt;OK&gt;
			;endif
			pDisabled = 1
			return &lt;OK&gt;
		elsecase
			vFieldSyntaxList = ""
	endselectcase

	getitem vFieldSyntaxItem, vFieldSyntaxList, 1
	while($status &gt; 0)
		
		vFieldSyntaxItem = $uppercase(vFieldSyntaxItem)
		if(vFieldSyntaxItem == "DIM")
			pDisabled = 1
			return &lt;OK&gt;
		endif

		delitem vFieldSyntaxList, 1
		getitem vFieldSyntaxItem, vFieldSyntaxList, 1
	endwhile

	pDisabled = 0
	return &lt;OK&gt;

end ;optionStatus
