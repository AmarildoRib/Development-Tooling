;------------------------------------------------
entry lpPopulateNodeChildren
;------------------------------------------------
params
	String	pID	: IN
endparams

variables
	String	vTmp
	String	vNodePath
	String	vBaseDir
	String	vMessages
	Numeric	vStatus	
endvariables

	getitem/id vTmp, $NodesProcessed$, pID
	if($status &gt; 0)
		;This node already has its children populated
		return &lt;OK&gt;
	endif

	;------------------------------------------------------
