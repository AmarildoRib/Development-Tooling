;--------------------------------------
entry lpDoMessages
;--------------------------------------
params
	String pMessages	: IN
endparams

variables
	String	vMessage
endvariables

	getitem vMessage, pMessages, 1
	delitem pMessages, 1
	while($status &gt; 0)
		OUTPUT.LOG = vMessage
		putmess vMessage
		show
		getitem vMessage, pMessages, 1
		delitem pMessages, 1
	endwhile

	return &lt;OK&gt;

end ;lpDoMessages

;--------------------------------------------------------------------
; Synchronize Uniface repository with working copy folders
; - export changes
; - if sandbox, import any updated export files
; - refresh tree to show updated status
;--------------------------------------------------------------------
entry lpSync
;--------------------------------------------------------------------
params
	string pSelectNode : IN ; Node to select after refresh
endparams
variables
	String  vMessages
	String  vPath
	String  vStatus
endvariables

	;Refresh from selected tree position
	;call lpGetNodePath(FILE_TREE.DUMMY, vPath)

	;call lpDoMessages("Exporting changes to %%vPath%%%...")
	call lpDoMessages("Exporting changes...")

	$status = $vcMain$-&gt;refreshVCStatus($baseDir$, vMessages)
	;$status = $vcMain$-&gt;refreshVCStatus(vPath, vMessages)
	#include VC_LIBRARY:IP_POSTCALL_PRES

	call lpExportChanged(CHK_FULLSYNC.BUTTONS, lpExportUfSig())

	call lpDoMessages("Checking for updates to working copy and performing imports...")
	$status = $vcMain$-&gt;import($baseDir$, 0, CHK_FULLSYNC.BUTTONS, vMessages)
	#include VC_LIBRARY:IP_POSTCALL_PRES

	call lpShowTree(pSelectNode) ; Build and show tree
	if ($status &lt; 0)
		message/error "An error occurred - please see message frame for details"
	else
		call lpDoMessages("Uniface repository synchronised with working copy.")
	endif

end ; lpSync

;----------------------------------------------------------------------
; Show tree
;----------------------------------------------------------------------
entry lpShowTree
params
	string pSelectNode : IN ; Node to select after refresh
endparams


	call lpBuildTree()
	call lpSetFieldSyntax()
	if ($status &lt; 0) done

	; If specified, select node and make buttons available
	if (pSelectNode)
		call lpApplySelectionPath(pSelectNode)
		call lpSetFieldSyntax()
	endif

end ;lpShowTree

;--------------------------------------------------------------------
entry lpBuildTree
;--------------------------------------------------------------------
variables
	String  vMessages
	String  vTreeNode
	String  vType
	String  vTreeList
	String  vSelectedNode
	Numeric vStatus
	Boolean vConflictFound
endvariables

	message/nobeep "Building tree..."

	$status = $vcMain$-&gt;refreshVCStatus($baseDir$, vMessages)
	#include VC_LIBRARY:IP_POSTCALL_PRES
	$vcMain$-&gt;calculateExtraProperties()	

	$NodesProcessed$ = ""
	call FILE_TREE_deleteTree()

	call lpGetNodeDetails($baseDir$, "", "", "", vType)
	call FILE_TREE_setObject("root", "", "-", vType, $baseDir$, "")

	call lpBuildNodesForFolder($baseDir$, $baseDir$, "root")

	return &lt;OK&gt;

end ;lpBuildTree


;--------------------------------------------------------------------
entry lpBuildNodesForFolder
;--------------------------------------------------------------------
params
	String	pBaseDir	: IN
	String	pDir      : IN
	String	pParent	: IN
endparams

variables
	String		vSubDir
	String		vContent
	String		vFilePath
	String		vID
	;Numeric	vRevision
	;String		vAuthor
	Datetime	vDate
	String		vType
	String		vFolderContents
	String		vNodeState
	String		vStateCode
	String		vFileProperties
	String		vFileAttributes
endvariables

	;Check that pDir is a directory
	if($lfileexists(pDir) != 2 | pDir == "")
		return &lt;OK&gt;
	endif

	vSubDir = ""

	;----------------------------------------------------
	; Build nodes for all the folders under pDir
	;----------------------------------------------------
	vContent = $ldirlist(pDir, "dir")
	getitem vFilePath, vContent, 1
	while ($status &gt; 0)
		if(vFilePath[1:1] != ".") ; hidden, e.g. .svn folder
			vSubDir = $concat(pDir, "\", vFilePath)
			vID = lpPathToID(vSubDir, pBaseDir)

			call lpGetNodeDetails(vSubDir, vDate, vType)

			;Does this folder have any child folders?
			vFolderContents = $ldirlist(vSubDir, "dir")

			;Should we have this folder expanded?
			getitem/id vNodeState, $NodeState$, vID
			if(vNodeState = "OPEN")
				vStateCode = "-"
			else
				if(vFolderContents == "" | vFolderContents == ".svn")
					vStateCode = ""		
				else
					vStateCode = "+"
				endif
			endif

			;call FILE_TREE_setObject(vID, pParent, vStateCode, vType, vFilePath, "%%vRevision%%%&uSEP;%%vDate%%%&uSEP;%%vAuthor%%%")
			call FILE_TREE_setObject(vID, pParent, vStateCode, vType, vFilePath, "%%vDate%%%")

			if(vNodeState = "OPEN")
				call lpBuildNodesForFolder(pBaseDir, vSubDir, vID)
			endif

  		endif

  		delitem vContent, 1
  		getitem vFilePath, vContent, 1
	endwhile

	;----------------------------------------------------
	; Build nodes for all the files under pDir
	;----------------------------------------------------
	vContent = ""
	vContent = $ldirlist(pDir, "file")
	getitem vFilePath, vContent, 1
	while ($status &gt; 0)

		vSubDir = "%%pDir%%%\%%vFilePath%%%"

		;Check to see if this file is a conflict file.
		if(	vFilePath != $syntax("*.*.mine") &amp; vFilePath != $syntax("*.*.r*") )

			vFileProperties = $lfileproperties(vSubDir)
			getitem/id vFileAttributes, vFileProperties, "FILEATTRIBUTES"
			if( ($scan(vFileAttributes, "H")) = 0 )
				;R=read-only, H=hidden, S=system, A=archive, C=compressed, E=encypted
				vID = $replace (vSubDir,	1 , pBaseDir,	"", -1)
				vID = $replace (vID,		1 , "\",		"", -1)
				vID = $replace (vID,		1 , ":",		"", -1)

				;call lpGetNodeDetails(vSubDir, vRevision, vAuthor, vDate, vType)
				;call FILE_TREE_setObject(vID, pParent, "", vType, vFilePath, "%%vRevision%%%&uSEP;%%vDate%%%&uSEP;%%vAuthor%%%")
				call lpGetNodeDetails(vSubDir, vDate, vType)
				call FILE_TREE_setObject(vID, pParent, "", vType, vFilePath, "%%vDate%%%")

			endif
		endif

  		delitem vContent, 1
  		getitem vFilePath, vContent, 1
	endwhile

	call FILE_TREE_flush()

	putitem/id $NodesProcessed$, pParent, "1"

	return &lt;OK&gt;

end ;lpBuildNodesForFolder

