;--------------------------------------------------------------------
entry lpGetNodeDetails
;--------------------------------------------------------------------
params
	String		pNodePath : IN
	Datetime	pDate     : OUT
	String		pNodeType : OUT
endparams

variables
	String  vItemSvnStatus
	String  vItemStatus
	String  vReturnedInfo
	String  vMessages
	Boolean vHasModification
	Boolean vIsUniface
	String  vDate
	Numeric vStatus
endvariables

	$status = $vcMain$-&gt;getWCInfo(pNodePath, vReturnedInfo, vMessages)
	#include VC_LIBRARY:IP_POSTCALL_PRES
	vItemStatus      = $item("Status", vReturnedInfo)
	vHasModification = $item("HasModifications", vReturnedInfo)
	vIsUniface       = $item("IsUniface", vReturnedInfo)
	vDate            = $item("Date", vReturnedInfo)

	; modified    - text or props have been modified
	; none        - does not exist
	; normal      - exists, but uninteresting

	selectcase $lfileexists(pNodePath) 
	  case 2 ;this node is a folder
		selectcase vItemStatus
			case "modified"
				pNodeType = "FLDMOD"
			case "normal"
				pNodeType = "FLDINSVN"
			elsecase
				pNodeType = "FOLDER"
		endselectcase

	  case 1	;this node is a file
		selectcase vItemStatus
			case "modified"
				pNodeType = "FILEMOD"
			case "normal"
				pNodeType = "FILEINSVN"
			elsecase
				pNodeType = "FILE"
		endselectcase
	endselectcase

	;pRevision = vRevisionNumber

	;Format the date
	;2010-10-07T12:51:02.957438Z
	;ccyy mm dd hh nn ss tt
	pDate = $concat($concat(vDate[1:4],  vDate[6:2],  vDate[9:2]), %\
	                $concat(vDate[12:2], vDate[15:2],	vDate[18:2], vDate[21:2]))
	return &lt;OK&gt;

end ;lpGetNodeDetails


;--------------------------------------------------------------------
entry lpGetNodePath
;--------------------------------------------------------------------
params
	String	pNodeID   : IN
	String	pNodePath : OUT
endparams

variables
	String	vNodeList
	String	vParentNodeID
	String	vNodeText
	String	vPath
	String	vMessages
	String	vBasePath
	Numeric	vStatus
	String	vCurrentParentNodeID
endvariables

	;Check to see if we have already worked out the path for this node
	getitem/id pNodePath, $TREE_NODE_PATHS$, pNodeID
	if($status &gt; 0)
		$status = $settingsSvc$-&gt;getWorkingCopyPath(vBasePath, vMessages)
		#include VC_LIBRARY:IP_POSTCALL_PRES
		pNodePath = lpConcatPaths(vBasePath, pNodePath)
		return &lt;OK&gt;
	endif

	vParentNodeID	= FILE_TREE_getParentObjectId(pNodeID)
	vNodeText		= FILE_TREE_getObjectName(pNodeID)

	if(pNodeID = "root")
	 	$status = $settingsSvc$-&gt;getWorkingCopyPath(vPath, vMessages)
		#include VC_LIBRARY:IP_POSTCALL_PRES
	else
		vPath = vNodeText
	endif

	while(vParentNodeID != "")
		vCurrentParentNodeID	= vParentNodeID
		vParentNodeID			= FILE_TREE_getParentObjectId(vCurrentParentNodeID)
		vNodeText				= FILE_TREE_getObjectName(vCurrentParentNodeID)		

		if(vNodeText = "root")
			;Cache the result before we add the base path
			putitem/id $TREE_NODE_PATHS$, pNodeID, vPath

			$status = $settingsSvc$-&gt;getWorkingCopyPath(vBasePath, vMessages)
			#include VC_LIBRARY:IP_POSTCALL_PRES
			vPath = lpConcatPaths(vBasePath, vPath)
		else
			vPath = lpConcatPaths(vNodeText, vPath)
		endif
	endwhile

	pNodePath = vPath

	return &lt;OK&gt;

end ;lpGetNodePath


;------------------------------------------------
entry lpUpdateAllNodeTypes
;------------------------------------------------
variables
	String	vMessages
	String	vStatus
	String	vPath
	String	vType
	String	vChildren
	String	vChildID
	String	vChildPath
	String	vChildType
	String	vExpandState
endvariables

	message/nobeep "Rebuilding Tree..."
	$status = $vcMain$-&gt;refreshVCStatus($baseDir$, vMessages)
	#include VC_LIBRARY:IP_POSTCALL_PRES
	
	call lpGetNodePath("root", vPath)
	;call lpGetNodeDetails(vPath, "", "", "", vType)
	call lpGetNodeDetails(vPath, "", vType)
	call FILE_TREE_setObjectType("root", vType)
	call lpUpdateChildNodeTypes("root")
	call FILE_TREE_flush()	
	message/nobeep "Done."

	return &lt;OK&gt;

end ;lpUpdateAllNodeTypes


;------------------------------------------------
entry lpUpdateChildNodeTypes
;------------------------------------------------
params
	String	pID	: IN
endparams

variables
	String    vChildren
	String    vChildID
	String    vChildPath
	String    vChildType
	String    vExpandState
	;Numeric   vRevision
	Datetime  vDate
	;String    vAuthor
	String    vUpdateString
	String    vObjectData
endvariables

	vChildren = FILE_TREE_getObjectChildren(pID)
	getitem vChildID, vChildren, 1
	while($status &gt; 0)
		;Update the type of this node
		call lpGetNodePath(vChildID, vChildPath)
		;call lpGetNodeDetails(vChildPath, vRevision, vAuthor, vDate, vChildType)
		call lpGetNodeDetails(vChildPath, vDate, vChildType)

		call FILE_TREE_setObjectType(vChildID, vChildType)
		;call FILE_TREE_setObjectColumns(vChildID, "%%vRevision%%%&uSEP;%%vDate%%%&uSEP;%%vAuthor%%%")
		call FILE_TREE_setObjectColumns(vChildID, "%%vDate%%%")
		
		;If node is not a file
		if($lfileexists(vChildPath) != 1)
			call lpUpdateChildNodeTypes(vChildID)
		endif

		delitem vChildren, 1
		getitem vChildID, vChildren, 1
	endwhile

	return &lt;OK&gt;

end ;lpUpdateChildNodeTypes


