endvariables

	call lpGetNodePath(FILE_TREE.DUMMY, vPath)

	call lpDoMessages("Calling TortoiseSVN get lock...")
	; Update from version control and import changes
	$status = $vcMain$-&gt;lock(vPath, $BaseDir$, vMessages)
	#include VC_LIBRARY:IP_POSTCALL_PRES

	;Refresh the SVN status as things may have changed
	$status = $vcMain$-&gt;refreshVCStatus($baseDir$, vMessages)
	#include VC_LIBRARY:IP_POSTCALL_PRES

	;message/nobeep "Add complete. Rebuilding tree..."
	;call lpUpdateAllNodeTypes()
	call lpDoMessages("Lock done")

	return &lt;OK&gt;

end ;lock

;--------------------------------------------------------------------
partner operation update
;--------------------------------------------------------------------
variables
	String  vCommand
	String  vPath
	String  vMessages
	String  vSelectionPath
	Numeric vStatus
endvariables

	$status = $vcMain$-&gt;refreshVCStatus($baseDir$, vMessages)
	#include VC_LIBRARY:IP_POSTCALL_PRES

	call lpGetNodePath(FILE_TREE.DUMMY, vPath)

	;Save the currently selected node
	vSelectionPath = lpBuildSelectionPath(FILE_TREE.DUMMY)

	call lpDoMessages("Updating from version control...")

	; Update from version control and import changes
	$status = $vcMain$-&gt;update(vPath, $BaseDir$, CHK_FULLSYNC.BUTTONS, vMessages)
	#include VC_LIBRARY:IP_POSTCALL_PRES

	;Rebuild the source tree view, as it might have changed
	call lpRefreshTree(vSelectionPath)

	call lpDoMessages("Update complete")

end ;update


;--------------------------------------------------------------------
partner operation commit
;--------------------------------------------------------------------
; TODO: move version control invocation logic to VC_MAIN
; TODO: check for reverts on commit
;--------------------------------------------------------------------
variables
	String  vPath
	String  vMessages
	String  vNodeList
	String  vNodeProperties
	String  vParentNodeID
	Numeric vStatus
endvariables

	call lpGetNodePath(FILE_TREE.DUMMY, vPath)

	$status = $vcMain$-&gt;commit(vPath, $baseDir$, CHK_FULLSYNC.BUTTONS, vMessages)
	#include VC_LIBRARY:IP_POSTCALL_PRES

	call lpBuildTree()
	call lpSetFieldSyntax()

	message/nobeep "Done"
	call lpDoMessages("Changes committed to version control.")

end ;commit


;--------------------------------------------------------------------
partner operation editConflicts
; TODO: move version control invocation logic to VC_MAIN
;--------------------------------------------------------------------
variables
	String  vPath
	String  vMessages
	String  vBaseDir
	Boolean vConflictFound
	String  vInParams
	Numeric vStatus
endvariables

	call lpGetNodePath(FILE_TREE.DUMMY, vPath)

	call lpDoMessages("Calling TortoiseSVN edit conflicts...")
	$status = $vcWrp$-&gt;callConflictEditor(vPath, vMessages)
	#include VC_LIBRARY:IP_POSTCALL_PRES

	message/info "Click OK after editing conflicts"
	call lpDoMessages("Edit conflicts complete.")

	;Reimport all the source under vPath, as it may have changed
	call lpDoMessages("Reimporting reverted source...")

	putitem/id vInParams, "PATH",			$baseDir$
	putitem/id vInParams, "LIST",			vPath
	putitem/id vInParams, "FORCE_IMPORT",	1
	$status = $sourceImp$-&gt;importFromList(vInParams, vMessages)
	#include VC_LIBRARY:IP_POSTCALL_PRES

	call lpUpdateAllNodeTypes()
	message/clear
	call lpDoMessages("Import of edited conflicts done")

end ;editConflicts


;--------------------------------------------------------------------
partner operation resolve
; TODO: move version control invocation logic to VC_MAIN
;--------------------------------------------------------------------
variables
	String  vPath
	String  vMessages
	Numeric vStatus
endvariables

	call lpGetNodePath(FILE_TREE.DUMMY, vPath)

	call lpDoMessages("Calling TortoiseSVN resolve...")
	$status = $vcWrp$-&gt;callResolve(vPath, vMessages)
	#include VC_LIBRARY:IP_POSTCALL_PRES

	call lpDoMessages("Resolve complete.")

	;Reimport all the source under vPath, as it may have changed
	message/nobeep "Importing updated source..."
	$vcMain$-&gt;import(vPath, $BaseDir$, CHK_FULLSYNC.BUTTONS, vMessages)

	call lpUpdateAllNodeTypes()
	message/nobeep "Done."

end ;resolve


;--------------------------------------------------------------------
; Sync 
;  export changes &amp; check filesystem for SVN working copy changes
;	reselecting selected node after refresh
partner operation refresh
;--------------------------------------------------------------------

	call lpSync(FILE_TREE.DUMMY)

end ;refresh


