;--------------------------------------------------------------------
entry lpBuildSelectionPath
;--------------------------------------------------------------------
returns String

params
	String	pNodeID	: IN
endparams

variables
	String	vSelectionPath
	String	vParentNodeID
endvariables

	putitem vSelectionPath, -1, pNodeID
	vParentNodeID	= FILE_TREE_getParentObjectId(pNodeID)
	while(vParentNodeID != "")
		putitem vSelectionPath, -1, vParentNodeID
		vParentNodeID	= FILE_TREE_getParentObjectId(vParentNodeID)
	endwhile

	$status = &lt;OK&gt;
	return vSelectionPath

end ;lpBuildSelectionPath


;--------------------------------------------------------------------
entry lpApplySelectionPath
;--------------------------------------------------------------------
params
	String	pSelectionPath	: IN
endparams

variables
	String	vNodeID
	String	vNodeName
endvariables

	getitem vNodeID, pSelectionPath, 1
	while($status &gt; 0)
		vNodeName = FILE_TREE_getObjectName(vNodeID)
		if(vNodeName != "")
			FILE_TREE.DUMMY = vNodeID
			return &lt;OK&gt;
		endif

		delitem pSelectionPath, 1
		getitem vNodeID, pSelectionPath, 1
	endwhile

	return &lt;OK&gt;

end ;lpApplySelectionPath

