<DAT name="INIT" xml:space='preserve'>variables
	String	 vMessages
	String	 vStatus
	Boolean bAutoSync
endvariables

message/nobeep "Starting UnifaceVC..."
show

$settingsSvc$-&gt;getFullSyncDefault(CHK_FULLSYNC.BUTTONS, vMessages)
#include VC_LIBRARY:IP_POSTCALL_PRES

$settingsSvc$-&gt;getAutoSync(bAutoSync, vMessages)
#include VC_LIBRARY:IP_POSTCALL_PRES

if (bAutoSync)

	;----------------------------------------------------
	; Export changed objects, sync repository with 
	; working copy &amp; load screen, selecting the root node
	;----------------------------------------------------
	call lpSync("root")

	message/hint "Synchronizing with filesystem. No integrated Version control interface configured."


else

	; Sync deferred until Sync button pressed (e.g. Shared repository)
	call lpShowTree("root") ; Build and show tree
	if ($status &lt; 0)
		message/error "An error occurred - please see message frame for details"
	else
		call lpDoMessages("Press sync to export Uniface changes prior to updating.")
		message/clear
	endif

endif

;----------------------------------------------------
; Show the screen.
;----------------------------------------------------
edit
</DAT>
<DAT name="GENERAL" xml:space='preserve'>#include VCOMMON_LIBRARY:IP_ADD_MESSAGE
