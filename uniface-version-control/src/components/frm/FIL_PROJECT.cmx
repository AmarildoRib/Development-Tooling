	$settingsSvc$-&gt;getWorkingCopyPath(vBaseDir, vMessages)
	#include VC_LIBRARY:IP_POSTCALL_PRES

	call lpBuildNodesForFolder(vBaseDir, vNodePath, pID)

	return &lt;OK&gt;

