FIELD_TEMPLATE_PATH  templates\field\template\&lt;object&gt;.ftx
FLD_INTERFACE_PATH   templates\field\interface\&lt;object&gt;.fix
FLD_LAYOUT_PATH      templates\field\layout\&lt;object&gt;.flx
FLD_SYNTAX_PATH      templates\field\syntax\&lt;object&gt;.fsx
~

end ;getWorkingCopyPath


;--------------------------------------------------------------------
; This will be the same as the working copy path.
; TODO: These operations were kept seperate when we supported
; Uniface clustering preferences, at some point we should renamed
; and merge them.
;--------------------------------------------------------------------
operation getBaseExportPath
;--------------------------------------------------------------------
params
	String pBaseExportPath		: OUT
	String pMessages			: INOUT
endparams

variables
	Boolean	vUseClusteringPrefs
	String	vClusterPrefs;, vUsysPathLogical, vSourceDir
endvariables

	$status = $instancehandle-&gt;getWorkingCopyPath(pBaseExportPath, pMessages)
	if($status &lt; 0)
		return &lt;FAILED&gt;
	endif
	if ($lfileexists(pBaseExportPath) = 0)
		ldircreate pBaseExportPath
		if($status &lt; 0)
			putitem pMessages, -1, "Error creating/setting SOURCE_DIR directory!"
			putitem pMessages, -1, $concat("SOURCE_DIR = ", pBaseExportPath)
			return &lt;FAILED&gt;
		endif
	endif

	return &lt;OK&gt;

end ;getBaseExportPath


;--------------------------------------------------------------------
; Returns true if libraries should be included with library objects.
;--------------------------------------------------------------------
operation getIncludeLib
;--------------------------------------------------------------------
params
	Boolean pIncludeLib	: OUT
	String  pMessages	: INOUT
endparams

variables
	Boolean	vUseClusteringPrefs
endvariables

	call lpGetSetting("INCLUDE_LIB", pIncludeLib, pMessages)
	return &lt;OK&gt;

end ;getIncludeLib


;--------------------------------------------------------------------
; Return a flag specifying whether to export Uniface signatures
;--------------------------------------------------------------------
operation getExportSignatures
;--------------------------------------------------------------------
