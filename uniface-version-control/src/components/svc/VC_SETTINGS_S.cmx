;-----------------------------------------------
entry lpGetSetting
;-----------------------------------------------
params
	String pSetting   : IN
	String pValue     : OUT
	String pMessages  : INOUT
endparams

	pValue = $logical(pSetting)
	return &lt;OK&gt;

end ;lpGetSetting


;----------------------------------------------
entry lpGetBooleanSetting
;----------------------------------------------
params
	string  psSetting  : IN
	boolean pbValue    : OUT
	string  psMessages : INOUT
endparams

	call lpGetSetting(psSetting, pbValue, psMessages)
	if (pbValue = "")	; Default to true if not set
		pbValue = 1
	endif
    return (0)

end ; lpGetBooleanSetting
