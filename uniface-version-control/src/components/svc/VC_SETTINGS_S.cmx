<DAT name="SFUNC" xml:space='preserve'>;--------------------------------------------------------------------
; Return the path of the current source directory
;--------------------------------------------------------------------
operation getWorkingCopyPath
;--------------------------------------------------------------------
params
	String pWorkingCopyPath : OUT
	String pMessages        : INOUT
endparams

variables
	String vWorkingDir
	String vAsnFile
	String vUsysPathLogical, vSourceDir
endvariables

	call lpGetSetting("SOURCE_DIR", pWorkingCopyPath, pMessages)

	if (pWorkingCopyPath = "") ; Logical Not configured
		putitem pMessages, -1, "Not configured (via .asn file logicals) - export synchronization will not be performed"
		putitem pMessages, -1, "Auto-migration of settings from previous versions (VC_SETTINGS table &amp; vc_path_settings.xml) is not currently available"

		; Create a template include .asn if one doesn't already exist
		if ($lfileexists("asn") = 2)
			vAsnFile = "asn\vc.asn"
		elseif ($lfileexists("adm") = 2)
			vAsnFile = "adm\vc.asn"
		else
			vAsnFile = "vc.asn"
		endif
		if ($lfileexists(vAsnFile) = 0)
			lfiledump /text $defaultLogicals, vAsnFile
			if ($status &gt;= 0)
				putitem pMessages, -1, "Default settings have been written to %%vAsnFile%%% for review &amp; inclusion in your idf.asn"
			endif
		else
			putitem pMessages, -1, "Please include %%vAsnFile%%% in your idf.asn, e.g. #file %%vAsnFile%%%, and restart"
		endif
		return &lt;FAILED&gt;
	endif

	if ($lowercase(pWorkingCopyPath[1:4]) = "usys" &amp; $split(pWorkingCopyPath, 1, ":", vUsysPathLogical, vSourceDir) &gt; 0)
		if ($fileexists($concat(vUsysPathLogical, ":")) = 0)
			putitem pMessages, -1, $concat("The directory specified by the USYS path logical ", vUsysPathLogical, " does not exist!")
			putitem pMessages, -1, "Please map the USYS path logical to an existing directory."
			return &lt;FAILED&gt;
		else
			if (vSourceDir[1:1] = "\") vSourceDir = vSourceDir[2]
			pWorkingCopyPath = $concat($item("FULLPATH", $fileproperties($concat(vUsysPathLogical, ":"))), vSourceDir)
		endif
	elseif (pWorkingCopyPath[1:2] != "\\" &amp; $scan(pWorkingCopyPath, ":") = 0)
		;The source directory is relative to the working directory
		vWorkingDir = $ldir()
		pWorkingCopyPath = lpConcatPaths(vWorkingDir, pWorkingCopyPath)
	endif
	return &lt;OK&gt;

defaultLogicals:blockdata ~
;Version Control Sync Tool Default Configuration - please modify and include in your .asn
[LOGICALS]
;The root folder to export source files to (relative to working directory)
SOURCE_DIR              src
;Run sync automatically when opening tool (Project menu option)
AUTO_SYNC               1
;Include signatures in the component export files
INCLUDE_COMPONENT_SIGS  0
;Force synchronization of all objects
;When enabled a number of performance optimisations are disabled
FULL_SYNC_DEFAULT       0
;Include library definition in library object exports
INCLUDE_LIB             0
;Enable exporting of signatures that only have a Uniface implementation
EXPORT_UF_SIG           0

;Source Export Paths - relative to SOURCE_DIR above
FORM_PATH            components\frm\&lt;object&gt;.cmx
