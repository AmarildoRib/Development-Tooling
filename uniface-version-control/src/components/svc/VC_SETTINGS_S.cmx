	Boolean pFullSync : OUT
	String  pMessages : INOUT
endparams

	call lpGetSetting("EXPORT_UF_SIG", pFullSync, pMessages)

end ;getExportSignatures


;--------------------------------------------------------------------
; Return the full sync default
;--------------------------------------------------------------------
operation getFullSyncDefault
;--------------------------------------------------------------------
params
	Boolean pFullSync : OUT
	String  pMessages : INOUT
endparams

	return lpGetBooleanSetting("FULL_SYNC_DEFAULT", pFullSync, pMessages)

end ;getFullSyncDefault


;--------------------------------------------------------------------
; Given an object type, returns a relative path to export it to.
; If a path hasn't been given then pExportPath will be "".
;--------------------------------------------------------------------
operation getExportPathForObject
;--------------------------------------------------------------------
params
	String	pObjectType	: IN
	String	pExportPath	: OUT
	String	pMessages	: INOUT
endparams

variables
	String	vSettingName
endvariables

	selectcase pObjectType
		case "&lt;FORM&gt;"
			vSettingName			= "FORM_PATH"
		case "&lt;SERVICE&gt;"
			vSettingName 			= "SVC_PATH"
		case "&lt;REPORT&gt;"
			vSettingName 			= "REPORT_PATH"
		case "&lt;ENT_SVC&gt;"
			vSettingName 			= "ENT_SVC_PATH"
		case "&lt;SESSION_SVC&gt;"
			vSettingName 			= "SESSION_SVC_PATH"
		case "&lt;USP&gt;"
			vSettingName 			= "USP_PATH"
		case "&lt;DSP&gt;"
			vSettingName 			= "DSP_PATH"
		case "&lt;PROC&gt;"
			vSettingName 			= "GLOBAL_PROC_PATH"
		case "&lt;CONSTANT&gt;"
			vSettingName 			= "CONSTANT_PATH"
		case "&lt;GLYPH&gt;"
			vSettingName 			= "GLYPH_PATH"
		case "&lt;INCLUDE&gt;"
			vSettingName 			= "INCLUDE_PATH"
		case "&lt;MENU&gt;"
			vSettingName 			= "MENU_PATH"
		case "&lt;MESSAGE&gt;"
			vSettingName 			= "MESSAGE_PATH"
		case "&lt;LIBRARY&gt;"
			vSettingName 			= "LIBRARY_DEF_PATH"
		case "&lt;MODEL&gt;"
			vSettingName 			= "MODEL_PATH"
		case "&lt;SIGNATURE&gt;"
			vSettingName 			= "SIGNATURE_PATH"
		case "&lt;STARTUPSHELL&gt;"
			vSettingName 			= "STARTUP_SHELL_PATH"
		case "&lt;FIELD_TEMPLATE&gt;"
			vSettingName 			= "FIELD_TEMPLATE_PATH"
		case "&lt;VARIABLES&gt;"
			vSettingName 			= "VARIABLES_PATH"
		case "&lt;TRANSLATION_TABLE&gt;"
			vSettingName 			= "TRANSLATION_PATH"
		case "&lt;PANEL&gt;"
			vSettingName 			= "PANEL_PATH"
		case "&lt;FORMAT&gt;"
			vSettingName 			= "FORMAT_PATH"
		case "&lt;FIELD_SYNTAX&gt;"
			vSettingName 			= "FLD_SYNTAX_PATH"
		case "&lt;FIELD_INTERFACE&gt;"
			vSettingName 			= "FLD_INTERFACE_PATH"
		case "&lt;FIELD_LAYOUT&gt;"
			vSettingName 			= "FLD_LAYOUT_PATH"
		case "&lt;ENTITY_INTERFACE&gt;"
			vSettingName 			= "ENT_INTERFACE_PATH"
		case "&lt;DEVICE_TABLE&gt;"
			vSettingName 			= "DEVICE_PATH"
		elsecase 
			return &lt;FAILED&gt;
	endselectcase 

	pExportPath = ""

	;Get the path from our settings
	call lpGetSetting(vSettingName, pExportPath, pMessages)

	return &lt;OK&gt;

end ;getExportPathForObject


