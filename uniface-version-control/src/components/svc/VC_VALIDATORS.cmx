operation setMessageCallback
;-----------------------------------------------------------------------------
params
	$MSG_HANDLE$ : IN
	$MSG_OP$     : IN
endparams

	return &lt;OK&gt;

end ; setMessageCallback


;-----------------------------------------------------------------------------
; Log export
;
; Post-conditions: VC_STATUS record created or updated with supplied timestamp
;-----------------------------------------------------------------------------
operation logExport
;-----------------------------------------------------------------------------
params
	;String		pExportID		: IN ; DJA 22/08/2012 No longer needed
	String		pFilePath		: IN
	DateTime	pTimeStamp	: IN
	String		pMessages		: INOUT
endparams

	clear/e "VC_STATUS"
	;ID.VC_STATUS/init = pExportID ; DJA 22/08/2012 Not needed now keyed by fileepath
	FILEPATH.VC_STATUS/init = pFilePath
	retrieve/e "VC_STATUS"
	if($status = -2 | $status = -4)
		;Item hasn't been exported before
		clear/e "VC_STATUS"
		;ID.VC_STATUS = pExportID
		FILEPATH.VC_STATUS = pFilePath
	elseif($status &lt; 0)
		return &lt;FAILED&gt;
	endif
	
	REPOSITORY_DT.VC_STATUS = pTimeStamp
	FILEMOD_DT.VC_STATUS    = $valuepart($lfileproperties(pFilePath, "MODIFICATIONDATE"))
	
	store/e "VC_STATUS"
	if($status &lt; 0)
		call lpAddMessage("&lt;$componentname&gt;: Store Failed: %%$status%%%", pMessages)
		return &lt;FAILED&gt;
	endif

	call lpAddMessage($concat(pFilePath, " exported"), pMessages)
	return &lt;OK&gt;

end ;logExport</DAT>
