<DAT name="FORMPIC" xml:space='preserve'>
 &uFRM;TYP=E&uSEP;NAM=VC_STATUS.VC&uSEP;WID=20&uSEP;HEI=1&uSEP;HOC=20&uSEP;VOC=1&uFRM;</DAT>
<DAT name="LISTING" xml:space='preserve'>Subversion Validators</DAT>
<DAT name="PERF">#IDF</DAT>
<DAT name="SFUNC" xml:space='preserve'>;--------------------------------------------------------------------
; Only export if the timestamp is newer than the last exported
; timestamp. If object is a signature, only export if it contains
; a non-Uniface implementation (otherwise it will be recreated)
;
; Signatures will not be exported if they only have a Uniface 
; implementation when pExportUfSig is false
;--------------------------------------------------------------------
operation validateExport
;--------------------------------------------------------------------
params
	String    pObjectType     : IN
	Boolean   pExportUfSig    : IN
	String		pExportFilePath : IN
	DateTime 	pLastModified   : IN
	String		pLibrary        : IN
	String		pLanguage       : IN
	String		pName           : IN
	Boolean  	pIsChanged      : OUT
	String		pMessages       : INOUT
endparams

variables
	Boolean  vIsUnifaceExport
	String   vUnifaceRelease
	String   vObjects
	String   vObject
	String   vObjectType
	DateTime vObjectTimeStamp
	String   vObjectName
	String   vObjectLang
	String   vObjectLib
	Boolean  vExpected
	DateTime vLatestTimeStamp
	Boolean  vIsExported
	Numeric  vNonUfImplCount
endvariables

	; If signature, we can choose whether to export them if they 
	; only have Uniface implementations 
	; (non-Uniface implementations must always be exported)
	if (!pExportUfSig &amp; pObjectType = "signature")
		activate "VREPO_COMMON".getNonUnifaceImplCount(pName, vNonUfImplCount)
		if (vNonUfImplCount = 0)	; Only a Uniface implementation found
			pIsChanged = 0			; no need to export
			return &lt;OK&gt;
		endif
	endif

	; Check when last exported
	clear/e "VC_STATUS"
	;ID.VC_STATUS/init = pExportID
	FILEPATH.VC_STATUS/init = pExportFilePath
	retrieve/e "VC_STATUS"
	if ($status &gt;= 0)
		; We've found a VC_STATUS record, check if it has been exported
		if (REPOSITORY_DT.VC_STATUS != "")
			; We've found a record of a previous export, set vLatestTimestamp to last export date
			vIsExported = 1
			vLatestTimeStamp = REPOSITORY_DT.VC_STATUS
		else
			vIsExported = 0
		endif
	elseif($status = -2 | $status = -4)
		;There's no VC_STATUS record, so this object hasn't been exported yet
		vIsExported = 0
	elseif($status &lt; 0)
		;Something is wrong. Don't export.
		call lpAddMessage("Tried to retrieve on VC_STATUS got %%$status%%% - halting export of object", pMessages)
		pIsChanged = 0
		return &lt;OK&gt;		
	endif

	;See if the file exists already
	if($lfileexists(pExportFilePath) != 1)
		;There is no file where we're going to export, so it hasn't been exported yet
		;and we will not be overwriting anything - go ahead.
		pIsChanged = 1
		return &lt;OK&gt;
	endif

	;There's an existing file so carry on
	if (!vIsExported)
		;No record of this item being exported or table not created yet.
		;Check the existing file for its timestamp
		activate "VREPO_EXPPARSER".parseUnifaceExport(pExportFilePath, vIsUnifaceExport, vUnifaceRelease, vObjects, pMessages)
		if($status &lt; 0)
			return &lt;FAILED&gt;
		endif
	
		if(!vIsUnifaceExport)
			;The file isn't a Uniface export, something is wrong. Don't export.
			call lpAddMessage("File at %%pExportFilePath%%% should be a Uniface export but it isn't - halting export of object", pMessages)
			pIsChanged = 0
			return &lt;OK&gt;
		endif

		vExpected = 0
		getitem vObject, vObjects, 1
		while($status &gt; 0)
			vObjectType      = $item("OBJECT_TYPE",      vObject)
			vObjectTimeStamp = $item("LATEST_TIMESTAMP", vObject)
			vObjectName      = $item("OBJECT_NAME",      vObject)
			vObjectLang      = $item("OBJECT_LANG",      vObject)
			vObjectLib       = $item("OBJECT_LIB",       vObject)

			if( (vObjectName==pName) &amp; (vObjectLang==pLanguage) &amp; (vObjectLib==pLibrary) )
				vExpected = 1
				; Set vLatestTimestamp to latest timestamp in all related Uniface source tables
				if(vLatestTimeStamp == "")
					vLatestTimeStamp = vObjectTimeStamp
				elseif(vObjectTimeStamp &gt; vLatestTimeStamp)
					vLatestTimeStamp = vObjectTimeStamp
				endif
			endif

			delitem vObjects, 1
			getitem vObject, vObjects, 1
		endwhile

		if(!vExpected)
			;The export isn't what we're expecting, something is wrong. Don't export.
			call lpAddMessage("File at %%pExportFilePath%%% should be %%pName%%% but it isn't - halting export of object", pMessages)
			pIsChanged = 0
			return &lt;OK&gt;
		endif

	endif

	; If the last exported timestamp is older than the timestamp in the export it has changed
	if( (vLatestTimeStamp &lt; pLastModified) | (vLatestTimeStamp = "" &amp; pLastModified != "") )
		pIsChanged = 1
	else
		pIsChanged = 0
	endif

	return &lt;OK&gt;

end ;validateExport


