<DAT name="GENERAL" xml:space='preserve'>;---------------------------------------
entry lpAddMessage
;---------------------------------------
params
	String	pMessage	: IN
	String	pMessages	: INOUT
endparams

variables
	Numeric	vStatus
endvariables

	vStatus = $status
	if ($MSG_HANDLE$)
		$MSG_HANDLE$-&gt;"%%$MSG_OP$%%%"("%%pMessage%%% [in &lt;$componentName&gt;]")
	else
		putitem pMessages, -1, "%%pMessage%%% [in &lt;$componentName&gt;]"
	endif
	return vStatus

end ;lpAddMessage
