<DAT name="GENERAL" xml:space='preserve'>#include VCOMMON_LIBRARY:IP_ADD_MESSAGE
#include VCOMMON_LIBRARY:IP_METADICTINFO
#include VCOMMON_LIBRARY:IP_CONCAT_PATHS
#include VCOMMON_LIBRARY:IP_GETOBJMODTIME


;-----------------------------------------
entry lpExportObjects
;-----------------------------------------
returns Numeric

params
	String  pObjectType         : IN
	String  pProfile            : IN
	String  pRelativeExportPath : IN
	Boolean pForce              : IN
	Boolean pPartialSync        : IN
	Boolean pIncludeSig         : IN
	Boolean pExportUfSig        : IN
	Boolean pIncludeLib         : IN
	String  pGroupBy            : IN
	String  pMessages           : INOUT
endparams

variables
   String   vMetaInfo
   String   vEntName
   String   vLabelFieldName
   String   vProfiles
   String   vProfile
   String   vProfileField
   String   vProfileValue
   String   vUDEType
   String   vLanguageField
   String   vLibraryField
   String   vTimeStampField
   String   vChildTimeStampEntities
   String   vChildTimeStampFields
   String   vChildTimeStampEntity
   String   vChildTimeStampField
   Boolean  vIsChanged
   String   vLabelFieldValue
   String   vLanguageFieldValue
   String   vLibraryFieldValue
   String   vTimeStampFieldValue
   DateTime vTimeStamp
   String   vExportFilePath
   Numeric  vExportCount
   Boolean  bWarning
   String   sTimestamp
   String   vDesc
endvariables

	;TODO: If pObjectType is invalid, an infinite loop can happen here
	vMetaInfo = lpGetMetaDictInfoForObjectType(pObjectType, pMessages)
	vEntName        = $item("ENTNAME",        vMetaInfo)
	vLabelFieldName = $item("LABELFIELD",     vMetaInfo)
	vProfiles       = $item("PROFILES",       vMetaInfo)
	vUDEType        = $item("UDETYPE",        vMetaInfo)
	vLanguageField  = $item("LANGFIELD",      vMetaInfo)
	vLibraryField   = $item("LIBFIELD",       vMetaInfo)
	vTimeStampField = $item("TIMESTAMPFIELD", vMetaInfo)
   
	bWarning = 1

	;Clear the Entity
	clear/e vEntName

	;Add user provided profiles - some databases (e.g. SQLite) are case sensitive, make sure name is uppercase as per Uniface standards
	@vLabelFieldName = $uppercase($item("NAME", pProfile))
	if(vLanguageField != "")
		@vLanguageField = $item("LANG", pProfile)
	endif
	if(vLibraryField != "")	
		@vLibraryField = $item("LIB", pProfile)
	endif

	;Add any other profiles required for this object type
	getitem vProfile, vProfiles, -1
	while($status &gt; 0)

		vProfileField	= $idpart(vProfile)
		vProfileValue	= $valuepart(vProfile)
		if(@vProfileField == "")
			@vProfileField/init = vProfileValue		
		else
			@vProfileField/init = "%%@vProfileField%%%&uAND;%%vProfileValue%%%"
		endif

		delitem vProfiles, -1
		getitem vProfile, vProfiles, -1
	endwhile
 
	;Retrieve
	retrieve/e vEntName
	if ($status &lt; 0)
		if ($status = -2 | $status = -4)
			;Don't try to export if nothing found or table doesn't exist
			$status = &lt;OK&gt;
		else
			;Give more detailed message in the event of an error
			vDesc = $item("DESCRIPTION", $procerrorcontext)
			call lpAddMessage("Export [profile:%%pProfile%%%] failed with status %%$status%%%: %%vDesc%%% retrieving %%vEntname%%%", pMessages)
		endif
		return 0
	endif

	;Workaround bug in IDF where timestamps on the main object table aren't updated when a
	;child object is changed. For models and menus we search the child objects and manually
	;set the parent timestamp (we don't actually store it). This allows later processing to
	;assume that parent timestamps are always up to date.
	selectcase vEntname
	  case "UCSCH"
		setocc "UCSCH", 1
		while ($status &gt; 0 &amp; U_VLAB.UCSCH != "")
			#for SUBENTITY = (UCFIELD,UCTABLE,UCRELSH,UCKEY,UCGROUP,UCTABLE)
			selectdb max(UTIMESTAMP) from "&lt;SUBENTITY&gt;" u_where (U_VLAB.&lt;SUBENTITY&gt; = U_VLAB.UCSCH) to ($1)
			if ($1 &gt; UTIMESTAMP.UCSCH)
				UTIMESTAMP.UCSCH = $1
			endif
			#endfor

			selectdb max(UTIMESTAMP) from "UCDTYP" u_where (UMODELNAME.UCDTYP = U_VLAB.UCSCH) to ($1)
			if ($1 &gt; UTIMESTAMP.UCSCH)
				UTIMESTAMP.UCSCH = $1
			endif

			selectdb max(UTIMESTAMP) from "UDIAGRAM" u_where (UMODEL.UDIAGRAM = U_VLAB.UCSCH) to ($1)
			if ($1 &gt; UTIMESTAMP.UCSCH)
				UTIMESTAMP.UCSCH = $1
			endif

			setocc vEntName, $curocc(vEntName) + 1
		endwhile
		sort/e "UCSCH","UTIMESTAMP.UCSCH:d"

	  case "USMENU"
		setocc "USMENU", 1
		while ($status &gt; 0 &amp; U_VLAB.UCSCH != "")
			selectdb max(UTIMESTAMP) from "USITEM" u_where (UMENU.USITEM = UMENU.USMENU) to ($1)
			if ($1 &gt; UTIMESTAMP.USMENU)
				UTIMESTAMP.USMENU = $1
			endif
			setocc vEntName, $curocc(vEntName) + 1
		endwhile
		sort/e "USMENU","UTIMESTAMP.USMENU:d"

	endselectcase

	;Loop over everything we found and export it
	if ($status &gt;= 0)
		setocc vEntName, 1
	endif
	while ($status &gt; 0)
		vIsChanged = 1

		if (vLabelFieldName != "")
			vLabelFieldValue = @vLabelFieldName
		endif
		if (vLanguageField != "")
			vLanguageFieldValue = @vLanguageField
		endif
		if (vLibraryField != "")
			vLibraryFieldValue = @vLibraryField
		endif
		if (vTimeStampField != "")
			vTimeStampFieldValue = @vTimeStampField
		endif

		;Get the latest modification time stamp for this object
		;TODO: can we remove these special cases?
		selectcase vEntname
		case "UCSCH"
			vTimeStamp = UTIMESTAMP.UCSCH
		case "USMENU"
			vTimeStamp = UTIMESTAMP.USMENU
		elsecase
			call lpGetLatestTimeStampForObject(pObjectType, vTimeStampFieldValue, vTimeStamp, pMessages)
		endselectcase

		;Work out where we should be exporting this object to
		vExportFilePath	= lpBuildExportFilePath(pRelativeExportPath, vLibraryFieldValue, vLanguageFieldValue, vLabelFieldValue)

		if (!pForce)
			vIsChanged = 1

			if ($PRE_ADD_HANDLE$)
				; Pass export type &amp; export Uniface signature preference
				; so validator can check if signatures need to be exported 
				;(can either be exported only if non-Uniface implementation, or always exported)
				$status = $PRE_ADD_HANDLE$-&gt;"%%$PRE_ADD_OP$%%%"(pObjectType, pExportUfSig, vExportFilePath, vTimeStamp, vLibraryFieldValue, vLanguageFieldValue, vLabelFieldValue, vIsChanged, pMessages)
				if ($status &lt; 0)
					call lpAddMessage("Pre add to export list call to %%$PRE_ADD_OP$%%% failed with status %%$status%%%", pMessages)
					$status = &lt;FAILED&gt;
					commit
					return &lt;FAILED&gt;
				endif
			endif

		endif

		if (vIsChanged | pForce)
			call lpExportObject(vLabelFieldValue, %\
									vUDEType, %\
									vLanguageFieldValue, %\
									vLibraryFieldValue, %\
									vTimestamp, %\
									vExportFilePath, %\
									pIncludeSig, %\
									pIncludeLib, %\
									pGroupBy, %\
									pMessages %\
			                   )
			if ($status = &lt;OK&gt;)
				;Increment our export count
				vExportCount += 1
			endif
		endif

		; All the retrieves should be in UTIMESTAMP order so if we've hit one that hasn't been modified then
		; all subsequent objects wont have been modified either. 
		; For a partial sync, we can just stop at this point and return the list we have. 
		; A full sync will take longer, but will recreate missing exports etc.
		if ( @vTimeStampField == "" )
			if ( bWarning )
				; only one warning for each type (no dialog)
				bWarning = 0
				sTimestamp = $replace( vTimeStampField, 1, ".", " in " )
				call lpAddMessage("empty timestamp fields exist for export type '%%pObjectType%%%' (%%sTimestamp%%%) - please update", pMessages)
			endif
		else
			if ( pPartialSync &amp; !vIsChanged &amp; !pForce  )
				break
			endif
		endif
     
		setocc vEntName, $curocc(vEntName) + 1
	endwhile

	;TODO: what's this committing?
	commit

	$status = &lt;OK&gt;
	return vExportCount

end ;lpExportObjects


;-----------------------------------------
; Export an individual object
; validating and logging if required
;-----------------------------------------
entry lpExportObject
;-----------------------------------------
params
	String   pName               : IN
	String   pObjectType         : IN
	String   pLanguage           : IN
	String   pLibrary            : IN
	DateTime pLastModified       : IN
	String   pExportFilePath     : IN
	Boolean  pIncludeSig         : IN ; Include signatures in components?
	Boolean  pIncludeLib         : IN
	String   pGroupBy            : IN
	String   pMessages           : INOUT
endparams

variables
	String  vName
	String  vFilePathList
	String  vTempFilePath
	String  vOptions
	String  vItemType
	Boolean vGroupExported
	String  vExportedGroups
	Boolean vDoExport
	Boolean vIncludeAll
	String  vFileProperties
	String  vFileAttributes
	Boolean vDoLog
	String  vModdedLocked
	Handle  vExportLogger
endvariables

	vDoLog	= 1

	vName = $uppercase(pName)

	vFilePathList = $replace(pExportFilePath, 1, "\", "&uSEP;", -1)
	delitem vFilePathList, -1
	vTempFilePath = $replace(vFilePathList, 1, "&uSEP;", "\", -1)
	call lpCreatePath(vTempFilePath)

	vIncludeAll	= 0
	vDoExport	= 1
	selectcase pGroupBy
		case "LIB"
			; If we're grouping the export by library only do an export
			; once per library and change the name to &uALL; and the
			; language to ""
			getitem/id vGroupExported, vExportedGroups, pLibrary
			if(vGroupExported != "")
				vDoExport = 0
			endif
			vName		= "&uALL;"
			pLanguage	= ""
			putitem/id vExportedGroups, pLibrary, 1
		case "LANG"
			; If we're grouping the export by language only do an export
			; once per library and change the name to &uALL; and the
			; library to ""
			getitem/id vGroupExported, vExportedGroups, pLanguage
			if(vGroupExported != "")
				vDoExport = 0
			endif
			vName		= "&uALL;"
			pLibrary	= ""
			putitem/id vExportedGroups, pLanguage, 1
		case "LIB_AND_LANG"
			; If we're grouping the export by library and language
			; only do an export once per library/language pair and
			; change the name to &uALL;.
			getitem/id vGroupExported, vExportedGroups, $concat(pLibrary, "_", pLanguage)
			if(vGroupExported != "")
				vDoExport = 0
			endif
			vName		= "&uALL;"
			putitem/id vExportedGroups, $concat(pLibrary, "_", pLanguage), 1
		case "ALL_IN_ONE"
			; If we're grouping the export by type only do an export
			; once. Change the name to &uALL; and the library and language
			; to ""
			getitem/id vGroupExported, vExportedGroups, "ALL_IN_ONE" ;Only going to do the one import
			if(vGroupExported != "")
				vDoExport = 0
			endif
			vName		= "&uALL;"
			pLibrary	= ""
			pLanguage	= ""
			putitem/id vExportedGroups, "ALL_IN_ONE", 1
		case "INC_ALL"
			; Exporting a library and including all its objects in the file
			getitem/id vGroupExported, vExportedGroups, "INC_ALL" ;Only going to do the one import
			if(vGroupExported != "")
				vDoExport = 0
			endif
			vIncludeAll = 1
			putitem/id vExportedGroups, "INC_ALL", 1
	endselectcase

	if(pLanguage != "")
		putitem/id vOptions, "language", pLanguage
	endif
	if(pLibrary != "")
		putitem/id vOptions, "library", pLibrary
	endif

	;-------------------------------------------------
	; Fire the file level pre-export validator
	;-------------------------------------------------
	if ($PRE_EXPORT_HANDLE$)
		$status = $PRE_EXPORT_HANDLE$-&gt;"%%$PRE_EXPORT_OP$%%%"(pExportFilePath, vDoExport, vDoLog, pMessages)
		if($status &lt; 0)
			call lpAddMessage("Pre export validator call to %%$PRE_EXPORT_OP$%%% failed with status %%$status%%%", pMessages)
			$status = &lt;FAILED&gt;
			return &lt;FAILED&gt;
		endif
	endif

	if(vDoExport)
   		call lpExportToXML(vName, pObjectType, pExportFilePath, pLibrary, pIncludeLib, pIncludeSig, vIncludeAll, vOptions)
		if($status &lt; 0)
			call lpAddMessage("Failed to export %%pObjectType%%% %%vName%%% to %%pExportFilePath%%% - halting export of object", pMessages)
			$status = &lt;FAILED&gt;
			return &lt;FAILED&gt;
		endif
	endif

	;-------------------------------------------------
	; Fire the post export logger
	;-------------------------------------------------
	if (vDoLog &amp; $LOGGER$)
		$status = $LOGGER$-&gt;"%%$LOGGER_OP$%%%"(pExportFilePath, pLastModified, pMessages)
		if($status &lt; 0)
			call lpAddMessage("Logger operation %%$LOGGER_OP$%%% call failed with status %%$status%%%", pMessages)
			$status = &lt;FAILED&gt;
			return &lt;FAILED&gt;
		endif
	endif

	return &lt;OK&gt;

end ;lpExportObject


;---------------------------------------
entry lpExportToXML
;---------------------------------------
params
	String  pItem        : IN
	String  pType        : IN
	String  pFileName    : IN
	String  pLibrary     : IN
	Boolean pIncludeLib  : IN
	Boolean pIncludeSig  : IN
	Boolean pIncludeAll  : IN
	String  pOptions     : IN
endparams

variables
	String  vBaseType
	Numeric vExportResult
	String  vInputRecords
	String  vOutputRecords
endvariables

	if(pIncludeLib)
		;Is this export a library object?
		getitem vBaseType, pType, 1
		if(vBaseType == "&lt;library&gt;")
			if(pType == "&lt;library&gt;")
				;We don't need to export the library seperately
				return &lt;OK&gt;
			endif
			putitem/id pOptions, "keepopen", 1
			vExportResult = $ude("export", "library", pLibrary, pFileName, pOptions)
			if(vExportResult &lt; 0)
				return &lt;FAILED&gt;
			endif
			delitem/id pOptions, "keepopen"
			putitem/id pOptions, "append", 1
		endif
	endif

	if(pIncludeSig)
		if(pType == "component")
			putitem/id pOptions, "keepopen", 1
			vExportResult = $ude("export", pType, pItem, pFileName, pOptions)
			if(vExportResult &lt; 0)
				return &lt;FAILED&gt;
			endif
			delitem/id pOptions, "keepopen"
			putitem/id pOptions, "append", 1
			pType = "&lt;signature&gt;"
		endif
	endif

	putitem/id pOptions, "printinterval", -1
	vExportResult = $ude("export", pType, pItem, pFileName, pOptions)
	if(vExportResult &lt; 0)
		return &lt;FAILED&gt;
	endif

	vInputRecords  = $item("InputRecords",	$procreturncontext)
	vOutputRecords = $item("OutputRecords",	$procreturncontext)

	if(vInputRecords != vOutputRecords)
		return &lt;FAILED&gt;	
	endif

	return &lt;OK&gt;

end ;lpExportToXML


;---------------------------------------
entry lpCreatePath
;---------------------------------------
params
	String	pFilePath	: IN
endparams

variables
	String	vFilePath
	String	vFilePathList
endvariables

	if($lfileexists(pFilePath) = 2)
		;This directory exists, so we don't need to do anything
		return &lt;OK&gt;
	endif

	;pFilePath doesn't exist, so we need to create it.
	;First we need to see if the parent directories have been created.
	vFilePathList = $replace(pFilePath, 1, "\", "&uSEP;", -1)
	delitem vFilePathList, -1
	vFilePath = $replace(vFilePathList, 1, "&uSEP;", "\", -1)

	call lpCreatePath(vFilePath)
	if($status &lt; 0)
		return &lt;FAILED&gt;
	endif

	ldircreate pFilePath

	return &lt;OK&gt;

end ;lpCreatePath


;---------------------------------------
entry lpBuildExportFilePath
;---------------------------------------
returns String

params
	String	pRelativeExportPath	: IN
	String	pLibrary				: IN
	String	pLanguage				: IN
	String	pName					: IN
endparams

variables
	String	vExportFilePath
endvariables

	vExportFilePath	 = lpConcatPaths($baseDir$, pRelativeExportPath)

	if(pLibrary != "")
		vExportFilePath = $replace(vExportFilePath, 1, "%%&lt;%%%lib%%&gt;%%%",	 pLibrary, -1)			
	endif

	if(pLanguage != "")
		vExportFilePath = $replace(vExportFilePath, 1, "%%&lt;%%%lang%%&gt;%%%", pLanguage, -1)
	endif

	if( $scan(vExportFilePath, "&lt;object&gt;") &gt; 0)
		vExportFilePath = $replace(vExportFilePath, 1, "%%&lt;%%%object%%&gt;%%%", pName, -1)
	endif

	$status = &lt;OK&gt;
	return vExportFilePath

end ;lpBuildExportFilePath</DAT>
