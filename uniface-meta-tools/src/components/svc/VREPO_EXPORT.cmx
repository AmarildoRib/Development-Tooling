<DAT name="SFUNC" xml:space='preserve'>;--------------------------------------------------------------------
; Run when a new instance is created
;--------------------------------------------------------------------
operation init
;--------------------------------------------------------------------
   
	;Set the project export base to a default
	$baseDir$ = $ldir()
	
	return &lt;OK&gt;
	
end ;init


;--------------------------------------------------------------------
operation cleanup
;--------------------------------------------------------------------

end ;cleanup


;--------------------------------------------------------------------
; Set the directory that all exports should go to. A relative path
; can be supplied to exportObjects to further specify the location
; of specific objects.
;--------------------------------------------------------------------
operation setBaseDirectory
;--------------------------------------------------------------------
params
	String pBaseDirectory : IN
	String pMessages      : INOUT
endparams

	if(pBaseDirectory == "")
		call lpAddMessage("Can't set Base Directory to blank", pMessages)
		return &lt;FAILED&gt;		
	endif

	if($lfileexists(pBaseDirectory) != 2)
		call lpAddMessage("Base directory %%pBaseDirectory%%% does not exist", pMessages)
		return &lt;FAILED&gt;
	endif

	$baseDir$ = pBaseDirectory

	return &lt;OK&gt;

end ;setBaseDirectory


;--------------------------------------------------------------------
; Register a component that will be called to validate if an object
; should be exported.
;--------------------------------------------------------------------
operation addValidator
;--------------------------------------------------------------------
params
	Handle pValidator : IN
	String pOperation : IN
	String pType      : IN
	String pMessages  : INOUT
endparams

	;TODO: remove entity (left in case we need to support multiple validators
	;      of the same type, which has been removed for performance and simplicity
	;if(VALIDATOR_HANDLE.VALIDATOR != "")
	;	creocc "VALIDATOR", -1 
	;endif

	;VALIDATOR_HANDLE.VALIDATOR	= pValidator
	;TYPE.VALIDATOR				= pType

	selectcase pType
		case "PRE_EXPORT"
			$PRE_EXPORT_HANDLE$ = pValidator
			$PRE_EXPORT_OP$     = pOperation
		case "PRE_ADD_TO_EXPORT_LIST"
			$PRE_ADD_HANDLE$    = pValidator
			$PRE_ADD_OP$        = pOperation
		case "LOGGER"
			$instanceHandle-&gt;setLogger(pValidator, pOperation)
		elsecase
			pMessages = "Validator type %%pType not currently supported in VREPO_EXPORT"
	endselectcase

	return &lt;OK&gt;

end ;addValidator


;--------------------------------------------------------------------
; set logger
;--------------------------------------------------------------------
operation setLogger
;--------------------------------------------------------------------
params
	$LOGGER$        : IN ; Handle for logger instance
	$LOGGER_OP$     : IN ; Operation to invoke
	String pMessage : OUT
endparams

	if (!$LOGGER$ | $LOGGER_OP$ = "")
		pMessage = "Logger instance handle or operation [%%$LOGGER_OP$%%%] not supplied to VREPO_EXPORT"
		return -1
	endif
	return 0

end ; setLogger


;--------------------------------------------------------------------
; Export an object or objects or a particular type to a given path
; In Params:
;	OBJECT_PROFILE	-	A profile for the name of the objects to be
;						exported (optional). If blank then all objects
;						of OBJECT_TYPE will be exported.
;							NAME	- Profile on the object name
;							LANG	- Profile on the object language
;							LIB		- Profile on the object library
;	OBJECT_TYPE		-	The type of the object to be exported:
;								form
;								service
;								report
;								entity_service
;								session_service
;								usp
;								proc
;								constants
;								glyph
;								include
;								menu
;								message
;								library
;								model
;								signature
;								startupshell
;								field_template
;								dsp
;								variables
;								translation_table
;								panel
;								format
;								field_syntax
;								field_interface
;								field_layout
;								entity_interface
;								device_table
;								deployment_archive
;								subsystem
;	EXPORT_PATH		-	The path to export to. Relative to $baseDir$
;						of this VREPO_EXPORT instance. If blank then
;						$baseDir$ will be used for the export.
;						Additionally, if EXPORT_PATH is blank, or ends
;						with a \ a &lt;object&gt;.xml will be added to the
;						end. This ensures a file is specified that we
;						can export to.			
;	FORCE_EXPORT	-	Ignore the last exported date when performing
;						this export.
;	PARTIAL_SYNC		- Partial synchronization flag. Default is false.
;						Exports are processed in descending timestamp order, 
;						and if set, exports will stop when we encounter an object
;						which doesn't need exporting (so a deleted export will not
;						be recreated)
;	INCLUDE_LIB		-	If INCLUDE_LIB is true and the object is a
;						library, then the library definition will be
;						prepended to the export file. If INCLUDE_LIB
;						is true and the export library is of type
;						&lt;LIBRARY&gt; then nothing will be exported
;	INCLUDE_SIG		-	If INCLUDE_SIG is true and the object is a
;						component, then the signature definition will
;						be appended to the export file.
;	EXPORT_UF_SIG		- If false, Uniface signatures that don't have
;						  a non-Uniface implementation will not be exported
;						  (default true)
;	GROUP_BY		-	Group exports by:
;							LIB				- Library objects will be
;											grouped into a single file
;											by the library they belong
;											to.
;							LANG			- Library objects will be
;											grouped into a single file
;											by their language.
;							LIB_AND_LANG	- Library objects will be
;											grouped into a single file
;											by the library and language
;							ALL_IN_ONE		- Library objects are placed
;											in a single file based on
;											their type only.
;							INC_ALL			- Only used when exporting
;											library definitions.
;											Includes every library
;											object.
;--------------------------------------------------------------------
operation exportObjects
;--------------------------------------------------------------------
params
	String pInParams  : IN
	String pOutParams : OUT
	String pMessages  : INOUT
endparams

variables
	String  vObjectProfile
	String  vObjectType
	String  vDefaultObjectName
	String  vExportPath
	Boolean vForceAll
	Boolean vPartialSync
	Boolean vIncludeLib
	Boolean vIncludeSig
	Boolean vExportUfSig
	String  vGroupBy
	String  vObjectList
	Numeric vExportCount
	String  vResults
endvariables

	;If the base directory isn't set then we don't know where to export to
	if($baseDir$ = "")
		call lpAddMessage("Working copy directory not set", pMessages)
		return &lt;FAILED&gt;
	endif

	;Extract parameters from pInParams and set defaults where options haven't
	;been specified
	vObjectProfile = $item("OBJECT_PROFILE", pInParams)
	vObjectType    = $item("OBJECT_TYPE",    pInParams)
	if(vObjectType = "")
		call lpAddMessage("OBJECT_TYPE is a required parameter", pMessages)
		return &lt;FAILED&gt;		
	endif

	vForceAll = $item("FORCE_EXPORT", pInParams)
	if(vForceAll = "")
		vForceAll = 0
	endif

	vPartialSync = $item("PARTIAL_SYNC", pInParams)
	if(vPartialSync = "")
		vPartialSync = 0
	endif

	vGroupBy = $item("GROUP_BY", pInParams)
	selectcase vGroupBy
		case "LIB"
			vDefaultObjectName  = "%%&lt;%%%lib%%&gt;%%%.xml"
		case "LANG"
			vDefaultObjectName  = "%%&lt;%%%lang%%&gt;%%%.xml"
		case "LIB_AND_LANG"
			vDefaultObjectName  = "%%&lt;%%%lib%%&gt;%%%_%%&lt;%%%lang%%&gt;%%%.xml"
		case "ALL_IN_ONE"
			vDefaultObjectName  = "all.xml"
		case "INC_ALL"
			if(vObjectType != "&lt;LIBRARY&gt;")
				vGroupBy = ""
			endif
			vDefaultObjectName  = "%%&lt;%%%object%%&gt;%%%.xml"
		elsecase
			vGroupBy            = ""
			vDefaultObjectName  = "%%&lt;%%%object%%&gt;%%%.xml"
	endselectcase

	vExportPath = $item("EXPORT_PATH", pInParams)
	if(vExportPath = "")
		vExportPath = vDefaultObjectName
	elseif(vExportPath[$length(vExportPath)] = "\")
		vExportPath = $concat(vExportPath, vDefaultObjectName)
	endif

	vIncludeLib = $item("INCLUDE_LIB", pInParams)
	if(vIncludeLib = "")
		vIncludeLib = 0
	elseif(vIncludeLib = 1 &amp; vObjectType = "&lt;LIBRARY&gt;")
		;We're including library definitions with the library items,
		;don't export libraries on their own.
		return &lt;OK&gt;
	endif

	vIncludeSig = $item("INCLUDE_SIG", pInParams)
	if(vIncludeSig = "")
		vIncludeSig = 0
	endif

	vExportUfSig = $item("EXPORT_UF_SIG", pInParams)
	if(vExportUfSig = "")
		vExportUfSig = 1
	endif

	vExportCount	= lpExportObjects(vObjectType, vObjectProfile, vExportPath, vForceAll, vPartialSync, vIncludeSig, vExportUfSig, vIncludeLib, vGroupBy, pMessages)
	if($status &lt; 0)
		return &lt;FAILED&gt;
	endif

	if(vExportCount &gt; 0)
		call lpAddMessage("%%vObjectType%%% - Exported %%vExportCount%%% item(s)", pMessages)
	endif
	
	return &lt;OK&gt;

end ;exportObjects</DAT>
