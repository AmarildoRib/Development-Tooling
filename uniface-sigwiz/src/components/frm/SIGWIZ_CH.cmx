operation SHOW
	
	params
		$TASK$        : IN
		$UDEFPATH$    : IN
		$uspecnam$    : IN
		$uopernam$    : IN
		$uparmnam$    : IN
		$uhinterface$ : INOUT
	endparams

	variables
		numeric TotOccs
		string FieldValRep
	endvariables	
	
	uhinterface.dummy = $uhinterface$[1:32]

	selectcase $TASK$
		case "CHANGE_UHINTERFACE"
			$formtitle = "Change Handle Reference..."
			activate "SIGWIZ_SVC".GET_USPECNAM(uhinterface.dummy, $UDEFPATH$, FieldValRep, TotOccs)
			if (TotOccs = 0)
				if (uhinterface.dummy[1:2] = "U_")
					activate "SIGWIZ_SVC".GET_USPECNAM(uhinterface.dummy[2], $UDEFPATH$, FieldValRep, TotOccs)
				else
					activate "SIGWIZ_SVC".GET_USPECNAM("", $UDEFPATH$, FieldValRep, TotOccs)
				endif
			endif
		case "COPY_TO"
			$formtitle = "Copy Operation to..."
			activate "SIGWIZ_SVC".GET_USPECNAM("", $UDEFPATH$, FieldValRep, TotOccs)
		case "MOVE_TO"
			$formtitle = "Move Operation to..."
			activate "SIGWIZ_SVC".GET_USPECNAM("", $UDEFPATH$, FieldValRep, TotOccs)
		elsecase
			message/error "[&lt;$componentName&gt;] - Unknown Task: [%%$TASK$]!"
	endselectcase
	
	$valrep(USPECNAM.DUMMY) = FieldValRep
	USPECNAMNO.DUMMY/init = TotOccs
	
	$prompt = uhinterface.dummy

	edit/modal
	
end;- SHOW

