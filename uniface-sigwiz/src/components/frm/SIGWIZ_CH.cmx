<DAT name="ACCEPT" xml:space='preserve'>if (uhinterface.dummy != "")
	selectcase $TASK$
		case "CHANGE_UHINTERFACE", "COPY_TO", "MOVE_TO"
			if ($uhinterface$ = UHINTERFACE.DUMMY)
				message/error "Name has not been changed!"
				return(-1)
			elseif ($instancehandle("SIGWIZ_SVC")-&gt;CHECK_USPECNAM(UHINTERFACE.DUMMY, "") = 1)
				message/error "Name [%%uhinterface.dummy] not valid!"
				return(-1)
			else
				$uhinterface$ = UHINTERFACE.DUMMY
			endif
		elsecase
			message/error "[&lt;$componentName&gt;] - Unknown Task: [%%$TASK$]!"
			macro "^QUIT"
	endselectcase
else
	message/error "Name cannot be empty!"
	return(-1)
endif
exit(0)
</DAT>
