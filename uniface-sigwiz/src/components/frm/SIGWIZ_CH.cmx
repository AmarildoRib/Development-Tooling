<DAT name="GENERAL" xml:space='preserve'>; ********************************************************************************

entry LP_SEARCHUSPECNAM
	
	variables
		numeric    TotOccs
		string     sSearchPrf, FieldValRep
	endvariables
	
	sSearchPrf = $concat(USPECNAMSR.DUMMY, "&uALL;")
	activate "SIGWIZ_SVC".GET_USPECNAM(sSearchPrf,$UDEFPATH$, FieldValRep, TotOccs)
	if ($status = 0)
		$valrep(USPECNAM.DUMMY) = FieldValRep
		if ($item(USPECNAM.DUMMY, $valrep(USPECNAM.DUMMY)) = "") USPECNAM.DUMMY/init = ""
		USPECNAMNO.DUMMY/init = TotOccs
	else
		$valrep(USPECNAM.DUMMY) = ""
		USPECNAM.DUMMY/init = ""
		USPECNAMNO.DUMMY/init = ""
	endif
	
end;- LP_SEARCHUSPECNAM

