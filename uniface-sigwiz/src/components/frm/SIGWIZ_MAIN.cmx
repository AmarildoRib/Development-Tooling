<DAT name="INIT" xml:space='preserve'>variables
	numeric nTotOccs
	string  sSigWizIni, sUPaths, sUPathId, sUPathDesc, sFieldValRep
endvariables

$formtitle = "&lt;TITLE&gt;"

$Refresh$ = "False"

; Read UPATH settings
sSigWizIni = $logical("sigwiz.cfg")
if (sSigWizIni = "") sSigWizIni = "sigwiz.cfg"
sUPaths = $setting(sSigWizIni, "UPATH\&uALL;", "INISETTINGS")
if (sUPaths != "")
	forlist sUPathId in sUPaths
		sUPathDesc = $setting(sSigWizIni, $concat("UPATH\", $idpart(sUPathId)), "INIDATA")
		if $idpart(sUPathId) = "ALL" sUPathId = "&uALL;"
		putitem/id $valrep(UPATH.DUMMY), $idpart(sUPathId), sUPathDesc
	endfor
	UPATH.DUMMY/init = $idpart($itemnr(1, $valrep(UPATH.DUMMY)))
else
	putmess "Could not find UPATH section/settings in sigwiz.cfg file; using defaults..."
	putitem/id $valrep(UPATH.DUMMY), "COM", "COM Call-Out"
	UPATH.DUMMY/init = "COM"
endif

; *** The glyph U_PIXEL is 1x1 px in size and has the color white
; *** We need this glyph, since it aparently is necessary to define
; *** the properties NORMAL and SELECTED with a valid, non emppty
; *** value; if this is not done then a tree leaf (property NODE=F)
; *** is treated as a node!?!
;$properties(UPARAMS.DUMMY) = "LISTONLY=T&uSEP;LISTVIEW=R&uSEP;LABELS=Name&uNOT;Direction&uNOT;Data Type&uSEP;FORMATS=CL15&uNOT;CL7&uNOT;CL18&uSEP;ITEMVALUE=id&uSEP;HEADER=T&uSEP;SORTHEADER=F&uSEP;AUTOEXPAND=F&uSEP;LISTMULTISELECT=T&uSEP;TYPE1=LEAF&uSEP;NODE1=F&uSEP;NORMAL1=^U_PIXEL&uSEP;SELECTED1=^U_PIXEL"

putitem/id $properties(UPARAMS.DUMMY), "ATTACH", "Hsize"
putitem/id $properties(UPARAMSNO.DUMMY), "ATTACH", "Hmove"

activate "SIGWIZ_MAIN2".exec()
SIGWIZ_MAIN2 = "SIGWIZ_MAIN2"

show

; Read Signatures
activate "SIGWIZ_SVC".GET_USPECNAM("", UPATH.DUMMY, sFieldValRep, nTotOccs)

; Initalize fields
$valrep(USPECNAM.DUMMY) = sFieldValRep
USPECNAM.DUMMY/init = ""
UIMPLATTR.DUMMY/init = ""
USPECNAMNO.DUMMY/init = nTotOccs
$valrep(UOPERNAM.DUMMY) = ""
ULITOPERNAM.DUMMY/init = ""
UOPERNAMNO.DUMMY/init = ""
$valrep(UPARAMS.DUMMY) = ""
UIPATTR.DUMMY/init = ""
UPARAMSNO.DUMMY/init = ""
$prompt = USPECNAM.DUMMY

edit/menu
</DAT>
