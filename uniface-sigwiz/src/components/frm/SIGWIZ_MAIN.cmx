operation CHECK_UPATH
	
	params
		string pUPath : IN
	endparams
	
	variables
		string sUPath
	endvariables
	
	forlist sUPath in pUpath
		if (sUPath = UPATH.DUMMY) return(1)
	endfor
	return(0)
	
end;- getUPath

; ********************************************************************************

operation ACTION
	
	params
		string pAction : IN
	endparams
	
	variables
		boolean bPrefix
		numeric TotOccs
		string sFieldValRep, sAskmessText, sTemp, sUspecNam, sFindResult
		numeric nStart, nEnd
		string sULITPARMNAM, sU_DTYP, sUIPATTR, sUHINTERFACE, sUSPECNAM_OLD, sUSPECNAM_NEW
	endvariables
	
	$RefreshNext$ = @$fieldname
	selectcase pAction
		case "ADD_PREFIX", "ADD_PREFIX_ALL", "RENAME_SIGS", "RENAME_SIGS_ALL"
			selectcase pAction
				case "ADD_PREFIX"
					sAskmessText= "Add Prefix to %%$itemcount(uspecnam.dummy)%%% signatures?%%^Proceed with caution!"
					sUspecNam = uspecnam.dummy
					bPrefix = "T"
				case "ADD_PREFIX_ALL"
					sAskmessText = "Add Prefix to all signatures?"
					sUspecNam = $valrep(USPECNAM.DUMMY)
					bPrefix = "T"
				case "RENAME_SIGS"
					sAskmessText= "Rename %%$itemcount(uspecnam.dummy)%%% signatures?%%^Proceed with caution!"
					sUspecNam = uspecnam.dummy
					bPrefix = "F"
				case "RENAME_SIGS_ALL"
					sAskmessText = "Rename all signatures?"
					sUspecNam = $valrep(USPECNAM.DUMMY)
					bPrefix = "F"
			endselectcase
			askmess/question sAskmessText
			if ($status = 1)
				if (USPECNAMSR.DUMMY != "") sUSPECNAM_OLD = USPECNAMSR.DUMMY
				activate "SIGWIZ_RC".SHOW(pAction, "", "", "", "", sUSPECNAM_OLD, sUSPECNAM_NEW)
				if ($status = 1) return(0)
				if ($instancehandle("SIGWIZ_SVC")-&gt;RENAME_SIGS(sUspecNam, sUSPECNAM_OLD, sUSPECNAM_NEW, bPrefix) &lt; 0) message/error "[ACTION] %%pAction failed!"
				if (USPECNAMSR.DUMMY != "" &amp; sUSPECNAM_NEW != "")
					sUspecNam = $concat(USPECNAMSR.DUMMY, "&uALL;&uOR;", sUSPECNAM_NEW, "&uALL;")
				else
					sUspecNam = ""
				endif
				activate "SIGWIZ_SVC".GET_USPECNAM(sUspecNam, UPATH.DUMMY, sFieldValRep, TotOccs)
				USPECNAM.DUMMY/init = ""
				$valrep(USPECNAM.DUMMY) = sFieldValRep
				USPECNAMNO.DUMMY/init = TotOccs
				if ($fieldname = "USPECNAM")
					callfieldtrigger "ValueChanged", "USPECNAM.DUMMY"
				else
					$prompt = USPECNAM.DUMMY
				endif
			endif
		case "CHECK_PARAMS"
			sUspecNam = $valrep(USPECNAM.DUMMY)
			askmess "Check Paramaters", "All,Only IN,Only OUT,Only INOUT"
			activate "SIGWIZ_SVC".CHECK_PARAMS( "F", sUspecNam, $status, "", sFindResult )
			activate "SIGWIZ_MAIN".showTabs(sFindResult)
		case "CHANGE_UHINTERFACE", "CHANGE_PARAM", "EDIT"
			$Refresh$ = ""
			selectcase pAction
				case "CHANGE_UHINTERFACE"
					getitem/id sUHINTERFACE, $valrep(uparams.dummy), uparams.dummy
					nStart = $scan(sUHINTERFACE, "HANDLE to %%"%%%") + 11
					if (nStart  &gt; 11)
						nEnd = $length(sUHINTERFACE) - 1
						sUHINTERFACE = sUHINTERFACE[nStart,nEnd]
						;if ($length(sUHINTERFACE) &gt; 32) sUHINTERFACE = sUHINTERFACE[1:32]
						activate "SIGWIZ_CH".SHOW("CHANGE_UHINTERFACE", "COM", uspecnam.dummy, uopernam.dummy, uparams.dummy, sUHINTERFACE)
						if (sUHINTERFACE != "")
							activate "SIGWIZ_SVC".SET_UHINTERFACE(uspecnam.dummy, uopernam.dummy, uparams.dummy, sUHINTERFACE)
							$Refresh$ = "True"
						endif
					endif
				case "CHANGE_PARAM"
					activate "sigwiz_svc".get_param(uspecnam.dummy, uopernam.dummy, uparams.dummy, sULITPARMNAM, sU_DTYP, sUIPATTR)
					getitem/id sUIPATTR, sUIPATTR, "UIPBASETYP"
					if (sU_DTYP = "N")
						sU_DTYP = "S"
					elseif  (sU_DTYP = "S")
						sU_DTYP = "N"
					else
						message/error "Unexpected error occured in menu %%"%%%String &lt;-&gt; Numeric (VT_VARIANT)%%"%%%"
					endif
					activate "sigwiz_svc".set_param(uspecnam.dummy, uopernam.dummy, uparams.dummy, sULITPARMNAM, sU_DTYP, sUIPATTR)
					$Refresh$ = "True"
				case "EDIT"
					selectcase $fieldname
						case "UOPERNAM"
							activate "SIGWIZ_EP".EDIT(uspecnam.dummy,uopernam.dummy,"")
							if ($status = 0)
								callfieldtrigger "ValueChanged", "UOPERNAM.DUMMY"
							endif
						case "UPARAMS"
							activate "SIGWIZ_EP".EDIT(uspecnam.dummy,uopernam.dummy,uparams.dummy)
							if ($status = 0)
								$Refresh$ = "True"
							endif
						elsecase
							message/error "Not yet implemented!"
							return(-1)
					endselectcase
			endselectcase
			if ($Refresh$) $prompt = UOPERNAM.DUMMY                
		case "CLEAN_ULANA"
			askmess/question "Clean ULANA/USYSANA/$RSO of orphaned Signatures?"
			if ($status = 1)
				activate "SIGWIZ_SVC".CLEAN_ULANA(UPATH.DUMMY)
			endif
		case "COMPILE", "COMPILE_ALL"
			if (pAction = "COMPILE")
				sAskmessText= "Compile %%$itemcount(uspecnam.dummy)%%% signatures?"
				sUspecNam = uspecnam.dummy
			else
				sAskmessText= "Compile all signatures?"
				sUspecNam = $valrep(USPECNAM.DUMMY)
			endif
			askmess/question sAskmessText
			if ($status = 1)
				activate "SIGWIZ_SVC".COMPILE_SIG(sUspecNam)
			endif
		case "EXPORT", "EXPORT_ALL"
			if (pAction = "EXPORT")
				sAskmessText= "Export %%$itemcount(uspecnam.dummy)%%% signatures?"
				sUspecNam = uspecnam.dummy
			else
				sAskmessText= "Export all signatures?"
				sUspecNam = $valrep(USPECNAM.DUMMY)
			endif
			askmess/question sAskmessText
			if ($status = 1)
				filebox/save "*.sgx"
				if ($status &lt;= 0) return(0)
				activate "SIGWIZ_SVC".EXPORT_SIG(sUspecNam, $result)
			endif
		case "IMPORT"
			askmess/question "Import signatures?"
			if ($status = 1)
				filebox "*.sgx"
				if ($status &lt;= 0) return(0)
				activate "SIGWIZ_SVC".IMPORT_SIG($result)
				if (USPECNAMSR.DUMMY != "") sUspecNam = $concat(USPECNAMSR.DUMMY, "&uALL;")
				activate "SIGWIZ_SVC".GET_USPECNAM(sUspecNam, UPATH.DUMMY, sFieldValRep, TotOccs)
				USPECNAM.DUMMY/init = ""
				$valrep(USPECNAM.DUMMY) = sFieldValRep
				USPECNAMNO.DUMMY/init = TotOccs
				if ($fieldname = "USPECNAM")
					callfieldtrigger "ValueChanged", "USPECNAM.DUMMY"
				else
					$prompt = uspecnam.dummy
				endif
			endif
		case "FIND_UHINTERFACE", "FIND_UIPATTR"
			if (pAction[$length("FIND_")+1] = "UHINTERFACE")
				activate "SIGWIZ_SVC".FIND_UHINTERFACE(@$fieldname, upath.dummy, sFindResult)
			elseif (pAction[$length("FIND_")+1] = "UIPATTR")
				activate "SIGWIZ_AF".SHOW(sUIPATTR)
				if (sUIPATTR != "")
					sUspecNam = $valrep(USPECNAM.DUMMY)
					activate "SIGWIZ_SVC".FIND_UIPATTR(sUIPATTR, sUspecNam, sFindResult)
				else
					return(-1)
				endif
			else
				message/error "Not yet implemented!"
				return(-1)
			endif
			activate "SIGWIZ_MAIN".showTabs(sFindResult)
		case "COPY"
			activate "SIGWIZ_RC".SHOW("COPY", $fieldname, uspecnam.dummy, uopernam.dummy, uparams.dummy, $RefreshNext$, "")
			if (@$fieldname != $RefreshNext$ &amp; $RefreshNext$ != "")
				selectcase $fieldname
					case "USPECNAM"
						activate "SIGWIZ_SVC".COPY_SIG(@$fieldname, $RefreshNext$)
						if (USPECNAMSR.DUMMY != "")
							sUspecNam = $concat(USPECNAMSR.DUMMY, "&uALL;&uOR;", $RefreshNext$, "&uALL;")
						endif
						activate "SIGWIZ_SVC".GET_USPECNAM(sUspecNam, UPATH.DUMMY, sFieldValRep, TotOccs)
						$valrep(USPECNAM.DUMMY) = sFieldValRep
						USPECNAM.DUMMY = $RefreshNext$
						USPECNAMNO.DUMMY/init = TotOccs
						$valrep(UOPERNAM.DUMMY) = ""
						UOPERNAM.DUMMY/init = ""
						UOPERNAMNO.DUMMY/init = ""
						$valrep(UPARAMS.DUMMY) = ""
						UPARAMS.DUMMY/init = ""
						UPARAMSNO.DUMMY/init = ""
						callfieldtrigger "ValueChanged", $fieldname
					case "UOPERNAM"
						activate "SIGWIZ_SVC".COPY_OPER(uspecnam.dummy, @$fieldname, $RefreshNext$)
						;$Refresh$ = "Multi"
						callfieldtrigger "ValueChanged", "USPECNAM.DUMMY"
						callfieldtrigger "ValueChanged", $fieldname
					case "UPARAMS"
						activate "SIGWIZ_SVC".COPY_PARAM(uspecnam.dummy, uopernam.dummy, @$fieldname, $RefreshNext$)
						;$Refresh$ = "True"
						callfieldtrigger "ValueChanged", "UOPERNAM.DUMMY"
					elsecase
						message/error "Not yet implemented!"
						return(-1)
				endselectcase
			endif
		case "COPY_TO", "MOVE_TO"
			selectcase $fieldname
				case "UOPERNAM"
					activate "SIGWIZ_CH".SHOW(pAction, upath.dummy, uspecnam.dummy, uopernam.dummy, "", sUHINTERFACE)
					if (sUHINTERFACE != "")
						activate "SIGWIZ_SVC"."%%pAction[1,4]%%%_OPER_TO"(uspecnam.dummy, sUHINTERFACE, uopernam.dummy)
						;$Refresh$ = "Multi"
						uspecnam.dummy/init = sUHINTERFACE
						callfieldtrigger "ValueChanged", "USPECNAM.DUMMY"
					endif
				case "UPARAMS"
				elsecase
					message/error "Not yet implemented!"
					return(-1)
			endselectcase
		case "RENAME"
			if (pAction = "RENAME")
				activate "SIGWIZ_RC".SHOW("RENAME", $fieldname, uspecnam.dummy, uopernam.dummy, uparams.dummy, $RefreshNext$, "")
				if (@$fieldname != $RefreshNext$ &amp; $RefreshNext$ != "")
					selectcase $fieldname
						case "USPECNAM"
							activate "SIGWIZ_SVC".RENAME_SIG(@$fieldname, $RefreshNext$)
							if (USPECNAMSR.DUMMY != "")
								sUspecNam = $concat(USPECNAMSR.DUMMY, "&uALL;&uOR;", $RefreshNext$, "&uALL;")
							endif
							activate "SIGWIZ_SVC".GET_USPECNAM(sUspecNam, UPATH.DUMMY, sFieldValRep, TotOccs)
							$valrep(USPECNAM.DUMMY) = sFieldValRep
							USPECNAM.DUMMY/init = $RefreshNext$
							USPECNAMNO.DUMMY/init = TotOccs
							callfieldtrigger "ValueChanged", $fieldname
						case "UOPERNAM"
							activate "SIGWIZ_SVC".RENAME_OPER(uspecnam.dummy, @$fieldname, $RefreshNext$)
							;$Refresh$ = "Multi"
							callfieldtrigger "ValueChanged", "USPECNAM.DUMMY"
							callfieldtrigger "ValueChanged", $fieldname
						case "UPARAMS"
							activate "SIGWIZ_SVC".RENAME_PARAM(uspecnam.dummy, uopernam.dummy, @$fieldname, $RefreshNext$)
							;$Refresh$ = "True"
							callfieldtrigger "ValueChanged", "UOPERNAM.DUMMY"
						elsecase
							message/error "Not yet implemented!"
							return(-1)
					endselectcase
				endif
			else
				sUSPECNAM_OLD = USPECNAMSR.DUMMY
				activate "SIGWIZ_RC".SHOW("RENAME_MULTI", $fieldname, uspecnam.dummy, uopernam.dummy, uparams.dummy, sUSPECNAM_OLD, sUSPECNAM_NEW)
				if (sUSPECNAM_OLD != "")
					activate "SIGWIZ_SVC".RENAME_SIG($concat(sUSPECNAM_OLD, "&uALL;"), sUSPECNAM_NEW)
					if (USPECNAMSR.DUMMY != "" &amp; sUSPECNAM_NEW != "")
						sUspecNam = $concat(USPECNAMSR.DUMMY, "&uALL;&uOR;", sUSPECNAM_NEW, "&uALL;")
					endif
					activate "SIGWIZ_SVC".GET_USPECNAM(sUspecNam, UPATH.DUMMY, sFieldValRep, TotOccs)
					USPECNAM.DUMMY/init = ""
					$valrep(USPECNAM.DUMMY) = sFieldValRep
					USPECNAMNO.DUMMY/init = TotOccs
					if ($fieldname = "USPECNAM")
						callfieldtrigger "ValueChanged", "USPECNAM.DUMMY"
					else
						$prompt = uspecnam.dummy
					endif
				endif
				message/error "Not yet implemented!"
				return(-1)
			endif
		case "DELETE"
			selectcase $fieldname
				case "USPECNAM"
					if ($itemnr(2, uspecnam.dummy) = "")
						sAskmessText = "Do you really want to delete the signature [%%uspecnam.dummy%%%] and all its operations and parameters?"
					else
						sAskmessText = "Do you really want to delete %%$itemcount(uspecnam.dummy)%%% signatures and all its operations and parameters?"
					endif
					askmess/question sAskmessText
					if ($status = 1)
						activate "SIGWIZ_SVC".delete_sig(uspecnam.dummy)
						if (USPECNAMSR.DUMMY != "") sUspecNam = $concat(USPECNAMSR.DUMMY, "&uALL;")
						activate "SIGWIZ_SVC".GET_USPECNAM(sUspecNam, UPATH.DUMMY, sFieldValRep, TotOccs)
						USPECNAM.DUMMY/init = ""
						$valrep(USPECNAM.DUMMY) = sFieldValRep
						USPECNAMNO.DUMMY/init = TotOccs
						callfieldtrigger "ValueChanged", $fieldname
					endif
				case "UOPERNAM"
					if ($itemnr(2, uopernam.dummy) = "")
						sAskmessText = "Do you really want to delete the operation [%%uopernam.dummy%%%] and all its parameters?"
					else
						getitem sTemp, uopernam.dummy, -1
						sAskmessText = "Do you really want to delete %%$status%%% operations and all its parameters?"
					endif
					askmess/question sAskmessText
					if ($status = 1)
						activate "SIGWIZ_SVC".delete_oper(uspecnam.dummy, uopernam.dummy)
						;$Refresh$ = "True"
						;callfieldtrigger "ValueChanged", "USPECNAM.DUMMY"
						;callfieldtrigger "ValueChanged", $fieldname
						$prompt = uspecnam.dummy
					endif
				case "UPARAMS"
					if ($itemnr(2, uparams.dummy) = "")
						sAskmessText = "Do you really want to delete the parameter [%%uparams.dummy%%%]?"
					else
						getitem sTemp, uparams.dummy, -1
						sAskmessText = "Do you really want to delete %%$status%%% parameters?"
					endif
					askmess/question sAskmessText
					if ($status = 1)
						activate "SIGWIZ_SVC".delete_param(uspecnam.dummy, uopernam.dummy, uparams.dummy)
						;callfieldtrigger "ValueChanged", "UOPERNAM.DUMMY"
						$prompt = uopernam.dummy
					endif
				elsecase
					message/error "Not yet implemented!"
					return(-1)
			endselectcase
	endselectcase            
	
end;- ACTION

