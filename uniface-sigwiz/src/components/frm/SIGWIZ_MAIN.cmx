<DAT name="DETAIL" xml:space='preserve'>variables
	numeric nStart, nEnd, TotOccs
	string sTarget, sUHINTERFACE, FieldValRep
endvariables

getitem/id sUHINTERFACE, $valrep(uparams.dummy), uparams.dummy
nStart = $scan(sUHINTERFACE, "HANDLE to %%"%%%") + 11
if (nStart &gt; 11)
	nEnd = $length(sUHINTERFACE) - 1
	sUHINTERFACE = sUHINTERFACE[nStart,nEnd]
	getitem/id sTarget, $valrep(uspecnam.dummy), sUHINTERFACE
	if ($status &gt; 0)
		uspecnam.dummy = sUHINTERFACE
;		$Refresh$ = "True"
;		$prompt = uspecnam.dummy
		uopernam.dummy = ""
		call LP_BUILDUOPERNAM
	else
		if (sUHINTERFACE != "") activate "SIGWIZ_SVC".GET_USPECNAM(sUHINTERFACE, UPATH.DUMMY, FieldValRep, TotOccs)
		if (TotOccs = 0)
			message/error "Incorrect Handle reference (classification) specified for%%^the parameter '%%uparams.dummy%%%'!%%^The referenced signature '%%sUHINTERFACE%%%' cannot be found.%%^Please update the Signatur to correct this issue."
			askmess/question "Do you want to update the signature now?"
;			if ($status = 1) activate "SIGWIZ_CH".SHOW("CHANGE_UHINTERFACE", "COM", uspecnam.dummy, uopernam.dummy, uparams.dummy, sUHINTERFACE)
			if ($status = 1) activate $instancename.ACTION("CHANGE_UHINTERFACE")
			;$Refresh$ = "True"
			$prompt = uopernam.dummy
		else
			askmess/question "Could not find Signature in result list. Do you want to add it?"
			if ($status = 1)
				putitem $valrep(uspecnam.dummy), -1, FieldValRep
				FieldValRep = $valrep(uspecnam.dummy)
				sort/list FieldValRep, "A"
				$valrep(uspecnam.dummy) = FieldValRep
				uspecnam.dummy = sUHINTERFACE
				uopernam.dummy = ""
				call LP_BUILDUOPERNAM
			endif
		endif
	endif
endif
</DAT>
