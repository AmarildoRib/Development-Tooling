<DAT name="SFUNC" xml:space='preserve'>; ********************************************************************************

operation showResult
	
	params
		string sResult : IN
	endparams
	
	variables
		numeric nPos, TotOccs
		string sUSpec, sUOper, sUParm, FieldValRep
		string uipattr_list
	endvariables
	
	if ($split(sResult, 1, ".", sUSpec, sUOper) = 0) sUSpec = sResult
	if ($item(sUSpec, $valrep(uspecnam.dummy)) != "")
		USPECNAM.DUMMY/init = sUSpec
	else
		activate "SIGWIZ_SVC".GET_USPECNAM(sUSpec, UPATH.DUMMY, FieldValRep, TotOccs)
		if (TotOccs = 0)
			message/error "Cannot find Signature '%%sUSpec%%%'."
			sUSpec = ""
		else
			askmess/question "Could not find Signature in result list. Do you want to add it?"
			if ($status = 1)
				putitem $valrep(uspecnam.dummy), -1, FieldValRep
				FieldValRep = $valrep(uspecnam.dummy)
				sort/list FieldValRep, "A"
				$valrep(uspecnam.dummy) = FieldValRep
				USPECNAM.DUMMY/init = sUSpec
			else
				sUSpec = ""
			endif
		endif
	endif
	if (sUSpec != "")
		call LP_BUILDUOPERNAM
		if ($scan(sUOper, ".") &gt; 0)
			sResult = sUOper
			nPos = $split(sResult, 1, ".", sUOper, sUParm)
		endif
		UOPERNAM.DUMMY/init = sUOper
		call LP_BUILDUPARAMS
		if (sUParm != "")
			UPARAMS.DUMMY = sUParm
			activate "SIGWIZ_SVC".GET_PARAM(uspecnam.dummy, uopernam.dummy, uparams.dummy, "", "", uipattr_list)
			getitem/id uipattr.dummy, uipattr_list, "UIPBASETYP"
		endif
	endif
	
end;- showResult

; ********************************************************************************

operation showTabs
	
	params
		string sFindResult : IN
	endparams
	
	if ($instances("SIGWIZ_SR", "") = "")
		newinstance "SIGWIZ_SR", "SIGWIZ_MAIN_SR"
		activate "SIGWIZ_TAB".exec()
		activate "SIGWIZ_TAB".createTabs("SIGWIZ_MAIN2=Syntax&uSEP;SIGWIZ_MAIN_SR=Results")
		SIGWIZ_MAIN2.DUMMY = "SIGWIZ_TAB"
	endif
	
	activate "SIGWIZ_MAIN_SR".show(sFindResult)
	activate "SIGWIZ_TAB".setTab("SIGWIZ_MAIN_SR")
	
end;- showTabs

; ********************************************************************************

operation removeTabs
	
	SIGWIZ_MAIN2.DUMMY = "SIGWIZ_MAIN2"
	deleteinstance "SIGWIZ_MAIN_SR"
	deleteinstance "SIGWIZ_TAB"
	
end;- removeTabs

