<DAT name="ASYNC" xml:space='preserve'>selectcase $msgid
	case "QUIT"
		macro "^QUIT"
	case "ACCEPT"
		macro "^ACCEPT"
	case "REMOVETABS"
		activate $instancename.	removeTabs()
endselectcase
