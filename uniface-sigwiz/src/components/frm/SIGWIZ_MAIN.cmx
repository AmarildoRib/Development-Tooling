<DAT name="MENU" xml:space='preserve'>variables
	handle hUser32, hYrtl
	numeric hWnd, styles
	string lpClassName
endvariables

newinstance "YRTL", hYrtl
newinstance "USER32", hUser32
hWnd = hYrtl-&gt;ugethwnd()
hWnd = hUser32-&gt;FindWindowEx(hWnd, 0, "MDIClient", 0)
hWnd = hUser32-&gt;FindWindowEx(hWnd, 0, "UniMDIForm", 0)
hWnd = hUser32-&gt;FindWindowEx(hWnd, 0, "UniCanvas", 0)
hWnd = hUser32-&gt;FindWindowEx(hWnd, 0, "OWITreeList", 0)
hWnd = hUser32-&gt;FindWindowEx(hWnd, 0, "SysListView32", 0)
if (hWnd &gt; 0)
	styles = hUser32-&gt;SendMessage(hWnd , &lt;LVM_GETEXTENDEDLISTVIEWSTYLE&gt;, 0, 0)
	styles += &lt;LVS_EX_INFOTIP&gt; + &lt;LVS_EX_FULLROWSELECT&gt;
	hUser32-&gt;SendMessage(hWnd , &lt;LVM_SETEXTENDEDLISTVIEWSTYLE&gt;, 0, styles)
else
	message/hint "FindWindowEx could not find Tree Widget window"
endif
</DAT>
