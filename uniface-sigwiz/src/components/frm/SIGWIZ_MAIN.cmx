	activate "SIGWIZ_SVC".GET_USPECNAM(sSearchPrf, UPATH.DUMMY, FieldValRep, TotOccs)
	if ($status = 0)
		$valrep(USPECNAM.DUMMY) = FieldValRep
		;putitem $valrep(USPECNAM.DUMMY), 1, "&uALL;=&lt;&lt;&lt; ALL &gt;&gt;&gt;"
		if ($item(USPECNAM.DUMMY, $valrep(USPECNAM.DUMMY)) = "") USPECNAM.DUMMY/init = ""
		UIMPLATTR.DUMMY/init = ""
		USPECNAMNO.DUMMY/init = TotOccs
		$valrep(UOPERNAM.DUMMY) = ""
		UOPERNAM.DUMMY = ""
		UOPERNAMNO.DUMMY/init = ""
		ULITOPERNAM.DUMMY/init = ""
		$valrep(UPARAMS.DUMMY) = ""
		UPARAMS.DUMMY/init = "" 
		UPARAMSNO.DUMMY/init = ""
		UIPATTR.DUMMY/init = ""
	else
		$valrep(USPECNAM.DUMMY) = ""
		USPECNAM.DUMMY/init = ""
		UIMPLATTR.DUMMY/init = ""
		USPECNAMNO.DUMMY/init = ""
		UOPERNAMSR.DUMMY = ""
		$valrep(UOPERNAM.DUMMY) = ""
		UOPERNAM.DUMMY/init = ""
		ULITOPERNAM.DUMMY/init = ""
		UOPERNAMNO.DUMMY/init = ""
		$valrep(UPARAMS.DUMMY) = ""
		UPARAMS.DUMMY/init = ""
		UIPATTR.DUMMY/init = ""
		UPARAMSNO.DUMMY/init = ""
	endif
	
end;- LP_SEARCHUSPECNAM

; ********************************************************************************

entry LP_SEARCHUOPERNAM
	
	variables
		numeric    TotOccs
		string     sSearchPrf, FieldValRep
	endvariables
	
	if ($valrep(USPECNAM.DUMMY) != "" &amp; $itemcount(USPECNAM.DUMMY) = 1)
		sSearchPrf = $concat(UOPERNAMSR.DUMMY, "&uALL;")
		activate "SIGWIZ_SVC".GET_UOPERNAM(USPECNAM.DUMMY, sSearchPrf, FieldValRep, TotOccs)
		if ($status = 0)
			$valrep(UOPERNAM.DUMMY) = FieldValRep
			UOPERNAM.DUMMY/init = ""
			ULITOPERNAM.DUMMY/init = ""
			UOPERNAMNO.DUMMY/init = TotOccs
			$valrep(UPARAMS.DUMMY) = ""
			UPARAMS.DUMMY/init = ""
			UIPATTR.DUMMY/init = ""
			;OPERNAM.DUMMY/init = ""
			;PARAMS.DUMMY/init = ""
			;RESULT.DUMMY/init = ""
			activate "SIGWIZ_MAIN2".clearFields("OPERNAM.DUMMY&uSEP;PARAMS.DUMMY&uSEP;RESULT.DUMMY")
		else
			$valrep(UOPERNAM.DUMMY) = ""
			UOPERNAM.DUMMY/init = ""
			ULITOPERNAM.DUMMY/init = ""
			UOPERNAMNO.DUMMY/init = ""
			$valrep(UPARAMS.DUMMY) = ""
			UPARAMS.DUMMY/init = ""
			UIPATTR.DUMMY/init = ""
			UPARAMSNO.DUMMY/init = ""
		endif
	endif
	
end;- LP_SEARCHUOPERNAM

; ********************************************************************************

entry LP_BUILDUOPERNAM
	
	variables
		numeric    TotOccs
		string        FieldValRep, UIMPLATTR_OUT, ClsID, sSearchPrf
	endvariables
	
	activate "SIGWIZ_SVC".GET_USIMPL(USPECNAM.DUMMY, UIMPLATTR_OUT)
	getitem/id UIMPLATTR.DUMMY, UIMPLATTR_OUT, "PROGID"
	if ($status = 0 | UIMPLATTR.DUMMY = "") getitem/id UIMPLATTR.DUMMY, UIMPLATTR_OUT, "LIT"
	getitem/id ClsID, UIMPLATTR_OUT, "CLSID"
	if (ClsID)
		message/hint "ClsID : %%ClsID%%%"
	else
		message/hint ""
	endif
	sSearchPrf = $concat(UOPERNAMSR.DUMMY, "&uALL;")
	activate "SIGWIZ_SVC".GET_UOPERNAM(USPECNAM.DUMMY, sSearchPrf, FieldValRep, TotOccs)
	$valrep(UOPERNAM.DUMMY) = FieldValRep
	UOPERNAM.DUMMY/init = ""
	ULITOPERNAM.DUMMY/init = ""
	UOPERNAMNO.DUMMY/init = TotOccs
	$valrep(UPARAMS.DUMMY) = ""
	UPARAMS.DUMMY/init = ""
	UIPATTR.DUMMY/init = ""
	UPARAMS.DUMMY/init = ""
	UPARAMSNO.DUMMY/init = ""
	;OPERNAM.DUMMY/init = ""
	;PARAMS.DUMMY/init = ""
	;RESULT.DUMMY/init = ""
	activate "SIGWIZ_MAIN2".clearFields("OPERNAM.DUMMY&uSEP;PARAMS.DUMMY&uSEP;RESULT.DUMMY")
	
end ; entry LP_BUILDUOPERNAM

; ********************************************************************************

entry LP_BUILDUPARAMS
	
	variables
		numeric    TotOccs, nCounter
		string    sParamsDef, sParamsDefValues, sParams, sParamsTree, sParamValue, sResult, sHandleNam
	endvariables
	
	activate "SIGWIZ_SVC".GET_UOPER(USPECNAM.DUMMY, UOPERNAM.DUMMY, ULITOPERNAM.DUMMY)
	activate "SIGWIZ_SVC".GET_PARAMS(USPECNAM.DUMMY, UOPERNAM.DUMMY, sParamsDef, sParamsTree, sParams, TotOccs)
	;OPERNAM.DUMMY/init = UOPERNAM.DUMMY
	activate "SIGWIZ_MAIN2".setField("OPERNAM.DUMMY", UOPERNAM.DUMMY)
	$valrep(UPARAMS.DUMMY) = sParamsTree
	UPARAMS.DUMMY/init = ""
	UIPATTR.DUMMY/init = ""
	if (TotOccs &gt; 0)
		UPARAMSNO.DUMMY/init = TotOccs
	else
		UPARAMSNO.DUMMY/init = ""
	endif
	;PARAMS.DUMMY/init = sParams
	activate "SIGWIZ_MAIN2".setField("PARAMS.DUMMY", sParams)
	nCounter = 1
	while (1 = 1)
		getitem sParamValue, sParamsDef, nCounter
		if (sParamsDefValues = "" &amp; $status &gt; 0)
			sParamsDefValues = $valuepart(sParamValue)
		elseif ($status &gt; 0)
			sParamsDefValues = "%%sParamsDefValues%%%, %%$valuepart(sParamValue)%%%"
		else
			break
		endif
		nCounter = nCounter + 1
	endwhile
	sResult =                  "; *** %%"%%%%%USPECNAM.DUMMY%%%%%"%%% -&gt; %%UOPERNAM.DUMMY%%% ( %%sParamsDefValues%%% )%%^"
	activate "SIGWIZ_MAIN2".getField("HANDLENAM.DUMMY", sHandleNam)
	sResult = $concat(sRESULT, "%%sHandleNam%%% -&gt; %%UOPERNAM%%% ( %%sParams%%% )")
	activate "SIGWIZ_MAIN2".setField("RESULT.DUMMY", sResult)
	show
	call LP_SETHEADERSIZE
	
end;- LP_BUILDUPARAMS

; ********************************************************************************

entry LP_SETHEADERSIZE
	
	variables
		handle hUser32, hYrtl
		numeric hWnd, hHeader, nItemCount, nCount
		string lpClassName, sSelected
	endvariables
	
	newinstance "YRTL", hYrtl
	newinstance "USER32", hUser32
	hWnd = hYrtl-&gt;ugethwnd()
	hWnd = hUser32-&gt;FindWindowEx(hWnd, 0, "MDIClient", 0)
	hWnd = hUser32-&gt;FindWindowEx(hWnd, 0, "UniMDIForm", 0)
	hWnd = hUser32-&gt;FindWindowEx(hWnd, 0, "UniCanvas", 0)
	hWnd = hUser32-&gt;FindWindowEx(hWnd, 0, "OWITreeList", 0)
	hWnd = hUser32-&gt;FindWindowEx(hWnd, 0, "SysListView32", 0)
	if (hWnd &gt; 0)
		hHeader = hUser32-&gt;SendMessage(hWnd, &lt;LVM_GETHEADER&gt;, 0, 0)
		nItemCount = hUser32-&gt;SendMessage(hHeader, &lt;HDM_GETITEMCOUNT&gt;, 0, 0)
		nItemCount -= 1
		for nCount = 0 to nItemCount
			hUser32-&gt;SendMessage(hWnd, &lt;LVM_SETCOLUMNWIDTH&gt;, nCount, &lt;LVSCW_AUTOSIZE_USEHEADER&gt;)
		endfor
	else
		message/hint "[LP_SETHEADERSIZE] FindWindowEx could not find Tree Widget window"
	endif
	
end;- LP_SETHEADERSIZE

; ********************************************************************************
