entry LP_CHECKINTEGRITY

	returns numeric

	params
		string pENTITY1 : IN
		string pENTITY2 : IN
		string pUSPECNAM : IN
		string pUOPERNAM : IN
	endparams

	variables
		string sUpecNam, sUoperNam
		Numeric TotOccs1, TotOccs2
	endvariables

	sUpecNam = $concat("USPECNAM.", pENTITY1)
	sUoperNam = $concat("UOPERNAM.", pENTITY1)
	clear/e pENTITY1
	@sUpecNam/init = pUSPECNAM
	@sUoperNam/init = pUOPERNAM
	TotOccs1 = LP_TOTOCCS(pENTITY1)

	sUpecNam = $concat("USPECNAM.", pENTITY2)
	sUoperNam = $concat("UOPERNAM.", pENTITY2)
	clear/e pENTITY2
	@sUpecNam/init = pUSPECNAM
	@sUoperNam/init = pUOPERNAM
	TotOccs2 = LP_TOTOCCS(pENTITY2)

	if (TotOccs1 != TotOccs2)
		message/error "%%pENTITY1%%% &lt;&gt; %%pENTITY2%%% : [%%TotOccs1%%% &lt;&gt; %%TotOccs2%%%]"
		return(-1)
	endif

	return(0)

end;- LP_CHECKINTEGRITY

