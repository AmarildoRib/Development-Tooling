operation COMPILE_SIG
	
	params
		string sUSpecNamList : IN
	endparams
	
	variables
		string sRetrieveList, sRetrieveListItem
		numeric nCounter, nHits, nResult
	endvariables
	
	nHits = $itemcount(sUSpecNamList)
	activate "SIGWIZ_PROGRESS".INIT_PROGRESS(nHits, "Compile Signature")
	
	forlist sRetrieveList, nCounter in sUSpecNamList
		nResult = $ude("compile", "signature", sRetrieveList)
		;****************************************
		activate "SIGWIZ_PROGRESS".SHOW_PROGRESS(nCounter, "Processed %%nCounter%%% of %%nHits%%%")
