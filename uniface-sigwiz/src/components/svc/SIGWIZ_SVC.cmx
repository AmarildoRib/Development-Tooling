operation GET_USPECNAM
	
	params
		string sSearchPrf : IN
		string sUDefPath : IN
		string sFieldValRep : OUT
		numeric nTotOccs : OUT
	endparams
	
	variables
		numeric    nCurOcc
		string    sLastVersion
	endvariables
	
	
	if ($length(sSearchPrf) &lt;= 32)
		
		clear/e "USSPEC"
		if (sSearchPrf != "") USPECNAM.USSPEC/init = sSearchPrf
		UDEFPATH.USSPEC/init = sUDefPath
		nTotOccs = LP_TOTOCCS("USSPEC")
		if (nTotOccs &lt;= 0) return 1
		activate "SIGWIZ_PROGRESS".INIT_PROGRESS(nTotOccs, "Load Signatures")
		retrieve/e "USSPEC"
		nCurOcc = 1
		setocc "USSPEC", nCurOcc
		sFieldValRep = USPECNAM.USSPEC
		sLastVersion = USPECNAM.USSPEC
		nCurOcc = nCurOcc + 1    
		while (nCurOcc &lt;= nTotOccs)
			setocc "USSPEC", nCurOcc
			if (USPECNAM.USSPEC != sLastVersion)
				putitem sFieldValRep, -1, USPECNAM.USSPEC
				sLastVersion = USPECNAM.USSPEC
			endif
			nCurOcc = nCurOcc + 1
			; ****************************************
			activate "SIGWIZ_PROGRESS".SHOW_PROGRESS(nCurOcc, USPECNAM.USSPEC)
			; ****************************************
		endwhile
		
		deleteinstance "SIGWIZ_PROGRESS"
		
	endif
	
	return(0)
	
end;- GET_USPECNAM

