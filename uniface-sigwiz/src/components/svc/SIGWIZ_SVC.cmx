operation RENAME_SIGS
	
	params
		string  USPECNAM_IN  : IN
		string  USPECNAM_OLD : IN
		string  USPECNAM_NEW : IN
		boolean ADD_PREFIX   : IN
	endparams
	
	variables
		numeric nHits, nCounter, nCheckErrors, nStringLength, nErrors, nSearchStartPos
		string sProgressText, sAction, sRetrieveList, sRetrieveListItem, sUSpecNam, sUSpecNamList, sUSpecNewNam, sUSpecNewNamList, sResult, sCheckResult
	endvariables
	
	if (ADD_PREFIX)
		sAction = "ADD_PREFIX"
		sProgressText = "Add Prefix"
	else
		sAction = "RENAME_SIGS"
		sProgressText = "Rename Signatures"
	endif
	nErrors = 0
	nHits = LP_GET_USPECNAM("usspec", "", "F")
	activate "SIGWIZ_PROGRESS".INIT_PROGRESS(nHits, $concat(sProgressText, " - Step 1/14"))
	activate "SIGWIZ_PROGRESS".RESIZE(0, 1)
	nCounter = 1
	while (nCounter &lt;= nHits)
		setocc "usspec", nCounter
		putitem sUSpecNamList, -1, uspecnam.usspec
		activate "SIGWIZ_PROGRESS".SHOW_PROGRESS(nCounter, "Building List of Signature Names")
		nCounter = nCounter + 1
	endwhile
	; ****************************************
	if (ADD_PREFIX)
		putitem sResult, 1, "1=Add Prefix to Signatures"
	else
		putitem sResult, 1, "1=Rename Signatures"
	endif
	if (USPECNAM_IN = "")  USPECNAM_IN = sUSpecNamList
	sRetrieveList = USPECNAM_IN
	nHits = LP_GET_USPECNAM("usspec", USPECNAM_IN, "F")
	activate "SIGWIZ_PROGRESS".INIT_PROGRESS(nHits, $concat(sProgressText, " - Step 2/14"))
	nCounter = 1
	while (nCounter &lt;= nHits)
		setocc "usspec", nCounter
		getitem sRetrieveListItem, USPECNAM_IN, nCounter
		if (!ADD_PREFIX)
			if ($scan(sRetrieveListItem, USPECNAM_OLD) = 1)
				sRetrieveListItem = sRetrieveListItem[$length(USPECNAM_OLD)+1]
			else
				sRetrieveListItem = ""
			endif
		endif
		nStringLength = $length(sRetrieveListItem)
		if (nStringLength &gt; 0)
			scan sRetrieveListItem, "_"
			if ($result &lt;= 1 | sRetrieveListItem[1:2] = "U_" | sRetrieveListItem[nStringLength-1] = '_#*')
				if (sRetrieveListItem[1:2] = "U_" &amp; USPECNAM_NEW != "")
					sUSpecNewNam = "%%USPECNAM_NEW%%%%%sRetrieveListItem[2]%%%"
				elseif (sRetrieveListItem[1:1] != "_")
					sUSpecNewNam = "%%USPECNAM_NEW%%%%%sRetrieveListItem%%%"
				else
					sUSpecNewNam = "U%%sRetrieveListItem%%%"
				endif
				length sUSpecNewNam
				if ($result &gt;= 32)
					sUSpecNewNam = sUSpecNewNam[1,32]
					if ($scan(sUSpecNewNamList, sUSpecNewNam) &gt; 0 &amp; $scan(sUSpecNewNamList, "%%sUSpecNewNam[1,30]%%%_") = 0)
						sUSpecNewNam = "%%sUSpecNewNam[1,30]%%%_%%1%%%"
					elseif ($scan(sUSpecNewNamList, sUSpecNewNam) &gt; 0 &amp; $scan(sUSpecNewNamList, "%%sUSpecNewNam[1,30]%%%_") = 0)
						nCounter = 1
						nSearchStartPos = 1
						while (nSearchStartPos &gt; 0)
							nSearchStartPos = $scan(sUSpecNewNamList[nSearchStartPos], "%%sUSpecNewNam[1,30]%%%_")
							if (nSearchStartPos &gt; 0) nCounter = nCounter + 1
						endwhile
						sUSpecNewNam = "%%sUSpecNewNam[1,30]%%%_%%nCounter%%%"
					endif
				endif
				;********************************************************
				;putmess "[%%sUSpecNewNam%%%] = [%%sRetrieveListItem%%%]"
				;********************************************************
				if ($item(sUSpecNewNam, sUSpecNamList) = "")
					if (ADD_PREFIX)
						putitem/id sUSpecNewNamList, sRetrieveListItem, sUSpecNewNam
					else
						putitem/id sUSpecNewNamList, $itemnr(nCounter, USPECNAM_IN), sUSpecNewNam
					endif
				else
					nErrors = nErrors + 1
					putitem/id sResult, sUSpecNewNam, "   ERROR: The signature [%%sUSpecNewNam%%%] already exists!"
					delitem/id sRetrieveList, sUSpecNewNam
				endif
			else
				nErrors = nErrors + 1
				putitem/id sResult, sRetrieveListItem, "   ERROR: The signature [%%sRetrieveListItem%%%] already has a prefix!"
				delitem/id sRetrieveList, sRetrieveListItem
			endif
		else
			nErrors = nErrors + 1
			putitem/id sResult, $itemnr(nCounter, USPECNAM_IN), "   ERROR: The signature [%%$itemnr(nCounter, USPECNAM_IN)%%%] does not match the profile [%%USPECNAM_OLD*]!"
			delitem/id sRetrieveList, $idpart($itemnr(nCounter, USPECNAM_IN))
		endif
		activate "SIGWIZ_PROGRESS".SHOW_PROGRESS(nCounter, "Building List of the new Signature Names")
		nCounter = nCounter + 1
	endwhile
	putitem sResult, -1, "2=&gt;&gt; %%sAction%%% returned %%nErrors%%% error(s)"
	; ****************************************
	USPECNAM_IN = sRetrieveList
	if (USPECNAM_IN = "")
		activate "SIGWIZ_MAIN".showTabs(sResult)
		message "%%sAction%%% failed! No signature found to process."
		return(-1)
	endif
	; ****************************************
	nHits = LP_GET_USPECNAM("usspec", USPECNAM_IN, "F")
	activate "SIGWIZ_PROGRESS".INIT_PROGRESS(nHits, $concat(sProgressText, " - Step 3/14"))
	release/e/mod "usspec"
	nCounter = 1
	while (nCounter &lt;= nHits)
		setocc "usspec", nCounter
		getitem/id uspecnam.usspec, sUSpecNewNamList, uspecnam.usspec
		activate "SIGWIZ_PROGRESS".SHOW_PROGRESS(nCounter, "Creating new entries in USSPEC:%%^Processed %%nCounter%%% of %%nHits%%%")
		nCounter = nCounter + 1
	endwhile
	if (LP_STORE("usspec") != 0) return(-1)
	
	nHits = LP_GET_USPECNAM("usimpl", USPECNAM_IN, "F")
	activate "SIGWIZ_PROGRESS".INIT_PROGRESS(nHits, $concat(sProgressText, " - Step 4/14"))
	release/e/mod "usimpl"
	nCounter = 1
	while (nCounter &lt;= nHits)
		setocc "usimpl", nCounter
		getitem/id uspecnam.usimpl, sUSpecNewNamList, uspecnam.usimpl
		getitem/id uimplnam.usimpl, sUSpecNewNamList, uimplnam.usimpl
		activate "SIGWIZ_PROGRESS".SHOW_PROGRESS(nCounter, "Creating new entries in USIMPL:%%^Processed %%nCounter%%% of %%nHits%%%")
		nCounter = nCounter + 1
	endwhile
	if (LP_STORE("usimpl") != 0) return(-1)
	
	nHits = LP_GET_USPECNAM("usoper", USPECNAM_IN, "F")
	activate "SIGWIZ_PROGRESS".INIT_PROGRESS(nHits, $concat(sProgressText, " - Step 5/14"))
	release/e/mod "usoper"
	nCounter = 1
	while (nCounter &lt;= nHits)
		setocc "usoper", nCounter
		getitem/id uspecnam.usoper, sUSpecNewNamList, uspecnam.usoper
		activate "SIGWIZ_PROGRESS".SHOW_PROGRESS(nCounter, "Creating new entries in USOPER:%%^Processed %%nCounter%%% of %%nHits%%%")
		nCounter = nCounter + 1
	endwhile
	if (LP_STORE("usoper") != 0) return(-1)
	
	nHits = LP_GET_USPECNAM("usioper", USPECNAM_IN, "F")
	activate "SIGWIZ_PROGRESS".INIT_PROGRESS(nHits, $concat(sProgressText, " - Step 6/14"))
	release/e/mod "usioper"
	nCounter = 1
	while (nCounter &lt;= nHits)
