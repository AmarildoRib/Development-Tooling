operation CLEAN_ULANA
	
	params
		string sUDefPath : IN
	endparams

	variables
		numeric nHits, nCounter, nDeleted
		string sSignaturesList, sSignature
	endvariables
	
	; *** CODE FROM OPER. COPY_PARAM ***
	
	clear/e "ULANA"
	retrieve/e "ULANA"
	nHits = $hits("ULANA")
	activate "SIGWIZ_PROGRESS".INIT_PROGRESS(nHits, "Delete Orphaned Compiled Signature(s) - Step 1/2")
	nCounter = 1
	nDeleted = 0
	while (nCounter &lt;= nHits)
		setocc "ULANA", nCounter
		if ($instancehandle-&gt;CHECK_USPECNAM(U_GLAB.ULANA, sUDefPath) = 1)
			delete
			if ($status &lt; 0)
				message/error $procerrorcontext
			else
				nDeleted = nDeleted + 1
			endif
		endif
		activate "SIGWIZ_PROGRESS".SHOW_PROGRESS(nCounter, "Processed: %%nCounter%%% of %%nHits%%%")
		nCounter = nCounter + 1
	endwhile
	if (LP_STORE("ulana") != 0) return(-1)
	
	; COMMIT WORK
	
	if (LP_COMMIT() != 0) return(-1)
	
	sSignaturesList = $ldirlist("$rso:sig")
	nHits = $itemcount(sSignaturesList)
	activate "SIGWIZ_PROGRESS".INIT_PROGRESS(nHits, "Delete Orphaned Compiled Signature(s) - Step 2/2")
	nDeleted = 0
	forlist sSignature, nCounter in sSignaturesList
		if ($instancehandle-&gt;CHECK_USPECNAM($uppercase(sSignature[1:$length(sSignature)-$length(".sig")]), sUDefPath) = 1)
			lfiledelete $concat("$rso:sig\", sSignature)
			if ($status &lt; 0)
				message/error $procerrorcontext
			else
				nDeleted = nDeleted + 1
			endif
		endif
		activate "SIGWIZ_PROGRESS".SHOW_PROGRESS(nCounter, "Processed: %%nCounter%%% of %%nHits%%%")
	endfor

	deleteinstance "SIGWIZ_PROGRESS"
	
	message "%%nDeleted%%% Orphaned Compiled Signature(s) deleted..."
	
	return(0)
	
end;- CLEAN_ULANA

