operation EXPORT_SIG
	
	params
		string sUSpecNamList : IN
		string sFileName : IN
	endparams
	
	variables
		string sUSpecNam, sExportFileName
		numeric nCounter, nHits
		boolean bExportToZip, bAppend
	endvariables
	
	nHits = $itemcount(sUSpecNamList)
	activate "SIGWIZ_PROGRESS".INIT_PROGRESS(nHits, "Export Signature")
	
	if ($fileexists(sFileName) = 1)
		filedelete sFileName
	endif    
	
	if ($uppercase(sFileName[$length(sFileName)-3]) = ".ZIP")
		bExportToZip = "True"
		bAppend = "False"
	else
		sExportFileName = sFileName
		bExportToZip = "False"
		bAppend = "True"
	endif

	forlist sUSpecNam, nCounter in sUSpecNamList
		if (bExportToZip)
			sExportFileName = $concat(sFileName, ":sig\", sUSpecNam, ".sgx")
		endif
		if ($ude("export", "signature", sUSpecNam, sExportFileName, $concat("append=", bAppend)) &lt; 0)
			message/error $procerrorcontext
			message/hint "Export of signature(s) failed..."
			return(-1)
		endif
		;****************************************
		activate "SIGWIZ_PROGRESS".SHOW_PROGRESS(nCounter, "Processed %%nCounter%%% of %%nHits%%%")
		;****************************************
	endfor

	if (bExportToZip)
		lflush $concat(sFileName, ":")
	endif
	
	message/hint "Exported %%nHits%%% signature(s) to %%sFileName%%% ..."
	
	deleteinstance "SIGWIZ_PROGRESS"
	
	return(0)
	
end;- EXPORT_SIG

; ********************************************************************************

operation IMPORT_SIG
	
	params
		string  sFileName : IN
	endparams
	
	variables
		numeric V_STATUS
		boolean bImportFromZip 
	endvariables
	
	if ($uppercase(sFileName[$length(sFileName)-3]) = ".ZIP")
		bImportFromZip = "True"
		sFileName = $concat(sFileName, ":sig\*.sgx")
	endif

	V_STATUS = $ude("import", "misc", sFileName)
	
	return(V_STATUS)
	
end;- IMPORT_SIG

