operation FIND_UIPATTR
	
	params
		string UIPATTR_IN : IN
		string sSearchPrf    :    IN
		string RESULT_OUT : OUT
	endparams
	
	variables
		Numeric nTotOccs, nCurOcc
	endvariables
	
	putitem RESULT_OUT, -1, "1=Find all parameters with Interface %%"%%UIPATTR_IN%%""
	
	clear/e "USIPARM"
	if (sSearchPrf != "" &amp; $itemcount(sSearchPrf) = 1)
		sSearchPrf = $concat(sSearchPrf, "&uALL;")
	endif
	;nTotOccs = LP_TOTOCCS("USIPARM")
	nTotOccs = LP_GET_USPECNAM("USIPARM", sSearchPrf, "F")
	if (nTotOccs &lt;= 0) return 1
	activate "SIGWIZ_PROGRESS".INIT_PROGRESS(nTotOccs, "Find parameters with Interface%%^%%"%%UIPATTR_IN%%"")
	activate "SIGWIZ_PROGRESS".RESIZE(1, 1)
	;retrieve/e "USIPARM"
	nCurOcc = 1
	setocc "USIPARM", nCurOcc
	while (nCurOcc &lt;= nTotOccs)
		; if (UHINTERFACE.USPARM = UHINTERFACE_IN) putitem/id RESULT_OUT, USPECNAM.USPARM, "%%USPECNAM.USPARM%%% -&gt; %%UOPERNAM.USPARM%%%"
		if ($item("UIPBASETYP", UIPATTR.USIPARM) = UIPATTR_IN) putitem/id RESULT_OUT, "%%USPECNAM.USIPARM%%%.%%UOPERNAM.USIPARM%%%.%%UPARMNAM.USIPARM%%%", "   %%USPECNAM.USIPARM%%% -&gt; %%UOPERNAM.USIPARM%%% ( %%UPARMNAM.USIPARM%%% )"
		nCurOcc = nCurOcc + 1
		;****************************************
		activate "SIGWIZ_PROGRESS".SHOW_PROGRESS(nCurOcc, "Searching entries in USIPARM:%%^Processed %%nCurOcc%%% of %%nTotOccs%%%")
		;****************************************
		setocc "USIPARM", nCurOcc
	endwhile
	
	deleteinstance "SIGWIZ_PROGRESS"
	
	sort/list RESULT_OUT, "$idpart: A"
	
	putitem RESULT_OUT, -1, "2=&gt;&gt; Matching parameters: %%($itemcount(RESULT_OUT)-1)%%%"
	
	return(0)
	
end;- FIND_UIPATTR

