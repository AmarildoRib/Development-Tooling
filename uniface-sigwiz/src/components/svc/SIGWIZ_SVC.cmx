operation CHECK_PARAMS
	
	params
		boolean bAutoCorrect  : IN
		string  sUSpecNamList : IN
		numeric nUDIREC       : IN
		numeric nErrors       : OUT
		string  sRESULT_OUT   : OUT
	endparams
	
	variables
		numeric nCounter, nHits, nScanStart, nScanEnd
		string sUSpecNam, sSearchPrf
	endvariables
	
	;if (sUSpecNamList = "" | $scan(sUSpecNamList, "&uALL;") &gt; 0)
	;sUSpecNam = sUSpecNamList
	;sUSpecNamList = ""
	;nHits = LP_GET_USPECNAM("usspec", sUSpecNam, "F")
	;if (nHits = 0) return(-1)
	;activate "SIGWIZ_PROGRESS".INIT_PROGRESS(nHits, "Building List of Signature Names")
	;nCounter = 1
	;while (nCounter &lt;= nHits)
	;setocc "usspec", nCounter
	;putitem sUSpecNamList, -1, uspecnam.usspec
	;activate "SIGWIZ_PROGRESS".SHOW_PROGRESS(nCounter, "Processed %%nCounter%%% of %%nHits%%%")
	;nCounter = nCounter + 1
	;endwhile
	;endif
	
	;if ($scan(sUSpecNam, "&uALL;") &gt; 0)
	;nHits = LP_GET_UPARMNAM("usparm", sUSpecNam, "", "", "F")
	;sUSpecNam = ""
	;else
	;nHits = LP_GET_UPARMNAM("usparm", "", "", "", "F")
	;endif
	
	selectcase nUDIREC
		case 1
			putitem sRESULT_OUT, 1, "1=Check all Parameters for unknown Handle classifications"
		case 2
			sSearchPrf = "UDIREC=I"
			putitem sRESULT_OUT, 1, "1=Check all IN Parameters for unknown Handle classifications"
		case 3
			sSearchPrf = "UDIREC=O"
			putitem sRESULT_OUT, 1, "1=Check all OUT Parameters for unknown Handle classifications"
		case 4
			sSearchPrf = "UDIREC=B"
			putitem sRESULT_OUT, 1, "1=Check all INOUT Parameters for unknown Handle classifications"
	endselectcase

	if (bAutoCorrect)
		nHits = LP_GET_UPARMNAM("USPARM", "", "", sSearchPrf, "F")
	else
		nHits = LP_GET_UPARMNAM("USPARM", sUSpecNamList, "", sSearchPrf, "F")
	endif
	
	activate "SIGWIZ_PROGRESS".INIT_PROGRESS(nHits, "Checking Handle Classification")
	newinstance "SIGWIZ_SVC", "SIGWIZ_CHKPARAMS"
	nCounter = 1
	nErrors = 0
	while (nCounter &lt;= nHits)
		setocc "USPARM", nCounter
		if (UPARMNAM.USPARM &amp; U_DTYP.USPARM = "H")
			sUSpecNam = $item(UHINTERFACE.USPARM, sUSpecNamList)
			if (sUSpecNam = "" &amp; $instancehandle("SIGWIZ_CHKPARAMS")-&gt;CHECK_USPECNAM(UHINTERFACE.USPARM, "") = 1)
				if ($status = 1 | UHINTERFACE.USPARM = "?UNKNOWN?" | bAutoCorrect = "F")
					putitem/id sRESULT_OUT, "%%USPECNAM.USPARM%%%.%%UOPERNAM.USPARM%%%.%%UPARMNAM.USPARM%%%", "   %%USPECNAM.USPARM%%% -&gt; %%UOPERNAM.USPARM%%% : Handle classification [%%UHINTERFACE.USPARM%%%] of the parameter [%%UPARMNAM.USPARM%%%] not found."
					nErrors = nErrors + 1
				else
					if ($item(UHINTERFACE.USPARM[1:32], sUSpecNamList) != "")
						sUSpecNam = $item(UHINTERFACE.USPARM[1:32], sUSpecNamList)
					else
						if (UHINTERFACE.USPARM[1:2] = "U_")
							sUSpecNam = UHINTERFACE.USPARM[2]
						else
							sUSpecNam = UHINTERFACE.USPARM
						endif
						nScanEnd = $scan(sUSpecNamList, "%%sUSpecNam%%%&uSEP;")
						nScanStart = $scan(sUSpecNamList[1,nScanEnd], "%%sUSpecNam%%%=")
						nScanStart = nScanStart + $length("%%sUSpecNam%%%=")
						nScanEnd = nScanEnd + $length("%%sUSpecNam%%%&uSEP;") - 2
						sUSpecNam = sUSpecNamList[nScanStart, nScanEnd]
						;getitem/id sUSpecNam, sUSpecNamList, sUSpecNam
					endif
					if (sUSpecNam = "")
						putitem/id sRESULT_OUT, "%%USPECNAM.USPARM%%%.%%UOPERNAM.USPARM%%%.%%UPARMNAM.USPARM%%%", "   %%USPECNAM.USPARM%%% -&gt; %%UOPERNAM.USPARM%%% : Handle classification [%%UHINTERFACE.USPARM%%%] of the parameter [%%UPARMNAM.USPARM%%%] not found."
						nErrors = nErrors + 1
					else
						activate "SIGWIZ_CHKPARAMS".SET_UHINTERFACE(uspecnam.usparm, uopernam.usparm, uparmnam.usparm, sUSpecNam)
					endif
				endif
			elseif (sUSpecNam != "" &amp; $instancehandle("SIGWIZ_CHKPARAMS")-&gt;CHECK_USPECNAM($valuepart($item(UHINTERFACE.USPARM, sUSpecNamList)), "") = 0 &amp; bAutoCorrect = "T")
				activate "SIGWIZ_CHKPARAMS".SET_UHINTERFACE(uspecnam.usparm, uopernam.usparm, uparmnam.usparm, sUSpecNam)
				;else
				;putitem/id sRESULT_OUT, "%%USPECNAM.USPARM%%%.%%UOPERNAM.USPARM%%%.%%UPARMNAM.USPARM%%%", "   %%USPECNAM.USPARM%%% -&gt; %%UOPERNAM.USPARM%%% : Handle classification [%%UHINTERFACE.USPARM%%%] of the parameter [%%UPARMNAM.USPARM%%%] not found."
				;nErrors = nErrors + 1
			endif
		endif
		activate "SIGWIZ_PROGRESS".SHOW_PROGRESS(nCounter, "Processed %%nCounter%%% of %%nHits%%%")
		nCounter = nCounter + 1
	endwhile
	
	message "CHECK_PARAMS returned %%nErrors%%% error(s)"
	
	deleteinstance "SIGWIZ_PROGRESS"
	deleteinstance "SIGWIZ_CHKPARAMS"
	
	sort/list sRESULT_OUT, "$idpart: A"
	
	putitem sRESULT_OUT, -1, "2=&gt;&gt; CHECK_PARAMS returned %%nErrors%%% error(s)"
	
	return(0)
	
end;- CHECK_PARAMS

