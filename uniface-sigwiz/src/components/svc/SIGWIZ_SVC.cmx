		nCounter = nCounter + 1
	endwhile
	if (LP_STORE("usioper") != 0) return(-1)
	
	nHits = LP_GET_USPECNAM("usparm", USPECNAM_IN, "F")
	activate "SIGWIZ_PROGRESS".INIT_PROGRESS(nHits, $concat(sProgressText, " - Step 7/14"))
