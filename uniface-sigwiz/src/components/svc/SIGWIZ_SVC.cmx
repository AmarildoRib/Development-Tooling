entry LP_GET_UIPATTR
	returns numeric

	params
		string UIPATTR_VALUE : IN
		string UIPATTR_LIST : OUT
	endparams

	selectcase UIPATTR_VALUE
		case "VT_I2"
			UIPATTR_LIST = "UIPBASETYP=VT_I2&uSEP;UIPFORMAT=VT_I2&uSEP;UIPLENGTH=2"
		case "VT_I4"
			UIPATTR_LIST = "UIPBASETYP=VT_I4&uSEP;UIPFORMAT=VT_I4&uSEP;UIPLENGTH=4"
		case "VT_R4"
			UIPATTR_LIST = "UIPBASETYP=VT_R4&uSEP;UIPFORMAT=VT_R4&uSEP;UIPLENGTH=4"
		case "VT_R8"
			UIPATTR_LIST = "UIPBASETYP=VT_R8&uSEP;UIPFORMAT=VT_R8&uSEP;UIPLENGTH=8"
		case "VT_CY"
			UIPATTR_LIST = "UIPBASETYP=VT_CY&uSEP;UIPFORMAT=VT_CY&uSEP;UIPLENGTH="
		case "VT_DATE"
;			UIPATTR_LIST = "UIPBASETYP=VT_DATE&uSEP;UIPFORMAT=VT_DATE&uSEP;UIPLENGTH=8"
			UIPATTR_LIST = "UIPBASETYP=VT_DATE&uSEP;UIPFORMAT=VT_DATE&uSEP;UIPLENGTH="
		case "VT_BSTR"
;			UIPATTR_LIST = "UIPBASETYP=VT_BSTR&uSEP;UIPFORMAT=VT_BSTR&uSEP;UIPLENGTH=4"
			UIPATTR_LIST = "UIPBASETYP=VT_BSTR&uSEP;UIPFORMAT=VT_BSTR&uSEP;UIPLENGTH="
		case "VT_DISPATCH"
;			UIPATTR_LIST = "UIPBASETYP=VT_DISPATCH&uSEP;UIPFORMAT=VT_DISPATCH&uSEP;UIPLENGTH=4"
			UIPATTR_LIST = "UIPBASETYP=VT_DISPATCH&uSEP;UIPFORMAT=VT_DISPATCH&uSEP;UIPLENGTH="
		case "VT_BOOL"
;			UIPATTR_LIST = "UIPBASETYP=VT_BOOL&uSEP;UIPFORMAT=VT_BOOL&uSEP;UIPLENGTH=2"
			UIPATTR_LIST = "UIPBASETYP=VT_BOOL&uSEP;UIPFORMAT=VT_BOOL&uSEP;UIPLENGTH="
		case "VT_VARIANT"
;			UIPATTR_LIST = "UIPBASETYP=VT_VARIANT&uSEP;UIPFORMAT=VT_VARIANT&uSEP;UIPLENGTH=16"
			UIPATTR_LIST = "UIPBASETYP=VT_VARIANT&uSEP;UIPFORMAT=VT_VARIANT&uSEP;UIPLENGTH="
