operation GET_PARAMS
	
	params
		string USPECNAM_IN : IN
		string UOPERNAM_IN : IN
		string parameters : OUT
		string parameters_tree : OUT
		string parameters_list : OUT
		Numeric nTotOccs : OUT
	endparams
	
	variables
		numeric    nCurOcc
		string    parameter_spec, parameter_tree_spec, parameter
		string    UDIREC_USPARM, U_DTYP_USPARM
	endvariables
	
	clear/e "USPARM"
	USPECNAM.USPARM/init = USPECNAM_IN
	UOPERNAM.USPARM/init = UOPERNAM_IN
	nTotOccs = LP_TOTOCCS("USPARM")
	if (nTotOccs &lt;= 0) return 1
	retrieve/e "USPARM"
	;  ********************************************************************************
	clear/e "USIPARM"
	USPECNAM.USIPARM/init = USPECNAM_IN
	UOPERNAM.USIPARM/init = UOPERNAM_IN
	nTotOccs = LP_TOTOCCS("USIPARM")
	if (nTotOccs &lt;= 0) return 1
	retrieve/e "USIPARM"
	;  ********************************************************************************
	nCurOcc = 1
	setocc "USIPARM", nCurOcc
	setocc "USPARM", nCurOcc
	while (nCurOcc &lt;= nTotOccs)
		if (UPARMNAM.USPARM)
			; What is UDIREC.USPARM?
			selectcase UDIREC.USPARM
				case "I"
					UDIREC_USPARM = "IN"
				case "O"
					UDIREC_USPARM = "OUT"
				case "B"
					UDIREC_USPARM = "INOUT"
				elsecase
					UDIREC_USPARM = "?%%UDIREC.USPARM%%%?"
			endselectcase
			; What is U_DTYP.USPARM?
			selectcase U_DTYP.USPARM
				case "B"
					U_DTYP_USPARM = "BOOLEAN"
				case "D"
					U_DTYP_USPARM = "DATE"
				case "E"
					U_DTYP_USPARM = "DATETIME"
				case "F"
					U_DTYP_USPARM = "FLOAT"
				case "H"
					U_DTYP_USPARM = "HANDLE to %%"%%%%%UHINTERFACE.USPARM%%%%%"%%%"
				case "I"
					U_DTYP_USPARM = "IMAGE"
				case "LD"
					U_DTYP_USPARM = "LINEAR DATE"
				case "LE"
					U_DTYP_USPARM = "LINEAR DATETIME"
				case "LT"
					U_DTYP_USPARM = "LINEAR TIME"
				case "N"
					U_DTYP_USPARM = "NUMERIC"
				case "R"
					U_DTYP_USPARM = "RAW"
				case "S"
					if (UOBJNAM.USPARM = "")
						U_DTYP_USPARM = "STRING"
					else
						U_DTYP_USPARM = "STRING {VT_DISPATCH to %%"%%%%%UOBJNAM.USPARM%%%%%"%%%}"
					endif
				case "T"
					U_DTYP_USPARM = "TIME"
				case "XM"
					U_DTYP_USPARM = "XML Stream"
				elsecase
					if (U_PTYP.USPARM = "C")
						if  (UPATH.USIPARM = "COM")
							U_DTYP_USPARM = "ADOR.Recordset of %%"%%%%%U_TLAB.USPARM%%%.%%U_VLAB.USPARM%%%%%"%%%"
						elseif (UPATH.USIPARM = "S3C")
							U_DTYP_USPARM = "entity parameter %%"%%%%%U_TLAB.USPARM%%%.%%U_VLAB.USPARM%%%%%"%%%"
						endif
					else
						U_DTYP_USPARM = "?%%U_DTYP.USPARM%%%?"
					endif
			endselectcase
			; ***
			parameter = "%%UPARMNAM.USPARM%%%"
			parameter_tree_spec = "&uSEP;LEAF&uSEP;%%UPARMNAM.USPARM%%%&uSEP;[%%UDIREC_USPARM%%%]&uSEP;%%U_DTYP_USPARM%%%"
			parameter_spec = "%%UPARMNAM.USPARM%%% [%%UDIREC_USPARM%%%] as %%U_DTYP_USPARM%%%"
		endif
		nCurOcc = nCurOcc + 1
		setocc "USIPARM", nCurOcc
		setocc "USPARM", nCurOcc
		if ($procerror &lt; 0)
			parameters_list = "%%parameters_list%%%%%parameter%%%"
		else
			parameters_list = "%%parameters_list%%%%%parameter%%%, "
		endif
		putitem/id parameters, parameter, parameter_spec
		putitem/id parameters_tree, parameter, parameter_tree_spec
		parameter_spec = ""
		parameter_tree_spec = ""
	endwhile
	
	return(0)
	
end;- GET_PARAMS

