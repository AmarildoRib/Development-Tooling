operation CHECK_USPECNAM
	
	params
		string USPECNAM_IN : IN
		string sUDefPath   : IN
	endparams
	
	if ($length(USPECNAM_IN) &lt;= 32)
		clear/e "USSPEC"
		USPECNAM.USSPEC/init = USPECNAM_IN
		UDEFPATH.USSPEC/init = sUDefPath
		if (LP_TOTOCCS("USSPEC") = 1) return(0)
	endif
	return(1)
	
end;- CHECK_USPECNAM

