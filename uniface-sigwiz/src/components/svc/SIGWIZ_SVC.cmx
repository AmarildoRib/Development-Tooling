entry LP_GET_USPECNAM

	returns numeric

	params
		string pEntityNam : IN
		string pSearchPrf : IN
		boolean pCompleteHitList : IN
	endparams

	variables
		string sRetrieveList, sRetrieveListItem, sUSpecNam
	endvariables

	if ($uppercase(pEntityNam) != "ULANA")
		sUSpecNam = $concat("USPECNAM.", pEntityNam)
	else
		sUSpecNam = $concat("U_GLAB.", pEntityNam)
	endif
	clear/e pEntityNam
	;if ($uppercase(EntityNam) = "USSPEC") UDEFPATH.USSPEC/init = "COM"
	if ($itemcount(pSearchPrf) &lt;= $vMaxHits$)
		@sUSpecNam/init = pSearchPrf
		retrieve/e pEntityNam
	else
		while ($itemnr(1, pSearchPrf ) != "")
			getitem sRetrieveListItem, pSearchPrf , 1
			delitem pSearchPrf, 1
			putitem sRetrieveList, -1, sRetrieveListItem
			if ($status = $number($vMaxHits$) | $itemnr(1, pSearchPrf ) = "")
				creocc pEntityNam, -1
				;if ($uppercase(EntityNam) = "USSPEC") UDEFPATH.USSPEC/init = "COM"
				@sUSpecNam/init = sRetrieveList
				retrieve/a pEntityNam
				sRetrieveList = ""
			endif
		endwhile
	endif
	if (pCompleteHitList = "T")
		setocc pEntityNam, -1
		return($status)
	else
		return($hits(pEntityNam))
	endif

end;- LP_GET_USPECNAM

; ********************************************************************************

entry LP_GET_UOPERNAM

	returns numeric

	params
		string pEntityNam : IN
		string pUSpecNam : IN
		string pSearchPrf : IN
		boolean CompleteHitList : IN
	endparams

	variables
		string sRetrieveList, sRetrieveListItem, sUSpecNam, sUOperNam
	endvariables

	sUSpecNam = $concat("USPECNAM.", pEntityNam)
	sUOperNam = $concat("UOPERNAM.", pEntityNam)
	clear/e pEntityNam
	if ($itemcount(pSearchPrf) &lt;= $vMaxHits$)
		@sUSpecNam/init = pUSpecNam
		@sUOperNam/init = pSearchPrf
		retrieve/e pEntityNam
	else
		while ($itemnr(1, pSearchPrf) != "")
			getitem sRetrieveListItem, pSearchPrf, 1
			delitem pSearchPrf, 1
			putitem sRetrieveList, -1, sRetrieveListItem
			if ($status = $number($vMaxHits$) | $itemnr(1, pSearchPrf) = "")
				creocc pEntityNam, -1
				@sUSpecNam/init = pUSpecNam
				@sUOperNam/init = sRetrieveList
				retrieve/a pEntityNam
				sRetrieveList = ""
			endif
		endwhile
	endif
	if (CompleteHitList = "T")
		setocc pEntityNam, -1
		return($status)
	else
		return($hits(pEntityNam))
	endif

end;- LP_GET_UOPERNAM

; ********************************************************************************

entry LP_GET_UPARMNAM

	returns numeric

	params
		string pEntityNam : IN
		string pUSpecNam : IN
		string pUOperNam : IN
		string pSearchPrf : IN
		boolean pCompleteHitList : IN
	endparams

	variables
		string sRetrieveList, sRetrieveListItem, spUSpecNam, spUOperNam, sUParmNam
	endvariables

	spUSpecNam = $concat("USPECNAM.", pEntityNam)
	spUOperNam = $concat("UOPERNAM.", pEntityNam)
	if ($scan(pSearchPrf, "=") = 0)
		sUParmNam = $concat("UPARMNAM.", pEntityNam)
	else
		sUParmNam = $concat($idpart(pSearchPrf), ".", pEntityNam)
		pSearchPrf = $valuepart(pSearchPrf)
	endif
	clear/e pEntityNam
	;if ($itemcount(pSearchPrf) &lt;= $vMaxHits$)
	if ($itemcount(pUSpecNam) &lt;= $vMaxHits$)
		@spUSpecNam/init = pUSpecNam
		@spUOperNam/init = pUOperNam
		@sUParmNam/init = pSearchPrf
		retrieve/e pEntityNam
	else
		clear/e pEntityNam
		;while ($itemnr(1, pSearchPrf) != "")
		while ($itemnr(1, pUSpecNam) != "")
			;getitem sRetrieveListItem, pSearchPrf, 1
			getitem sRetrieveListItem, pUSpecNam, 1
			;delitem pSearchPrf, 1
			delitem pUSpecNam, 1
			putitem sRetrieveList, -1, sRetrieveListItem
			;if ($status = $number($vMaxHits$) | $itemnr(1, pSearchPrf) = "")
			if ($status = $number($vMaxHits$) | $itemnr(1, pUSpecNam) = "")
				creocc pEntityNam, -1
				;@spUSpecNam/init = pUSpecNam
				@spUSpecNam/init = sRetrieveList
				@spUOperNam/init = pUOperNam
				@sUParmNam/init = pSearchPrf
				retrieve/a pEntityNam
				sRetrieveList = ""
			endif
		endwhile
	endif
	if (pCompleteHitList = "T")
		setocc pEntityNam, -1
		return($status)
	else
		return($hits(pEntityNam))
	endif

end;- LP_GET_UPARMNAM

