<DAT name="SFUNC" xml:space='preserve'>;----------------------------------------------------
operation init
;----------------------------------------------------

	#ifdefined NODEBUG
	nodebug
	#endif

end ;init


;--------------------------------------------------------
; Update the screen with the latest test results.
;--------------------------------------------------------
operation updateScreen
;--------------------------------------------------------
params
	String		pData			: IN
	String		pInputList		: IN
	Boolean		pFullUpdate		: IN
endparams

variables
	Numeric	vTotalTests
	Numeric	vPercentComplete
endvariables

	;Update the test list screens
	if(pData != "")
		activate "VUT_TESTLIST".setData(pData, pFullUpdate)
		activate "VUT_TESTTREE".setData(pData, pFullUpdate)
	endif

	putitem/id $fieldproperties("NO_ERRORS.FOOTER"), "BACKCOLOR", "WHITE"
	putitem/id $fieldproperties("NO_ERRORS.FOOTER"), "FORECOLOR", "BLACK"
	putitem/id $fieldproperties("NO_FAILED.FOOTER"), "BACKCOLOR", "WHITE"
	putitem/id $fieldproperties("NO_FAILED.FOOTER"), "FORECOLOR", "BLACK"
	putitem/id $fieldproperties("NO_IGNORED.FOOTER"), "BACKCOLOR", "WHITE"
	putitem/id $fieldproperties("NO_IGNORED.FOOTER"), "FORECOLOR", "BLACK"
	putitem/id $fieldproperties("NO_SKIPPED.FOOTER"), "BACKCOLOR", "WHITE"
	putitem/id $fieldproperties("NO_SKIPPED.FOOTER"), "FORECOLOR", "BLACK"

	;Populate the status values
	if(pInputList == "")
		return 0
	endif

	getitem/id NO_CASES.FOOTER,	pInputList, "CASES"
	getitem/id NO_ERRORS.FOOTER,	pInputList, "ERRORS"
	getitem/id NO_FAILED.FOOTER,	pInputList, "FAILURES"
	getitem/id NO_IGNORED.FOOTER,	pInputList, "IGNORED"
	getitem/id NO_SKIPPED.FOOTER,	pInputList, "SKIPPED"
	getitem/id RUN_TIME.FOOTER,	pInputList, "ELAPSEDTIME"

	if (NO_ERRORS.FOOTER &gt; 0)
		putitem/id $fieldproperties("NO_ERRORS.FOOTER"), "BACKCOLOR", "RED"
		putitem/id $fieldproperties("NO_ERRORS.FOOTER"), "FORECOLOR", "WHITE"
	endif
	if (NO_FAILED.FOOTER &gt; 0)
		putitem/id $fieldproperties("NO_FAILED.FOOTER"), "BACKCOLOR", "RED"
		putitem/id $fieldproperties("NO_FAILED.FOOTER"), "FORECOLOR", "WHITE"
	endif
	if (NO_IGNORED.FOOTER &gt; 0)
		putitem/id $fieldproperties("NO_IGNORED.FOOTER"), "BACKCOLOR", "YELLOW"
		putitem/id $fieldproperties("NO_IGNORED.FOOTER"), "FORECOLOR", "BLACK"
	endif
	if (NO_SKIPPED.FOOTER &gt; 0)
		putitem/id $fieldproperties("NO_SKIPPED.FOOTER"), "BACKCOLOR", "CYAN"
		putitem/id $fieldproperties("NO_SKIPPED.FOOTER"), "FORECOLOR", "BLACK"
	endif
	
	;Update the progress bar
	$status = $svcHandle$-&gt;getTotalNumberOfTests(vTotalTests)
	if(vTotalTests != 0)
		vPercentComplete = (NO_CASES.FOOTER / vTotalTests) * 100
	else
		vPercentComplete = 0
	endif
	
	PROGRESS_BAR.FOOTER = vPercentComplete
	if(NO_ERRORS.FOOTER &gt; 0 || NO_FAILED.FOOTER &gt; 0)
		putitem/id $fieldproperties("PROGRESS_BAR.FOOTER"), "BACKCOLOR", "#FF0000"
		putitem/id $fieldproperties("PROGRESS_BAR.FOOTER"), "FORECOLOR", "#FF0000"
	else
		putitem/id $fieldproperties("PROGRESS_BAR.FOOTER"), "BACKCOLOR", "#00FF00"
		putitem/id $fieldproperties("PROGRESS_BAR.FOOTER"), "FORECOLOR", "#00FF00"
	endif

	show

	return 0

end ;updateScreen


