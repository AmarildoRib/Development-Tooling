;--------------------------------------------------------
; Clear the screen
;--------------------------------------------------------
operation doClear
;--------------------------------------------------------

	call lpClear()

end ;doClear


;--------------------------------------------------------
; Retrieve tests
;--------------------------------------------------------
operation doRetrieve
;--------------------------------------------------------

	call lpRetrieve("F")

end ;doRetrieve


;--------------------------------------------------------
; Run the tests specified by pData. If this component was
; able to register with a local urouter and the logical
; RUN_TESTS_ASYNC is set, a new Uniface process will be
; created to run the tests in. Otherwise they will be run
; in this process.
; After the tests are run, results are fetched from the
; test service and displayed on the screen.
;--------------------------------------------------------
operation runTests
;--------------------------------------------------------
params
	xmlstream pData 	: IN
endparams

variables
	String		vInParams
	String		vOutParams
	String		vLastRunResults
endvariables

	;Get the current state from VUT_TESTLIST.
	;The user might have marked tests for debugging
	;etc.
	if (pData = "")
		activate "VUT_TESTLIST".getData(pData)
	endif

	;Call the test service to run all the tests
	if($logical("RUN_TESTS_ASYNC") &amp; $foundURouter$)
		;A URouter is available, so we can start the test runner in a new process
		call lpRunTestsAsync(pData)
	else
		;A URouter isn't available - call the test runner in this process
		putitem/id vInParams, "DATA",	pData
		putitem/id vInParams, "GUI",	$instancehandle
		$status = $svcHandle$-&gt;runAllTests(vInParams, vOutParams)
		if($status &lt; 0)
			return -1
		endif

		;Update the screen with the results
		call lpUpdateFooter(vOutParams)

		$status = $svcHandle$-&gt;getLastRunResults(vLastRunResults)
		activate "VUT_FAILURES".updateScreen(vLastRunResults)

		$status = $svcHandle$-&gt;resultsToXML(vOutParams)
		activate "VUT_XMLVIEW".updateScreen(vOutParams)

		$status = $svcHandle$-&gt;getLastRunResults(vOutParams)
		activate "VUT_IGNORED".updateScreen(vOutParams)
	
		SELECTED_TEST.FOOTER = ""
	endif

	return 0

end ;runTests


;--------------------------------------------------------
; Called from one of the components in the TEST_TAB tab
; container. Updates the selected test text on this form.
;--------------------------------------------------------
operation updateSelected
;--------------------------------------------------------
params
	string pSelectedValue : IN
endparams

	SELECTED_TEST.FOOTER = pSelectedValue

	return(0)

end ; updateSelected</DAT>
