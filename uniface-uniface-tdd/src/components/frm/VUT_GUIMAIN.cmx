<DAT name="INIT" xml:space='preserve'>params
	handle $svcHandle$	: IN
endparams

variables
	Datetime vDateTime
	String   vUST
	Boolean  vIsStartupRetrieve
endvariables

	;Connect to the local urouter so other Uniface processes can talk to this component
	$foundURouter$ = 0
	if($logical("RUN_TESTS_ASYNC"))
		vDateTime = $datim
		vUST = $concat("UUNIT", vDateTime[H], vDateTime[N], vDateTime[S], vDateTime[T])
		message "Attempting to connect to local urouter..."
		open "localhost|||%%vUST%%%","$DNP"
		message "Attempting to connect to local urouter...done."
		if($status == 0)
			$foundURouter$ = 1
		endif
	endif

	;Populate left tab pane
	newinstance	"VUT_TESTLIST"
	activate	"VUT_TESTLIST"
	newinstance	"VUT_TESTTREE"
	activate	"VUT_TESTTREE"
	TESTS_TAB.TAB_DUMMY = "VUT_TESTLIST"

	;Populate bottom right tab pane
	newinstance	"VUT_FAILURES",	$failures_handle$
	newinstance	"VUT_IGNORED",	$ignores_handle$
	newinstance	"VUT_XMLVIEW",	$xmlview_handle$
	activate	"VUT_FAILURES"
	activate	"VUT_IGNORED"
	activate	"VUT_XMLVIEW"
	OUTPUT_TAB.OUTPUT = "VUT_FAILURES"

	putitem/id $fieldproperties("PROGRESS_BAR.FOOTER"), "BACKCOLOR", "#FF0000"
	putitem/id $fieldproperties("PROGRESS_BAR.FOOTER"), "FORECOLOR", "#FF0000"

	;When dealing with environments where there are thousands of components
	;a retrieve of those on startup takes a lot of time. We check for
	;assignment settings to determine what to do.
	vIsStartupRetrieve    = $logical("UUNIT_STARTUP_RETRIEVE")
	if (vIsStartupRetrieve)
		$NO_SCAN_ALL_WARNING$ = "T" ; no warning if auto-retrieving
		call lpRetrieve("T")
	else
		$NO_SCAN_ALL_WARNING$ = $logical("UUNIT_SCAN_WARNING")
		; Disable the run button &amp; refresh buttons until tests retrieved
		$fieldsyntax(CMD_RUN.TOOLBAR)="DIM"		
		$fieldsyntax(CMD_REFRESH.TOOLBAR)="DIM"
	endif
	show
	$windowproperties($instancename, "STATE") = "STATE=Maximized"

	message "Ready."

	edit

	rollback
	close
	apexit</DAT>
