<DAT name="ASYNC" xml:space='preserve'>variables
	String	vData
	String	vInputList
	Boolean	vFullUpdate
	String	vOutParams
	String	vXMLResults
	String	vLastRunResults
endvariables

selectcase $msgid
case "UPDATE"
	getitem/id vData,			$msgdata,	"DATA"
	getitem/id vInputList,		$msgdata,	"INPUTLIST"
	getitem/id vFullUpdate,	$msgdata,	"FULLUPDATE"
	$instancehandle-&gt;updateScreen(vData, vInputList, vFullUpdate)
case "DONE"
	
	getitem/id vOutParams,			$msgdata,	"OUTPARAMS"
	getitem/id vXMLResults,		$msgdata,	"XMLRESULTS"
	getitem/id vLastRunResults,	$msgdata,	"RESULTS"

	call lpUpdateFooter(vOutParams)
	activate "VUT_FAILURES".updateScreen(vLastRunResults)
	activate "VUT_XMLVIEW".updateScreen(vXMLResults)
	activate "VUT_IGNORED".updateScreen(vLastRunResults)
	call lpUnlockScreen()
endselectcase</DAT>
<DAT name="GENERAL" xml:space='preserve'>;-------------------------------------------------------
; Clear this screen and all the attached tabs
;-------------------------------------------------------
entry lpClear
;-------------------------------------------------------

	if($screenLocked$)
		return 0
	endif

	$RETRIEVE_PROFILE$ = "" ; Reset component retrieve profile

	clear/e "FOOTER" ; added this to reset the test result data
	clear/e "TOOLBAR" ; added this to reset the test result data
	$status = $svcHandle$-&gt;clearTests()	
	activate "VUT_TESTLIST".clearData()
	activate "VUT_TESTTREE".clearData()	
	activate "VUT_FAILURES".clearData()	
	activate "VUT_IGNORED".clearData()	
	activate "VUT_XMLVIEW".clearData()	

	$fieldsyntax(CMD_RETRIEVE.TOOLBAR) = "NED&uSEP;YPR" ; Enable retrieve button
	$fieldsyntax(CMD_RUN.TOOLBAR)      = "DIM" ; Disable the run button
	$fieldsyntax(CMD_REFRESH.TOOLBAR)  = "DIM"	; Disable the refresh button
	TESTS_TAB.TAB_DUMMY = "VUT_TESTLIST" ; Set focus back to profile list tab
	setformfocus "VUT_TESTLIST"
	return 0	

end ;lpClear

;-----------------------------------------------------------
; Save profile entered on VUT_TESTLIST in $RETRIEVE_PROFILE$
; and then load matching tests.
;-----------------------------------------------------------
entry lpRetrieve
;-----------------------------------------------------------
params
	boolean pIsStartup : IN
endparams
variables
	string vComponentProfile
endvariables

	if($screenLocked$)
		return 0
	endif

	; List of tests returns user entered profile prior to first retrieve
	activate "VUT_TESTLIST".getProfile(vComponentProfile)

	; If new profile entered, save it for refresh
	if (vComponentProfile != "")
		$RETRIEVE_PROFILE$ = vComponentProfile
	elseif ($RETRIEVE_PROFILE$ = "" &amp; pIsStartup != "T" &amp; !$NO_SCAN_ALL_WARNING$)
		askmess "Scanning all components for unit tests may take a while. Are you sure?~Scan all components?"
		if ($status = 0) return 0
	endif

	; Load tests in components matching $RETRIEVE_PROFILE$
	call lpLoadTests()
	$fieldsyntax(CMD_RETRIEVE.TOOLBAR)="DIM" ; Disable retrieve button until next clear

end ;lpRetrieve

;-------------------------------------------------------
; Query VUT_MAIN for tests matching $RETRIEVE_PROFILE$
;-------------------------------------------------------
entry lpLoadTests
;-------------------------------------------------------
variables
	String	vProfile
	String	vInParams
	String	vOutParams
	String	vData
endvariables

	;Look for tests
	putitem/id vProfile,	"ULABEL", $RETRIEVE_PROFILE$
	putitem/id vInParams,	"PROFILE", vProfile
	$status = $svcHandle$-&gt;getTests(vInParams, vOutParams)
	getitem/id vData, vOutParams, "DATA"

	;Load the data into VUT_TESTLIST &amp; VUT_TESTTREE
	activate "VUT_TESTLIST".setData(vData, "T")
	activate "VUT_TESTTREE".setData(vData, "T")

	$fieldsyntax(CMD_RUN.TOOLBAR)="NED&uSEP;YPR"		; Enable the run button
	$fieldsyntax(CMD_REFRESH.TOOLBAR)="NED&uSEP;YPR"	; Enable the refresh button

	return 0

end ; lpLoadTests

;-------------------------------------------------------------
; Refresh - re-scan the profiled components for changed tests 
;-------------------------------------------------------------
entry lpRefresh
;-------------------------------------------------------------
variables
	string vRetrieveProfile
endvariables
	if($screenLocked$)
		return 0
	endif

	vRetrieveProfile = $RETRIEVE_PROFILE$	; Save retrieve profile	
	call lpClear() 								; Clear profile and existing results
	$RETRIEVE_PROFILE$ = vRetrieveProfile	; Re-instate retrieve profile
	$fieldsyntax(CMD_RETRIEVE.TOOLBAR)="DIM" ; Disable retrieve button until next clear
	call lpLoadTests() ; Re-retrieve tests matching saved $RETRIEVE_PROFILE$

	return 0

end ;lpRefresh

;-------------------------------------------------------
; Update the fields in the footer of this screen with
; the passed values.
;-------------------------------------------------------
entry lpUpdateFooter
;-------------------------------------------------------
params
	String	pFooterValues	: IN
endparams

	getitem/id NO_CASES.FOOTER,	pFooterValues, "CASES"
	getitem/id NO_ERRORS.FOOTER,	pFooterValues, "ERRORS"
	getitem/id NO_FAILED.FOOTER,	pFooterValues, "FAILURES"
	getitem/id NO_IGNORED.FOOTER,	pFooterValues, "IGNORED"
	getitem/id NO_SKIPPED.FOOTER,	pFooterValues, "SKIPPED"
	getitem/id RUN_TIME.FOOTER,	pFooterValues, "ELAPSEDTIME"

	return 0

end ;lpUpdateFooter


;-------------------------------------------------------
; Lock the screen whilst async tests are running.
;-------------------------------------------------------
entry lpLockScreen
;-------------------------------------------------------

	$screenLocked$ = 1
	$fieldsyntax("CMD_CLEAR.TOOLBAR")		= "DIM"
	$fieldsyntax("CMD_RETRIEVE.TOOLBAR")	= "DIM"
	$fieldsyntax("CMD_RUN.TOOLBAR")		= "DIM"

end ;lpLockScreen


;-------------------------------------------------------
; Unlock the screen once async tests are complete.
;-------------------------------------------------------
entry lpUnlockScreen
;-------------------------------------------------------

	$screenLocked$ = 0
	$fieldsyntax("CMD_CLEAR.TOOLBAR")		= ""
	$fieldsyntax("CMD_RETRIEVE.TOOLBAR")	= ""
	$fieldsyntax("CMD_RUN.TOOLBAR")		= ""

end ;lpUnlockScreen


;-------------------------------------------------------
; Dump the tests to a temporary XML file and start a 
; new Uniface process running the start-up shell
; VUT_ASYNC. The Asynchronous Interrupt trigger then
; handles post messages back from the process running
; the tests.
;-------------------------------------------------------
entry lpRunTestsAsync
;-------------------------------------------------------
params
	String	pData	: IN
endparams

variables
	String		vCallBackPath
	String		vCallBackInstance
	String		vCallBack
	DateTime	vDateTime
	String		vFileName
	String		vAssignmentFile
	String		vIniFile
	String		vAdmPath
	String		vSwitches
	String		vUnifacePath
endvariables

	;Work out how to reference this component via a post message
	vCallBackPath		= $instancepath
	vCallBackInstance	= $instancename
	vCallBack			= "%%vCallBackPath%%%:%%vCallBackInstance%%%"
	;Put the callback argument in quotes in case we have a space in the path somewhere
	vCallBack			= $concat("%%"%%%", vCallBack, "%%"%%%")

	;Look up what assignment file and ini file to use
	vAssignmentFile = $logical("ASYNC_ASSIGNMENT_FILE")
	if(vAssignmentFile == "")
		putmess "Logical ASYNC_ASSIGNMENT_FILE not set. Can't run tests asynchronously"
		return 0
	endif

	;Check the assignment file exists
	if( !$lfileexists(vAssignmentFile) )
		putmess "Couldn't find %%vAssignmentFile%%%"
		return 0
	endif	

	vIniFile = $logical("ASYNC_INI_FILE")
	if(vIniFile == "")
		vIniFile = "usys.ini"
	endif

	vAdmPath = $valuepart($fileproperties("usysuniface:.\adm", "FULLPATH"))
	if( !$lfileexists(vAdmPath) )
		putmess "Couldn't find the uniface adm directory"
		return 0
	endif

	;Find the uniface.exe file so we can run it
	vUnifacePath = $valuepart($fileproperties("usys:..\bin\uniface.exe", "FULLPATH"))
	if( !$lfileexists(vUnifacePath) )
		putmess "Couldn't find uniface.exe"
		return 0
	endif

	;Dump the tests we need to run to file
	vDateTime = $datim
	vFileName = $concat(vDateTime[Y],	vDateTime[M], vDateTime[D], vDateTime[H])
	vFileName = $concat(vFileName,		vDateTime[N], vDateTime[S], vDateTime[T])
	vFileName = "%%vFileName%%%tests_tmp.xml"
	filedump pData, vFileName

	;Lock the screen while the tests run
	call lpLockScreen()

	vSwitches = "/ini=%%vIniFile%%% /asn=%%vAssignmentFile%%%"
	if(vAdmPath != "")
		vSwitches = "/adm=%%"%%vAdmPath%%%%%" %%vSwitches%%%"
	endif

	;TODO: Should be using "OS".command, but this prevents the debugger from coming to the screen
	;activate "OS".command("%%vUnifacePath%%% /ini=%%vIniFile%%% /asn=%%vAssignmentFile%%% VUT_ASYNC %%vCallBack%%% %%vFileName%%%")
	spawn "%%vUnifacePath%%% %%vSwitches%%% VUT_ASYNC %%vCallBack%%% %%vFileName%%%"
	return 0

end ;lpRunTestsAsync</DAT>
