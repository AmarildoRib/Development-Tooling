;---------------------------------------------
; Return an XML string of tests to run, based
; on what is currently selected in the tree.
;---------------------------------------------
operation getData
;---------------------------------------------
params
	String	pData	: OUT
endparams

variables
	String	vData
	String	vObjectType
	String	vTemp
	Boolean	vFound
endvariables

	;Make sure we have the full set of tests loaded in TESTS.VUT
	activate "VUT_TESTLIST".getData(vData)
	clear/e "TESTS"
	xmlload vData, "DTD:TESTS.VUT"

	setocc "TESTS", 1
	while(($status &gt;= 0) &amp; (vFound = 0))
		vTemp = "%%COMPONENT_NAME.TESTS%%%_%%TEST_NAME.TESTS%%%"
		if(TREE.TREE == vTemp)
			vFound = 1
			break
		endif
		setocc "TESTS", $curocc("TESTS") + 1
	endwhile	

	vObjectType = lpGetObjectType(TREE.TREE)
	pData = lpGetSelectedTests(vObjectType)
	return 0

end ;getData


;---------------------------------------------
; Takes a set of tests as an XML stream and
; adds them to the screen. If a test already
; exists on the screen then it is updated,
; unless pFull is true, in which case the
; screen is cleared first.
;---------------------------------------------
operation setData
;---------------------------------------------
params
	String	pData		: IN
	Boolean	pFull		: IN
endparams

variables
	string	vTreeItem
	string	vTestSuite
	string	vTempTestSuite
	numeric	vLength
	string	vHoldOptions
	string	vKeepOptions
	string	vGroupList
	string	vGroupListTemp
	string	vGroupHold
	string	vGroupName
	string	vNodeID
	string	vNodeList
	string	vNodeParentID
	string	vNodeType
	string	vNodeName
	string	vNodeExpand
	numeric	vCurrentOcc
endvariables

	if ($CURRENTVIEW$ = "")
		$CURRENTVIEW$ = "VIEW_TESTSUITE"
	endif

	vTestSuite = ""
	if (pFull)
		clear/e "TREE"
		$fieldvalrep(TREE.TREE) = ""
		show
		; add root node to tree
		vTestSuite	= "All Tests"
		call lpUpdateTree(vTestSuite, "", "-", "TESTHEAD", vTestSuite)
	endif

	clear/e "TESTS"
	xmlload pData, "DTD:TESTS.VUT"
	
	setocc "TESTS", 1	
	while($status &gt; 0)
		selectcase $CURRENTVIEW$
		case "VIEW_TESTSUITE"
			if (COMPONENT_NAME.TESTS != vTestSuite)
				vTestSuite = COMPONENT_NAME.TESTS
				call lpUpdateTree(vTestSuite, "All Tests", "-", "TESTSUITE", vTestSuite)
			endif

			selectcase RESULT.TESTS
			case "Not Run"
				vNodeType = "TESTCASE"
			case "Passed"
				vNodeType = "TESTCASEPASS"
			case "Failed", "Error"
				vNodeType = "TESTCASEFAIL"
			case "Ignored", "Skipped"
				vNodeType = "TESTCASEIGNORE"
			endselectcase
			call lpUpdateTree("%%vTestSuite%%%_%%TEST_NAME.TESTS%%%", vTestSuite, "-", vNodeType, TEST_NAME.TESTS)

		case "VIEW_GROUP"
			if (vGroupList = "")
				;Build a list of groups
				vCurrentOcc = $curocc("TESTS")
				setocc "TESTS", 1
				while ($status &gt;= 0)
					vKeepOptions = OPTIONS.TESTS
					getitem vGroupHold, vKeepOptions, 1
					while (vGroupHold != "")

						getitem/id vGroupName, vGroupHold, "GROUP"
						if (vGroupName != "")
							putitem vGroupList, -1, vGroupName
						endif

						delitem vKeepOptions, 1
						getitem vGroupHold, vKeepOptions, 1
					endwhile
						
					setocc "TESTS", $curocc("TESTS") + 1
				endwhile
				setocc "TESTS", vCurrentOcc

				;Add groups to the tree
				vGroupListTemp = vGroupList
				sort/list vGroupListTemp
				getitem vGroupName, vGroupListTemp, 1
				while (vGroupName != "")
					call lpUpdateTree(vGroupName, "All Tests", "-", "TESTSUITE", vGroupName)

					delitem vGroupListTemp, 1
					getitem vGroupName, vGroupListTemp, 1
				endwhile
			endif
				
			vKeepOptions = OPTIONS.TESTS
			getitem/id vTestSuite, vKeepOptions, "GROUP"
			if (vTestSuite = "")
				vTestSuite = "No Group"
				call lpUpdateTree(vTestSuite, "All Tests", "-", "TESTSUITE", vTestSuite)	
			endif

			;Get all the groups for this test, and add it to each one	
			while (vTestSuite != "")
				selectcase RESULT.TESTS
				case "Not Run"
					vNodeType = "TESTCASE"
				case "Passed"
					vNodeType = "TESTCASEPASS"
				case "Failed", "Error"
					vNodeType = "TESTCASEFAIL"
				case "Ignored","Skipped"
					vNodeType = "TESTCASEIGNORE"
				endselectcase

				call lpUpdateTree("%%vTestSuite%%%_%%COMPONENT_NAME.TESTS%%%_%%TEST_NAME.TESTS%%%", vTestSuite, "-", vNodeType, TEST_NAME.TESTS)

				delitem/id vKeepOptions, "GROUP"
				getitem/id vTestSuite, vKeepOptions, "GROUP"
			endwhile

		endselectcase
				
		setocc "TESTS", $curocc("TESTS") + 1
	endwhile

	show

	return 0

end ;setData


;---------------------------------------------
; Force this form to render - fixes some
; graphical glitching.
;---------------------------------------------
operation doShow
;---------------------------------------------

	show

	return 0

end ;doShow


;---------------------------------------------
; Dynamic menu operations - called from
; VUT_TREE_POPUP menu to handle what should
; be displayed in the menu based on what was
; clicked on.
;---------------------------------------------
operation menuPredisplay
;---------------------------------------------
params
	string pMenu       : IN
	string pInlineMenu : OUT
endparams

variables
	string  	lvRecord
	string  	lvTemp
	boolean 	lvFound
	xmlstream 	lvData
	boolean		lvRunFlag
	boolean		lvDebugFlag
	string		lvGroup
	boolean		lvIsInGroup
	numeric 	lvPosition
endvariables

	lvRecord = TREE.TREE
	lvFound = 0

	activate "VUT_TESTLIST".getData(lvData)

	clear/e "TESTS"
	xmlload lvData, "DTD:TESTS.VUT"

	selectcase pMenu
	case "UPDATE"
		setocc "TESTS", 1
		while ($status &gt;= 0)&amp;(lvFound = 0)
			lvTemp = "%%COMPONENT_NAME.TESTS%%%_%%TEST_NAME.TESTS%%%"
			if (lvRecord == lvTemp)
				putitem/id pInlineMenu, "UPDATE_DEBUG", menuItem("OPTION", "Debug ?", DEBUG_FLAG.TESTS, 0)
				putitem/id pInlineMenu, "UPDATE_RUN", menuItem("OPTION", "Run ?", RUN_TEST.TESTS, 0)
				lvFound = 1
			elseif (lvRecord = COMPONENT_NAME.TESTS)
				lvRunFlag = "T"
				lvDebugFlag = "T"
				setocc "TESTS", 1
				while ($status &gt;= 0)
					if (COMPONENT_NAME.TESTS = lvRecord)
						if (!RUN_TEST.TESTS)
							lvRunFlag = "F"
						endif
						if (!DEBUG_FLAG.TESTS)
							lvDebugFlag = "F"
						endif
					endif
					setocc "TESTS", $curocc(TESTS)+1
				endwhile
				putitem/id pInlineMenu, "UPDATE_DEBUG", menuItem("OPTION", "Debug ?", lvDebugFlag, 0)
				putitem/id pInlineMenu, "UPDATE_RUN", menuItem("OPTION", "Run ?", lvRunFlag, 0)
				lvFound = 1
			elseif ($CurrentView$ = "VIEW_GROUP")&amp;(lvRecord != "All Tests")
				lvIsInGroup = "F"
				lvPosition = $curocc(TESTS)
				call lpCheckTestGroup(lvRecord, lvIsInGroup, lvGroup)
				if (lvIsInGroup)&amp;(lvGroup = lvRecord)
					lvRunFlag = "T"
					lvDebugFlag = "T"
					setocc "TESTS", 1
					while ($status &gt;= 0)
						lvIsInGroup = "F"
						call lpCheckTestGroup(lvRecord, lvIsInGroup, lvGroup)
						if (lvIsInGroup)
							if (!RUN_TEST.TESTS)
								lvRunFlag = "F"
							endif
							if (!DEBUG_FLAG.TESTS)
								lvDebugFlag = "F"
							endif
						endif
						setocc "TESTS", $curocc(TESTS)+1
					endwhile
					
				endif
				putitem/id pInlineMenu, "UPDATE_DEBUG", menuItem("OPTION", "Debug ?", lvDebugFlag, 0)
				putitem/id pInlineMenu, "UPDATE_RUN", menuItem("OPTION", "Run ?", lvRunFlag, 0)
				setocc "TESTS", lvPosition+1
			else

				putitem/id pInlineMenu, "UPDATE_DEBUG", menuItem("OPTION", "Debug ?", "", 1)
				putitem/id pInlineMenu, "UPDATE_RUN", menuItem("OPTION", "Run ?", "", 1)
				setocc "TESTS", $curocc(TESTS)+1
			endif
		endwhile	
	; display the options available to Run the selected test(s)
	case "RUNNOW"
		if (lvRecord = "All Tests")
			putitem/id pInlineMenu, "RUN_THIS", menuItem("OPTION", "Run", 0, 1)
		else
			putitem/id pInlineMenu, "RUN_THIS", menuItem("OPTION", "Run", 0, 0)
		endif
		setocc "TESTS", 1
		while ($status &gt;= 0)&amp;(lvFound = 0)
			if (RESULT.TESTS = "Failed")|(RESULT.TESTS = "Error")
				lvFound = 1
			endif
			setocc "TESTS", $curocc(TESTS)+1
		endwhile

		setocc "TESTS", 1
		while ($status &gt;= 0)&amp;(lvFound = 0)
			if (RESULT.TESTS = "Failed")|(RESULT.TESTS = "Error")
				lvFound = 1
			endif
			setocc "TESTS", $curocc(TESTS)+1
		endwhile

		if ($CurrentView$ = "VIEW_TESTSUITE")
			putitem/id pInlineMenu, "RUN_ALL", menuItem("OPTION", "Run All", 0, 0)
		endif
		if (lvFound)&amp;($CurrentView$ = "VIEW_TESTSUITE")
			putitem/id pInlineMenu, "RUN_FAILS", menuItem("OPTION", "Run Fails / Errors", 0, 0)
		endif
	; display the current state for the selected test
	case "CURRENT"
		setocc "TESTS", 1
		while ($status &gt;= 0)&amp;(lvFound = 0)
			lvTemp = "%%COMPONENT_NAME.TESTS%%%_%%TEST_NAME.TESTS%%%"
			if (lvRecord == lvTemp)
				putitem/id pInlineMenu, "DISABLED", menuItem("OPTION", RESULT.TESTS, 0, 0)
				lvFound = 1
			else
				putitem/id pInlineMenu, "DISABLED", menuItem("OPTION", "", 0, 1)
				setocc "TESTS", $curocc(TESTS)+1
			endif
		endwhile	

	case "VIEW"
		selectcase $CURRENTVIEW$
		case "VIEW_TESTSUITE"
			putitem/id pInlineMenu, "VIEW_TESTSUITE", menuItem("OPTION", "Group by Test Suite", 1, 0)
			putitem/id pInlineMenu, "VIEW_GROUP", menuItem("OPTION", "Group by Group", 0, 0)
		case "VIEW_GROUP"
			putitem/id pInlineMenu, "VIEW_TESTSUITE", menuItem("OPTION", "Group by Test Suite", 0, 0)
			putitem/id pInlineMenu, "VIEW_GROUP", menuItem("OPTION", "Group by Group", 1, 0)
		endselectcase		
	endselectcase
	
	return 0

end ;menuPredisplay


;---------------------------------------------
; Execute commands selected from menu
; VUT_TREE_POPUP based on what is selected.
;---------------------------------------------
operation menuOption
;---------------------------------------------
params
	string  pMenuItem : IN
endparams

variables
	string	lvData
	boolean	lvFull
	string	lvCurrentTest
	string	lvCurrentComp
	string	lvRecord
	boolean	lvHeadTest
	boolean	lvFound
	boolean	lvIsInGroup
	string	lvGroup
	string	lvTemp
	boolean	lvThisRecord
	string	lvMenuOptions
	boolean	lvChecked
	boolean	lvGroupHead
	string	lvObjectType
endvariables

	lvGroupHead 	= "F"
	lvThisRecord 	= "F"
	lvHeadTest		= "F"
	lvRecord 		= TREE.TREE
	lvIsInGroup		= "F"
	lvObjectType	= lpGetObjectType(TREE.TREE)

	if(lvObjectType == "TESTHEAD")
		lvHeadTest = "T"
	elseif(lvObjectType == "TESTSUITE" &amp; $CURRENTVIEW$ == "VIEW_GROUP")
		; Check if this is a Group Header Item
		call lpCheckTestGroup(lvRecord, lvIsInGroup, lvGroup)
		if(lvIsInGroup &amp; (lvGroup = lvRecord))
			lvGroupHead = "T"
		endif
	endif

	;If a test case was clicked, go to that occurrence
	if(lvObjectType == "TESTCASE")
		setocc "TESTS", 1
		while( ($status &gt;= 0) &amp; (!lvThisRecord) )
			lvTemp = "%%COMPONENT_NAME.TESTS%%%_%%TEST_NAME.TESTS%%%"
			if (lvRecord == lvTemp)
				lvThisRecord = "T"
				break
			endif

			setocc "TESTS", $curocc(TESTS)+1
		endwhile
	endif

	selectcase $uppercase(pMenuItem)

	; Update debug flag
	case "UPDATE_DEBUG"
		;If a single test is selected, just update that one
		if (lvObjectType == "TESTCASE")
			DEBUG_FLAG.TESTS = !DEBUG_FLAG.TESTS

			xmlsave/one lvData, "DTD:TESTS.VUT"
			activate "VUT_TESTLIST".setData(lvData, 0)
			return 0
		endif

		; Get menu options from those in the current right click menu
		getitem/id lvMenuOptions, $UpdMenuOptions$, "UPDATE_DEBUG"
		getitem/id lvChecked, lvMenuOptions, "CHECKED"
		setocc "TESTS", 1
		while ($status &gt;= 0)
			selectcase $CURRENTVIEW$ 
			case "VIEW_TESTSUITE"
				if (COMPONENT_NAME.TESTS = lvRecord)
					DEBUG_FLAG.TESTS = !lvChecked
				endif
			case "VIEW_GROUP"
				lvIsInGroup = "F"
				call lpCheckTestGroup(lvRecord, lvIsInGroup, lvGroup)
				if (lvIsInGroup)
					DEBUG_FLAG.TESTS = !lvChecked
				endif
			endselectcase
			setocc "TESTS", $curocc(TESTS)+1
		endwhile

		xmlsave lvData, "DTD:TESTS.VUT"
		activate "VUT_TESTLIST".setData(lvData, 0)
		return 0

	; Update run flag
	case "UPDATE_RUN"
		if (lvObjectType == "TESTCASE")
			RUN_TEST.TESTS = !RUN_TEST.TESTS

			xmlsave/one lvData, "DTD:TESTS.VUT"
			activate "VUT_TESTLIST".setData(lvData, 0)
			return 0
		endif

		; Get menu options from those in the current right click menu
		getitem/id lvMenuOptions, $UpdMenuOptions$, "UPDATE_RUN"
		getitem/id lvChecked, lvMenuOptions, "CHECKED"
		setocc "TESTS", 1
		while ($status &gt;= 0)
			selectcase $CURRENTVIEW$ 
			case "VIEW_TESTSUITE"
				if (COMPONENT_NAME.TESTS = lvRecord)
					RUN_TEST.TESTS = !lvChecked
				endif
			case "VIEW_GROUP"
				lvIsInGroup = "F"
				call lpCheckTestGroup(lvRecord, lvIsInGroup, lvGroup)
				if (lvIsInGroup)
					RUN_TEST.TESTS = !lvChecked
				endif
			endselectcase
			setocc "TESTS", $curocc(TESTS)+1
		endwhile

		xmlsave lvData, "DTD:TESTS.VUT"
		activate "VUT_TESTLIST".setData(lvData, 0)
		return 0

	; run this specific test / test suite / group, by the right click option
	case "RUN_THIS"	
		call lpRunSelected(lvObjectType)
		return 0

	; Run all available tests
	case "RUN_ALL"	; run all tests
		call lpRunAll()
		return 0

	; Run Failures Only
	case "RUN_FAILS"
		setocc "TESTS", 1
		while ($status &gt;= 0)
			if (RESULT.TESTS = "Failed") | (RESULT.TESTS = "Error")
				RUN_TEST.TESTS = "T"
			else
				RUN_TEST.TESTS = "F"
			endif
			setocc "TESTS", $curocc(TESTS)+1
		endwhile

		xmlsave lvData, "DTD:TESTS.VUT"
		$instanceparent-&gt;runTests(lvData)

	; Change tree view to test suites
	case "VIEW_TESTSUITE"
		$CURRENTVIEW$ = $uppercase(pMenuItem)
		xmlsave lvData, "DTD:TESTS.VUT"
		$instancehandle-&gt;setData(lvData, 1)
	
	; Change tree view to test groups
	case "VIEW_GROUP"
		$CURRENTVIEW$ = $uppercase(pMenuItem)
		xmlsave lvData, "DTD:TESTS.VUT"
		$instancehandle-&gt;setData(lvData, 1)
		
	endselectcase

	return 0

end ;menuOption</DAT>
