<DAT name="GENERAL" xml:space='preserve'>;-----------------------------------------------
; Build a menu item ready to be inserted into
; a dynamic menu.
;-----------------------------------------------
entry menuItem
;-----------------------------------------------
returns string

params
	string  pMenuId       : IN
	string  pMenuText     : IN
	boolean pMenuChecked  : IN
	boolean pMenuDisabled : IN
endparams

variables
	string  vMenuItem
endvariables

	putitem/id vMenuItem , "TYPE", pMenuId
	putitem/id vMenuItem , "TEXT", pMenuText
	if (pMenuChecked = 1)  putitem/id vMenuItem , "CHECKED", "1"
	if (pMenuDisabled = 1) putitem/id vMenuItem , "DISABLED", "1"

	return vMenuItem

end ;menuItem


;-----------------------------------------------
; For the selected object pSelected returns
; pIsInGroup indicating if the node is in a test
; group. If pIsInGroup is true, pGroup has the
; name of the group.
;-----------------------------------------------
entry lpCheckTestGroup
;-----------------------------------------------
params
	string	pSelected	: IN
	boolean	pIsInGroup 	: OUT
	string	pGroup		: OUT
endparams

variables
	string	vGroupList
	string	vHoldOptions
	string	vGroupName
	string	vCombinedName
	numeric	vNoGroupStart
	numeric	vGroupCount
endvariables

	pIsInGroup 	= "F"
	vGroupList 	= OPTIONS.TESTS

	vGroupName 	= ""
	vGroupCount	= 0
	getitem/id vGroupName, vGroupList, "GROUP"
	while (vGroupName != "")&amp;(!pIsInGroup)
		vGroupCount += 1
		if (vGroupName = pSelected)
			pIsInGroup = "T"
			pGroup     = vGroupName
		else
			vCombinedName = "%%vGroupName%%%_%%COMPONENT_NAME.TESTS%%%_%%TEST_NAME.TESTS%%%"
			if (vCombinedName = pSelected)
				pIsInGroup = "T"
				pGroup     = vGroupName
			endif
		endif
		delitem/id vGroupList, "GROUP"
		getitem/id vGroupName, vGroupList, "GROUP"
	endwhile

	; If no group has been found, is the selected test No Group or in the No Group.
	vNoGroupStart = $scan(TREE.TREE, "No Group")
	if (vNoGroupStart &gt; 0)&amp;(vGroupCount = 0)
		pIsInGroup = "T"
		pGroup = "No Group"
		return (0)
	endif

end ; lpCheckTestGroup


;-----------------------------------------------
; If pObjectId exists in the tree then it's type
; is updated to pObjectType.
; If pObjectId doesn't exist, then it is added
; with the provided details.
;-----------------------------------------------
entry lpUpdateTree
;-----------------------------------------------
params
	String	pObjectId			: IN
	String	pParentObjectId		: IN
	String	pObjectExpand		: IN
	String	pObjectType			: IN
	String	pObjectName			: IN
endparams

variables
	String	vCurrentType
endvariables

	vCurrentType = TREE_getObjectType(pObjectId)	

	if(vCurrentType == "")
		;This object doesn't exist in the tree, so add it
		call TREE_setObject(pObjectId, pParentObjectId, pObjectExpand, pObjectType, pObjectName, "")
	elseif(vCurrentType != pObjectType)
		;The object exists, but we need to update the type
		call TREE_setObjectType(pObjectId, pObjectType)
	endif

	return 0

end ;lpUpdateTree


;-----------------------------------------------
; Runs the selected tests.
;-----------------------------------------------
entry lpRunSelected
;-----------------------------------------------
params
	String	pObjectType	: IN
endparams

variables
	String lvData
endvariables

	lvData = lpGetSelectedTests(pObjectType)
	$instanceparent-&gt;runTests(lvData)
	return 0

end ;lpRunSelected


;-----------------------------------------------
; Runs all the tests in the tree.
;-----------------------------------------------
entry lpRunAll
;-----------------------------------------------
variables
	String	lvData
endvariables

	lvData = lpGetAllTests()
	$instanceparent-&gt;runTests(lvData)
	return 0

end ;lpRunAll


;-----------------------------------------------
; Returns the type of the Object specified by
; pObjectId.
; Type will be TESTSUITE | TESTHEAD | TESTCASE
;-----------------------------------------------
entry lpGetObjectType
;-----------------------------------------------
returns String

params
	String	pObjectId	: IN
endparams

variables
	String	lvObjectType
endvariables

	; Figure out what type of node was clicked on
	lvObjectType = TREE_getObjectType(pObjectId)
	if(lvObjectType[1:8] == "TESTCASE")
		lvObjectType = "TESTCASE"
	endif

	return lvObjectType

end ;lpGetObjectType


;-----------------------------------------------
; Return all the tests current loaded into this
; form.
;-----------------------------------------------
entry lpGetAllTests
;-----------------------------------------------
returns String

variables
	String	vData
endvariables

	setocc "TESTS", 1
	while ($status &gt;= 0)
		RUN_TEST.TESTS = "T"
		setocc "TESTS", $curocc(TESTS)+1
	endwhile

	xmlsave vData, "DTD:TESTS.VUT"
	return vData	

end ;lpGetAllTests


;-----------------------------------------------
; Return the currently selected tests.
;-----------------------------------------------
entry lpGetSelectedTests
;-----------------------------------------------
returns String

params
	String	pObjectType	: IN
endparams

variables
	Boolean	lvHeadTest
	String	lvData
	Boolean	lvIsInGroup
endvariables

	lvHeadTest 		= "F"

	;Root node was clicked, get all tests
	if (TREE.TREE == "All Tests" | TREE.TREE == "")
		return lpGetAllTests()
	endif

	;Test case node clicked, just get that test
	if(pObjectType == "TESTCASE")
		RUN_TEST.TESTS = "T"
		xmlsave/one lvData, "DTD:TESTS.VUT"
		return lvData
	endif

	;Test suite or group clicked, get all tests under it
	setocc "TESTS", 1
	while ($status &gt;= 0)
		if($CURRENTVIEW$ == "VIEW_TESTSUITE")
			if(TREE.TREE = COMPONENT_NAME.TESTS)
				RUN_TEST.TESTS = 1
			else
				RUN_TEST.TESTS = 0
			endif
		elseif($CURRENTVIEW$ == "VIEW_GROUP")
			lvIsInGroup = "F"
			call lpCheckTestGroup(TREE.TREE, lvIsInGroup, "")
			if (lvIsInGroup)
				RUN_TEST.TESTS = 1
			else
				RUN_TEST.TESTS = 0
			endif
		endif

		setocc "TESTS", $curocc(TESTS)+1
	endwhile

	$onlySaveTestsToRun$ = 1
	xmlsave lvData, "DTD:TESTS.VUT"
	$onlySaveTestsToRun$ = 0

	return lvData

end ;lpGetSelectedTests</DAT>
