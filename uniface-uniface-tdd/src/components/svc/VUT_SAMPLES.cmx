<DAT name="SFUNC" xml:space='preserve'>;------------------------------------------------
operation getRecord
;------------------------------------------------
params
	String	pID		: IN
	String	pValue	: OUT
endparams

	clear/e "TEST_ENT_1"
	if($status &lt; 0)
		return $status
	endif

	ID.TEST_ENT_1 = pID
	retrieve/e "TEST_ENT_1"
	if($status &lt; 0)
		return $status
	endif

	pValue = VALUE.TEST_ENT_1
	return 0

end ;getRecord


;------------------------------------------------
operation storeRecord
;------------------------------------------------
params
	String	pID		: IN
	String	pValue	: IN
endparams

	;Store a value
	clear/e "TEST_ENT_1"
	if($status &lt; 0)
		return $status
	endif

	ID.TEST_ENT_1		= pID
	VALUE.TEST_ENT_1	= pValue

	store/e "TEST_ENT_1"
	if($status &lt; 0)
		return $status
	endif

	commit
	if($status &lt; 0)
		return $status
	endif

	return 0

end ;storeRecord


