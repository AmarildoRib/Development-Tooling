<DAT name="SFUNC" xml:space='preserve'>;-------------------------------------------------------------------------
;	Initialise results to 0
;-------------------------------------------------------------------------
operation init
;-------------------------------------------------------------------------

	#ifdefined NODEBUG
		nodebug
	#endif

	clear/e "RESULTS"

	TOTAL_TESTS.RESULTS		= 0
	TOTAL_ERRORS.RESULTS	= 0
	TOTAL_FAILURES.RESULTS	= 0
	TOTAL_IGNORED.RESULTS	= 0
	TOTAL_SKIPPED.RESULTS	= 0

	return 0

end ;init


;-------------------------------------------------------------------------
;	Start timing test run
;-------------------------------------------------------------------------
operation setRunStartTime
;-------------------------------------------------------------------------

	RUN_START_TIME.RESULTS = $clock

	return 0

end ;setRunStartTime


;-------------------------------------------------------------------------
;	Start a new test
;-------------------------------------------------------------------------
operation startTest
;-------------------------------------------------------------------------
params
	String	pComponentName	: IN
	String	pTestName		: IN
endparams

	if(COMPONENT_NAME.TEST_RESULT != "")
		creocc "TEST_RESULT", 1
	endif

	COMPONENT_NAME.TEST_RESULT	= pComponentName
	TEST_NAME.TEST_RESULT		= pTestName
	START_TIME.TEST_RESULT		= $clock

	return 0

end ;startTest


;-------------------------------------------------------------------------
;	End the test
;-------------------------------------------------------------------------
operation endTest
;-------------------------------------------------------------------------
params
	String	pResult	: IN
endparams

	END_TIME.TEST_RESULT	= $clock
	RESULT.TEST_RESULT		= pResult

	TOTAL_TESTS.RESULTS += 1

	selectcase pResult
		case "Error"
			TOTAL_ERRORS.RESULTS	+= 1
		case "Failed"
			TOTAL_FAILURES.RESULTS	+= 1
		case "Skipped"
			TOTAL_SKIPPED.RESULTS	+= 1
		case "Ignored"
			TOTAL_IGNORED.RESULTS	+= 1
	endselectcase

	return 0

end ;endTest


;-------------------------------------------------------------------------
;	Log error message
;-------------------------------------------------------------------------
operation logTestMessage
;-------------------------------------------------------------------------
params
	String	pMessage	: IN
	String	pContext	: IN
	String	pTestType	: IN
endparams

	TEST_MESSAGE.TEST_RESULT	= pMessage
	CONTEXT.TEST_RESULT			= $replace($replace(pContext,1,"&uNOT;","!",-1),1,"&uSEP;",";",-1)
	MESSAGE_TYPE.TEST_RESULT	= pTestType

	return 0

end ;logErrorMessage


;-------------------------------------------------------------------------
;	Get results in a Uniface list
;-------------------------------------------------------------------------
operation getResults
;-------------------------------------------------------------------------
params
	String	pResults	: OUT
endparams

variables
	String		vTestResultList
	Datetime	vElapsedTime
	String		vMessageList
	String		vGroupResults
	String		vGroupTests
	Datetime	vTotalElapsedTime
	Numeric		vErrorCount
	Numeric		vFailureCount
	Numeric		vIgnoredCount
endvariables

	RESULTS.RESULTS = ""

	setocc "TEST_RESULT", 1
	while($status &gt; 0)

		vElapsedTime = END_TIME.TEST_RESULT - START_TIME.TEST_RESULT

		vTestResultList = ""
		putitem/id vTestResultList, "component",	COMPONENT_NAME.TEST_RESULT
		putitem/id vTestResultList, "name",		TEST_NAME.TEST_RESULT
		putitem/id vTestResultList, "time",		vElapsedTime

		putitem/id vMessageList, "message",	TEST_MESSAGE.TEST_RESULT
		putitem/id vMessageList, "type",		MESSAGE_TYPE.TEST_RESULT	
		putitem/id vMessageList, "context",	CONTEXT.TEST_RESULT

		if(RESULT.TEST_RESULT == "Error")
			putitem/id vTestResultList, "error",	vMessageList
		elseif(RESULT.TEST_RESULT == "Failed")
			putitem/id vTestResultList, "failure",	vMessageList
		endif

		;Pack the results of this test into list RESULTS.RESULTS
		getitem/id vGroupResults, RESULTS.RESULTS, COMPONENT_NAME.TEST_RESULT

		;Extract the group tests, error, failure &amp; ignored count
		getitem/id vGroupTests,		vGroupResults, "TESTS"
		getitem/id vTotalElapsedTime,	vGroupResults, "TIME"
		getitem/id vErrorCount,		vGroupResults, "ERRORS"
		if (vErrorCount = "")
			vErrorCount = 0
		endif
		getitem/id vFailureCount,		vGroupResults, "FAILURES"
		if (vFailureCount = "")
			vFailureCount = 0
		endif
		getitem/id vIgnoredCount,		vGroupResults, "IGNORED"
		if (vIgnoredCount = "")
			vIgnoredCount = 0
		endif

		;Update the tests list, increment the counts etc...
		putitem/id vGroupTests, $curocc("TEST_RESULT"), vTestResultList

		vTotalElapsedTime = vTotalElapsedTime + vElapsedTime

		if(RESULT.TEST_RESULT == "Error")
			vErrorCount		= vErrorCount + 1
		elseif(RESULT.TEST_RESULT == "Failed")
			vFailureCount	= vFailureCount + 1
		elseif(RESULT.TEST_RESULT == "Ignored")
			vIgnoredCount	= vIgnoredCount + 1	
		endif

		;Pack these values back up...
		putitem/id vGroupResults, "ERRORS",	vErrorCount
		putitem/id vGroupResults, "FAILURES",	vFailureCount
		putitem/id vGroupResults, "IGNORED",	vIgnoredCount
		putitem/id vGroupResults, "TIME",		vElapsedTime
		putitem/id vGroupResults, "TESTS",		vGroupTests

		;..and add them back to the main results list
		putitem/id RESULTS.RESULTS, COMPONENT_NAME.TEST_RESULT, vGroupResults

		setocc "TEST_RESULT", $curocc("TEST_RESULT") + 1
	endwhile

	pResults = RESULTS.RESULTS
	return 0

end ;getResults


;-------------------------------------------------------------------------
;	Return test details as Uniface List for a specified TestResultID
;-------------------------------------------------------------------------
operation getTestDetails
;-------------------------------------------------------------------------
params
	String	pTestResultID	: IN
	String	pTestDetails	: OUT
endparams

variables
	String	vLastRunResults
	String	vTestGroupItem
	String	vTestGroupID
	String	vTestGroup
	String	vTests
endvariables

	$status = $instancehandle-&gt;getResults(vLastRunResults)

	;loop over result list
	getitem vTestGroupItem, vLastRunResults, 1
	while($status &gt; 0)
		vTestGroupID	= $idpart(vTestGroupItem)
		vTestGroup	= $valuepart(vTestGroupItem)

		getitem/id vTests, vTestGroup, "TESTS"
		getitem/id pTestDetails, vTests, pTestResultID
		if($status &gt; 0)
			;We found our ID, and the results are in pTestDetails.
			return 1
		endif
		
		delitem vLastRunResults, 1
		getitem vTestGroupItem, vLastRunResults, 1
	endwhile

	;We didn't find the ID we were looking for
	pTestDetails = ""
	return -1

end ;getTestDetails


;-------------------------------------------------------------------------
;	Return All test results in a Uniface List
;-------------------------------------------------------------------------
operation packResults
;-------------------------------------------------------------------------
params
	String	pOutputList	: OUT
endparams

variables
	String	vElapsedTime
	String	vOutputList
endvariables

	$Elapsed_Time$	= $clock - RUN_START_TIME.RESULTS
	vElapsedTime	= $Elapsed_Time$

	vOutputList = ""
	putitem/id vOutputList, "CASES",			TOTAL_TESTS.RESULTS
	putitem/id vOutputList, "ERRORS",			TOTAL_ERRORS.RESULTS
	putitem/id vOutputList, "FAILURES",		TOTAL_FAILURES.RESULTS
	putitem/id vOutputList, "IGNORED",			TOTAL_IGNORED.RESULTS
	putitem/id vOutputList, "SKIPPED",			TOTAL_SKIPPED.RESULTS
	putitem/id vOutputList, "ELAPSEDTIME",		vElapsedTime

	pOutputList = vOutputList
	return 0

end ;packResults


;-------------------------------------------------------------------------
;	Return Results as XML string
;-------------------------------------------------------------------------
operation resultsToXML
;-------------------------------------------------------------------------
params
	String	pOutput			: OUT
endparams

variables
	String	vAllTestResults
	String	vTestGroupItem
	String	vTestGroupID
	String	vTestGroup
	String	vXMLOutput
endvariables

	$status = $instancehandle-&gt;getResults(vAllTestResults)

	;loop over result list
	getitem vTestGroupItem, vAllTestResults, 1
	while($status &gt; 0)
		vTestGroupID	= $idpart(vTestGroupItem)
		vTestGroup		= $valuepart(vTestGroupItem)
		
		call lpLogXMLResultsViaParser(vTestGroupID, vTestGroup, vXMLOutput)

		pOutput = $concat(pOutput, "%%^", vXMLOutput)

		delitem vAllTestResults, 1
		getitem vTestGroupItem, vAllTestResults, 1
	endwhile

	return 0

end ;resultsToXML


;-------------------------------------------------------------------------
;	Dump XML results to the specified file
;-------------------------------------------------------------------------
operation dumpResultsToFile
;-------------------------------------------------------------------------
params
	String	pOutputPath	: IN
endparams

variables
	String	vAllTestResults
	String	vTestGroupItem
	String	vTestGroupID
	String	vTestGroup
	String	vXMLOutput
	String	vXMLOutputFile
endvariables

	$status = $instancehandle-&gt;getResults(vAllTestResults)

	;Loop over result list
	getitem vTestGroupItem, vAllTestResults, 1
	while($status &gt; 0)
		vTestGroupID	= $idpart(vTestGroupItem)
		vTestGroup	= $valuepart(vTestGroupItem)
		
		call lpLogXMLResultsViaParser(vTestGroupID, vTestGroup, vXMLOutput)

		vXmlOutputFile = $concat(pOutputPath, vTestGroupID, ".xml")
		if ($lfileexists(vXmlOutputFile) = 1)
			lfiledelete vXmlOutputFile
			if ($status &lt; 0)
				putmess "Error %%$procerrorcontext deleting %%vXmlOutputFile"
			endif
		endif

		lfiledump vXMLOutput, vXmlOutputFile, "UTF-8"
	
		delitem vAllTestResults, 1
		getitem vTestGroupItem, vAllTestResults, 1
	endwhile

	return 0

end ;dumpResultsToFile</DAT>
