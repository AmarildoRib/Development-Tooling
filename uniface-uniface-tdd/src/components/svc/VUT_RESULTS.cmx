<DAT name="INIT" xml:space='preserve'>;---------------------------------------------------
; Encapsulates the test results. This service allows
; counting of run, erroring, failed, ignored and
; skipped test as well as timing individual tests
; and complete test runs. Once these details have
; been logged VUT_RESULTS can return the results in
; various formats including lists and XML.
;---------------------------------------------------</DAT>
<DAT name="GENERAL" xml:space='preserve'>;---------------------------------------
; Format the DateTime parameter
; pTimestamp as an XML timestamp.
;---------------------------------------
entry lpXmlTimestamp
;---------------------------------------
returns String

params
	DateTime pTimestamp	: IN
endparams

variables
	String vFormatted
endvariables

	$Formatted_Date$ = pTimeStamp[date]
	$Formatted_Time$ = pTimestamp[clock]
	
	vFormatted = "%%$Formatted_Date$%%%T%%$Formatted_Time$%%%"
	return vFormatted

end ;lpXmlTimestamp


;---------------------------------------
; Convert the DateTime parameter
; pDateTime into seconds.
;---------------------------------------
entry lpDateTimeToSeconds
;---------------------------------------
returns String

params
	DateTime	pDateTime	: IN
endparams

variables
	Numeric	vHours
	Numeric	vMinutes
	Numeric	vSecs
	Numeric	vTicks
	String	vFormatted
endvariables

	vHours		= pDateTime[H] * 3600
	vMinutes	= pDateTime[N] * 60
	vSecs		= pDateTime[S]
	vTicks		= pDateTime[T] / 100

	vFormatted = vHours + vMinutes + vSecs + vTicks
	return vFormatted

end ;lpDateTimeToSeconds


;---------------------------------------
; Log the results of the tests in
; pTestGroupID to XML.
;---------------------------------------
entry lpLogXMLResultsViaParser
;---------------------------------------
params
	String	pTestGroupID		: IN
	String	pTestGroupResults	: IN
	String	pXMLOutput			: OUT
endparams

variables
	handle 		xmlwriter
	String		vComponent
	String		vContext
	String		vCurrentTime
	DateTime	vElapsedTime
	String		vErrorList
	Numeric		vErrors
	String		vFailureList
	Numeric		vFailures
	Numeric		vIgnored
	Boolean		vIsIgnored
	Numeric		vItemCount
	Numeric		vItemNo
	String		vMessage
	Numeric		vSecs
	String		vTestName
	String		vTestResultItem
	String		vTestResultList
	String		vTests
	DateTime	vTime
	String		vType
	String		vVersion
	String		vXMLOutput
	numeric		vRemaining
	string		vErrorDetail
endvariables

	getitem/id vVersion,		$about,				"version"
	getitem/id vErrors,			pTestGroupResults,	"ERRORS"
	getitem/id vFailures,		pTestGroupResults,	"FAILURES"
	getitem/id vIgnored,		pTestGroupResults,	"IGNORED"
	getitem/id vTime,			pTestGroupResults,	"TIME"
	getitem/id vTests, 			pTestGroupResults,	"TESTS"
	vCurrentTime = lpXmlTimestamp($datim)
	vItemCount = $itemcount(vTests)
	vSecs = lpDateTimeToSeconds(vTime)

	; create Uniface XML Writer instance
	newinstance "UXMLWRITER" , xmlwriter
	xmlwriter-&gt;startdocument(1)

	; create testsuite element and attributes
	xmlwriter-&gt;startelement("", "testsuite", "")
	xmlwriter-&gt;addAttribute("name", pTestGroupID, 0)
	xmlwriter-&gt;addAttribute("tests", vItemCount, 0)
	xmlwriter-&gt;addAttribute("errors", vErrors, 0)
	xmlwriter-&gt;addAttribute("failures", vFailures, 0)
	xmlwriter-&gt;addAttribute("ignored", vIgnored, 0)
	xmlwriter-&gt;addAttribute("timestamp", vCurrentTime, 0)
	xmlwriter-&gt;addAttribute("time", vSecs, 0)
	
	; create properties element
	xmlwriter-&gt;startelement("", "properties", "")

	; create property element and attributes
	xmlwriter-&gt;startelement("", "property", "")
	xmlwriter-&gt;addAttribute("name", "uniface.version", 0)
	xmlwriter-&gt;addAttribute("value", vVersion, 0)

	xmlwriter-&gt;endelement() ; end property element
	xmlwriter-&gt;endelement() ; end properties element

	vItemNo = 0
	; loop through the tests that have been carried out.
	while (vItemNo &lt; vItemCount)
		vItemNo += 1
		getitem vTestResultItem, vTests, 1
		delitem vTests, 1
		vTestResultList = $valuepart(vTestResultItem)

		getitem/id vComponent,		vTestResultList, "component"
		getitem/id vTestName,		vTestResultList, "name"
		getitem/id vElapsedTime,	vTestResultList, "time"
		vSecs = lpDateTimeToSeconds(vElapsedTime)
		getitem/id vErrorList,		vTestResultList, "error"
		getitem/id vFailureList,	vTestResultList, "failure"

		if (vErrorList != "" | vFailureList != "")

			; create testcase element and attributes
			xmlwriter-&gt;startelement("", "testcase", "")
			xmlwriter-&gt;addAttribute("classname", vComponent, 0)
			xmlwriter-&gt;addAttribute("name", vTestName, 0)
			xmlwriter-&gt;addAttribute("time", vSecs, 0)
			
			if (vErrorList != "")
				getitem/id vMessage,	vErrorList, "message"
				getitem/id vType,		vErrorList, "type"
				getitem/id vContext,	vErrorList, "context"

				; create error element
				xmlwriter-&gt;startelement("", "error", "")
			endif
			if (vFailureList != "")
				getitem/id vMessage,	vFailureList, "message"
				getitem/id vType,		vFailureList, "type"
				getitem/id vContext,	vFailureList, "context"

				; create failure element
				xmlwriter-&gt;startelement("", "failure", "")
			endif

			vErrorDetail = vMessage
			getitem/id vMessage, vMessage, "DESCRIPTION"
			if (vMessage = "")
				vMessage = vErrorDetail

				; clear out angled brackets, so not to affect the xml
				vMessage = $replace(vMessage, 1, "&lt;", "&amp;lt;", -1)
				vMessage = $replace(vMessage, 1, "&gt;", "&amp;gt;", -1)

				; add atributes to the element
				xmlwriter-&gt;addAttribute("message", vMessage, 0)
				xmlwriter-&gt;addAttribute("type", vType, 0)
				if (vContext != "")
					xmlwriter-&gt;characters(vContext)
				endif
			else
				vErrorDetail = $replace(vErrorDetail, 1, "&uSEP;", "%%^", -1)
				vErrorDetail = $replace(vErrorDetail, 1, "&uNOT;", "", -1)

				; add atributes to the element
				xmlwriter-&gt;addAttribute("message", vMessage, 0)
				xmlwriter-&gt;characters(vErrorDetail)

			endif
			xmlwriter-&gt;endelement()	; end error or failure element
			xmlwriter-&gt;endelement()	; end testcase element

		else
			; create testcase element and attributes
			xmlwriter-&gt;startelement("", "testcase", "")
			xmlwriter-&gt;addAttribute("classname", vComponent, 0)
			xmlwriter-&gt;addAttribute("name", vTestName, 0)
			xmlwriter-&gt;addAttribute("time", vSecs, 0)
			getitem/id vIsIgnored,	vTestResultList, "ignored"
			if (vIsIgnored)
				xmlwriter-&gt;addAttribute("ignored", "true", 0)
			endif
			xmlwriter-&gt;endelement()	; end testcase element
		endif
	endwhile

	; create system-out element and attributes
	xmlwriter-&gt;startelement("", "system-out", "")
	xmlwriter-&gt;characters("&lt;![CDATA[]]&gt;")
	xmlwriter-&gt;endelement() 

	; create system-err element and attributes
	xmlwriter-&gt;startelement("", "system-err", "")
	xmlwriter-&gt;characters("&lt;![CDATA[]]&gt;")
	xmlwriter-&gt;endelement()

	; Finish document and output, ensuring we cater for large XML streams.
	xmlwriter-&gt;enddocument()
	vRemaining = xmlwriter-&gt;getdocument(vXMLOutput, "true")
	pXMLOutput = vXMLOutput
	while (vRemaining &gt; 0)
		vRemaining = xmlwriter-&gt;getdocument(vXMLOutput, "true")
		pXMLOutput = $concat(pXMLOutput, vXMLOutput)
	endwhile

	pXMLOutput = $replace(pXMLOutput, 1, "&lt;system-out&gt;&amp;lt;", "&lt;system-out&gt;&lt;", -1)
	pXMLOutput = $replace(pXMLOutput, 1, "&amp;gt;&lt;/system-out&gt;", "&gt;&lt;/system-out&gt;", -1)
	pXMLOutput = $replace(pXMLOutput, 1, "&lt;system-err&gt;&amp;lt;", "&lt;system-err&gt;&lt;", -1)
	pXMLOutput = $replace(pXMLOutput, 1, "&amp;gt;&lt;/system-err&gt;", "&gt;&lt;/system-err&gt;", -1)

end ; lpLogXMLResultsViaParser</DAT>
