<DAT name="SFUNC" xml:space='preserve'>;-------------------------------------------------
; Initialise the service
;-------------------------------------------------
operation init
;-------------------------------------------------

	#ifdefined NODEBUG
	nodebug
	#endif

	$cMessage$ = ""	
	$cContext$ = ""

end ;init


;-------------------------------------------------
; Creates the instance of the component to use in
; the test.
;-------------------------------------------------
operation createInstance
;-------------------------------------------------
params
	String	pComponentName	: IN
	String	pMessage		: OUT
endparams

	;The attached flag is required to allow detached
	;components to interact with the testing framework.
	newinstance/attached pComponentName, $componentInstance$
	if ($status &lt; 0)
		pMessage = $procerrorcontext
		return $status
	endif

	return 0

end ;createInstance


;-------------------------------------------------
; Execute a test operation.
; Will return the following:
;	&lt; 0 : An error occured
;	  0 : The test passed
;	&gt; 0 : An assertion failed
;-------------------------------------------------
operation runTest
;-------------------------------------------------
params
	String	pTestName	: IN
	Boolean	pDebug		: IN
	String	pMessage	: OUT
	string	pContext	: OUT
endparams

variables
	Numeric	vStatus
endvariables

	if (pDebug = "T")
		debug
	endif

	vStatus = $componentInstance$-&gt;"%%pTestName%%%"()
	if ($procerror &lt; 0)
		pMessage = "Activation Error"
		pContext = $procerrorcontext
	else
		pMessage = $cMessage$
		pContext = $cContext$
	endif

	return vStatus

end ;runTest


;-------------------------------------------------
; Execute a test operation in an entity.
; Will return the following:
;	&lt; 0 : An error occured
;	  0 : The test passed
;	&gt; 0 : An assertion failed
;-------------------------------------------------
operation runTestInEntity
;-------------------------------------------------
params
	String	pEntityName		: IN
	String	pTestName		: IN
	Boolean	pDebug			: IN
	String	pMessage		: OUT
	string	pContext		: OUT
endparams

variables
	Numeric	vStatus
endvariables

	if (pDebug = "T")
		debug
	endif

	vStatus = $componentInstance$-&gt;runTestInEntity(pEntityName, pTestName, pMessage)
	if ($procerror &lt; 0)
		pMessage = "Activation Error"
		pContext = $procerrorcontext
	else
		pMessage = $cMessage$
		pContext = $cContext$
	endif

	return vStatus

end ;runTestInEntity


;-------------------------------------------------
; If pActual equals pExpected return 0 (Success).
; Otherwise set $cMessage$ to pMessage and
; return 1 (Failed).
;-------------------------------------------------
operation assertEqual
;-------------------------------------------------
params
	String	pActual		: IN
	String	pExpected	: IN
	String	pMessage	: IN
endparams

	if(pActual != pExpected)
		;Set the message to be returned
		$cMessage$ = pMessage
		return 1
	endif

	return 0

end ;assertEqual


;-------------------------------------------------
; If pBoolean is false return 0 (Success).
; Otherwise set $cMessage$ to pMessage and
; return 1 (Failed).
;-------------------------------------------------
operation assertFalse
;-------------------------------------------------
params
	Boolean	pBoolean	: IN
	String	pMessage	: IN
endparams

	if (pBoolean)
		;Set the message to be returned
		$cMessage$ = pMessage
		return 1
	else
		return 0
	endif

end ;assertFalse


;-------------------------------------------------
; If pA is greater than pB return 0 (Success).
; Otherwise set $cMessage$ to pMessage
; and return 1 (Failed).
;-------------------------------------------------
operation assertGreaterThan
;-------------------------------------------------
params
	Numeric	pA			: IN
	Numeric	pB			: IN
	String	pMessage	: IN
endparams

	if( !(pA &gt; pB) )
		;Set the message to be returned
		$cMessage$ = pMessage
		return 1
	endif

	return 0

end ;assertGreaterThan


