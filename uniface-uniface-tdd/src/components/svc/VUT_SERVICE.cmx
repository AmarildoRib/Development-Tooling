	Date	pB			: IN
	String	pMessage	: IN
endparams

	if( pA &gt; pB )
		;Set the message to be returned
		$cMessage$ = pMessage
		return 1
	endif

	return 0

end ;assertDateLessThan


;-------------------------------------------------
; If pA is later than pB return 0 (Success).
; Otherwise set $cMessage$ to pMessage
; and return 1 (Failed).
;-------------------------------------------------
operation assertDatetimeGreaterThan
;-------------------------------------------------
params
	Datetime	pA			: IN
	Datetime	pB			: IN
	String		pMessage	: IN
endparams

	if( !(pA &gt; pB) )
		;Set the message to be returned
		$cMessage$ = pMessage
		return 1
	endif

	return 0

end ;assertDatetimeGreaterThan


;-------------------------------------------------
; If pA is earlier than pB return 0 (Success).
; Otherwise set $cMessage$ to pMessage
; and return 1 (Failed).
;-------------------------------------------------
operation assertDatetimeLessThan
;-------------------------------------------------
params
	Datetime	pA			: IN
	Datetime	pB			: IN
	String		pMessage	: IN
endparams

	if( pA &gt; pB )
		;Set the message to be returned
		$cMessage$ = pMessage
		return 1
	endif

	return 0

end ;assertDatetimeLessThan


;-------------------------------------------------
; If pValue is not equal to "" then return 0
; (Success). Otherwise set $cMessage$ to pMessage
; and return 1 (Failed).
;-------------------------------------------------
operation assertNotNull
;-------------------------------------------------
params
	String	pValue		: IN
	String	pMessage	: IN
endparams

	if (pValue != "")
		return 0
	else
		;Set the message to be returned
		$cMessage$ = pMessage
		return 1
	endif

end ;assertNotNull


;-------------------------------------------------
; If pValue is equal to "" then return 0
; (Success). Otherwise set $cMessage$ to pMessage
; and return 1 (Failed).
;-------------------------------------------------
operation assertNull
;-------------------------------------------------
params
	String	pValue		: IN
	String	pMessage	: IN
endparams

	if (pValue = "")
		return 0
	else
		;Set the message to be returned
		$cMessage$ = pMessage
		return 1
	endif

end ;assertNull


;-------------------------------------------------
