	$cMessage$ = pMessage
	$cContext$ = pContext
	return pStatus

end ; Error

;-------------------------------------------------
operation assertFilesEqual
;-------------------------------------------------
params
	String	pActual		: IN
	String	pExpected	: IN
	String	pMessage	: IN
endparams

variables
	String	vActualContents
	String	vExpectedContents
endvariables

	lfileload pActual, vActualContents
	if($status &lt;= 0)
		$cMessage$ = "Could not load %%pActual%%%"
		$cContext$ = $procerrorcontext
		return $status
	endif

	lfileload pExpected, vExpectedContents
	if($status &lt; 0)
		$cMessage$ = "Could not load %%pExpected%%%"
		$cContext$ = $procerrorcontext
		return $status
	endif

	if(vActualContents != vExpectedContents)
		$cMessage$ = pMessage
		return 1	; Fail
	endif

	return 0		; Success

end ;assertFilesEqual

;-------------------------------------------------
operation assertTextEqualToFile
;-------------------------------------------------
params
	String	pText		: IN
	String	pFile		: IN
	String	pMessage	: IN
endparams

variables
	String	vExpectedContents
endvariables

	lfileload pFile, vExpectedContents
	if($status &lt; 0)
		$cMessage$ = $concat("Could not load ", pFile)
		$cContext$ = $procerrorcontext
		return $status
	endif

	if(pText != vExpectedContents)
		$cMessage$ = pMessage
		return 1	; Fail
	endif

	return 0		; Success

end ;assertTextEqualToFile


;----------------------------------------------------------------------------------
operation assertXmlElementsEqualToFile
;----------------------------------------------------------------------------------
; Note: parse xml2list doesn't currently handle attributes, which are not compared
;----------------------------------------------------------------------------------
params
	String	pXml		: IN
	String	pFile		: IN
	String	pMessage	: IN
endparams

variables
	String	vExpected
	String	vXmlAsList
	String	vExpectedList
endvariables

	lfileload pFile, vExpected
	if ($status &lt; 0)
		$cMessage$ = "Could not find %%pFile%%%"
		$cContext$ = $procerrorcontext
		return $status
	endif

	activate "VUT_XMLPARSER".xml2List(vExpected,	vExpectedList)
	if ($status &lt; 0)
		$cMessage$ = "Could not activate XML Parser"
		$cContext$ = $procerrorcontext
		return $status
	endif
	activate "VUT_XMLPARSER".xml2List(pXml, vXmlAsList)	
	if ($status &lt; 0)
		$cMessage$ = "Could not activate XML Parser"
		$cContext$ = $procerrorcontext
		return $status
	endif

	if(vXmlAsList != vExpectedList)
		$cMessage$ = pMessage
		return 1 ; Fail
	endif

	return 0 ; Pass

end ;assertXmlElementsEqualToFile

