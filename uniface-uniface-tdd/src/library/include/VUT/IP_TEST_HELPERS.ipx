<DAT name="UTEXT" xml:space='preserve'>;-------------------------------------------------
entry lpEraseAllFromEntity
;-------------------------------------------------
params
	String	pEntityName	: IN
endparams
variables
	numeric vStatus
endvariables

		clear/e pEntityName
		#include VUT:HANDLE_ERROR

		call lpEraseEntity(pEntityName)

		;Check everything is actually gone
		clear/e pEntityName
		retrieve/e pEntityName
		if($status != -2)
			vStatus = $status
			if ($status &gt;= 0)
				$instanceparent-&gt;Fail($concat(pEntityName, " records not deleted."))
				vStatus = 1
			else
				$instanceparent-&gt;Error($status, $concat("Error ", vStatus, " retrieving ", pEntityName), $procerrorcontext)
			endif
			exit vStatus
		endif

		return 0
	
end ;lpEraseAllFromEntity

;-------------------------------------------------
entry lpEraseEntity
;-------------------------------------------------
params
	String	pEntityName	: IN
endparams
variables
	numeric vStatus
endvariables

		retrieve/e pEntityName
		if($status &lt; 0 &amp; $status != -2)
			vStatus = $status
			$instanceparent-&gt;Error(vStatus, $concat("Error ", vStatus, " retrieving ", pEntityName), $procerrorcontext)
			exit vStatus
		endif
	
		if ($hits(pEntityName) &gt; 0)
			setocc pEntityName, -1
			erase/e	pEntityName
			if($status &lt; 0)
				vStatus = $status
				$instanceparent-&gt;Error(vStatus, $concat("Erase error ", vStatus, " on ", pEntityName), $procerrorcontext)
				exit vStatus
			endif
			commit
			if($status &lt; 0)
				vStatus = $status
				$instanceparent-&gt;Error(vStatus, $concat("Erase commit error ", vStatus, " on ", pEntityName), $procerrorcontext)
				rollback
				exit vStatus
			endif
		endif

		return 0

end ; lpEraseEntity

;-------------------------------------------------
entry lpImportData
;-------------------------------------------------
params
	String	pXMLFilePath	: IN
endparams

variables
	String	vInputRecords
	String	vOutputRecords
endvariables

	entitycopy pXMLFilePath, "def:", "prcadditional=true"
	if($status &lt;= 0)
		$instanceparent-&gt;fail("entitycopy failed with status %%$status%%%")
		#include VUT:HANDLE_ASSERT
	endif

	vInputRecords	= $item("InputRecords",	$procreturncontext)
	vOutputRecords	= $item("OutputRecords",	$procreturncontext)

	if(	vInputRecords != vOutputRecords)
		$instanceparent-&gt;fail("entitycopy found %%vInputRecords%%% record(s), but could only import %%vOutputRecords%%% record(s)")
		#include VUT:HANDLE_ASSERT			
	endif

	return 0

end ;lpImportData


;-------------------------------------------------
entry lpImportUnifaceExport
;-------------------------------------------------
params
	String	pXMLFilePath	: IN
endparams

variables
	String	vInputRecords
	String	vOutputRecords
endvariables

	entitycopy pXMLFilePath, "idf:", "prcadditional=true&uSEP;supersede=true"
	if($status &lt;= 0)
		$instanceparent-&gt;fail("entitycopy failed with status %%$status%%%")
		#include VUT:HANDLE_ASSERT
	endif

	vInputRecords	= $item("InputRecords",	$procreturncontext)
	vOutputRecords	= $item("OutputRecords",	$procreturncontext)

	if(	vInputRecords != vOutputRecords)
		$instanceparent-&gt;fail("entitycopy found %%vInputRecords%%% record(s), but could only import %%vOutputRecords%%% record(s)")
		#include VUT:HANDLE_ASSERT			
	endif

	return 0

end ;lpImportUnifaceExport

